          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "Expose a standalone function as magic function for IPython."
                }
              }
            ],
            "inner": []
          }
        }
      ],
      "title": null
    },
    "Extended Summary": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "This will create an IPython magic (line, cell or both) from a standalone function.  The functions should have the following signatures:"
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "* For line magics: "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "def",
                    " ",
                    "f(line)"
                  ],
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " * For cell magics: "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "def",
                    " ",
                    "f(line,",
                    " ",
                    "cell)"
                  ],
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " * For a function that does both: "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "def",
                    " ",
                    "f(line,",
                    " ",
                    "cell=None)"
                  ],
                  "domain": null,
                  "role": null
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "In the latter case, the function will be called with "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "cell==None"
                  ],
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " when invoked as "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "%f"
                  ],
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ", and with cell as a string when invoked as "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "%%f"
                  ],
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": "."
                }
              }
            ],
            "inner": []
          }
        }
      ],
      "title": null
    },
