                "type": "Words",
                "data": {
                  "value": "Set floating point precision for pretty printing."
                }
              }
            ]
          }
        }
      ]
    },
    "Extended Summary": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "Can set either integer precision or a format string."
                }
              }
            ]
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "If numpy has been imported and precision is an int, numpy display precision will also be set, via "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "numpy.set",
                    "_",
                    "printoptions"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": "."
                }
              }
            ]
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "If no argument is given, defaults will be restored."
                }
              }
            ]
          }
        }
      ]
