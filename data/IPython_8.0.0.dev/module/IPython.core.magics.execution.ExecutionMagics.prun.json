                      "_number": 66,
                      "_offset": 0
                    },
                    {
                      "_line": "  \"name\"        function name",
                      "_number": 67,
                      "_offset": 0
                    },
                    {
                      "_line": "  \"nfl\"         name/file/line",
                      "_number": 68,
                      "_offset": 0
                    },
                    {
                      "_line": "  \"stdname\"     standard name",
                      "_number": 69,
                      "_offset": 0
