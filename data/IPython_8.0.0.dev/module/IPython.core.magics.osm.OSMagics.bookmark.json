    "Extended Summary": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "%bookmark <name>       - set bookmark to current dir %bookmark <name> <dir> - set bookmark to <dir> %bookmark -l           - list all bookmarks %bookmark -d <name>    - remove bookmark %bookmark -r           - remove all bookmarks"
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "You can later on access a bookmarked folder with::"
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "BlockVerbatim",
          "data": {
            "lines": {
              "_lines": [
                {
                  "_line": "  %cd -b <name>",
                  "_number": 8,
                  "_offset": 2
                },
                {
                  "_line": "",
                  "_number": 9,
                  "_offset": 2
                }
              ]
            }
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "or simply '%cd <name>' if there is no directory called <name> AND there is such a bookmark defined."
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "Your bookmarks persist through IPython sessions, but they are associated with each profile."
                }
              }
            ],
            "inner": []
          }
        }
