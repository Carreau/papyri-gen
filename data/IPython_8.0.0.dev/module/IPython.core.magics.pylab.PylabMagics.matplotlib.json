              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "savefig"
                  ],
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ". This behavior can be disabled using the "
