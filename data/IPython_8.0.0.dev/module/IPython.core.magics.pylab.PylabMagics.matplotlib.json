              }
            ]
          }
        },
        {
          "type": "BlockVerbatim",
          "data": {
            "lines": {
              "_lines": [
                {
                  "_line": "    In [1]: from IPython.display import set_matplotlib_formats",
                  "_number": 11,
                  "_offset": 4
                },
                {
                  "_line": "",
                  "_number": 12,
                  "_offset": 4
                },
                {
                  "_line": "    In [2]: set_matplotlib_formats('pdf', 'svg')",
                  "_number": 13,
                  "_offset": 4
