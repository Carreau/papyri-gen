            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "Note that this version interprets a : as specifying a format string (as per standard string formatting), so if slicing is required, you must explicitly create a slice."
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "This is to be used in templating cases, such as the parallel batch script templates, where simple arithmetic on arguments is useful."
                }
              }
            ],
            "inner": []
          }
        }
