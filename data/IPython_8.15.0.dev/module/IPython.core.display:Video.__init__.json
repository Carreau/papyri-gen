    },
    "Parameters": {
      "children": [
        {
          "children": [
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "The raw video data or a URL or filename to load the data from. Raw data will require passing "
                    },
                    {
                      "type": "inlineCode",
                      "value": "embed=True"
                    },
                    {
                      "type": "text",
                      "value": "."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "data",
              "type": "Param",
              "type_": "unicode, str or bytes"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "A URL for the video. If you specify "
                    },
                    {
                      "type": "inlineCode",
                      "value": "url="
                    },
                    {
                      "type": "text",
                      "value": ", the image data will not be embedded."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "url",
              "type": "Param",
              "type_": "unicode"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Path to a local file containing the video. Will be interpreted as a local URL unless "
                    },
                    {
                      "type": "inlineCode",
                      "value": "embed=True"
                    },
                    {
                      "type": "text",
                      "value": "."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "filename",
              "type": "Param",
              "type_": "unicode"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Should the video be embedded using a data URI (True) or be loaded using a <video> tag (False)."
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Since videos are large, embedding them should be avoided, if possible. You must confirm embedding as your intention by passing "
                    },
                    {
                      "type": "inlineCode",
                      "value": "embed=True"
                    },
                    {
                      "type": "text",
                      "value": "."
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Local files can be displayed with URLs without embedding the content, via      "
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "type": "code",
                  "value": "Video('./video.mp4')"
                }
              ],
              "param": "embed",
              "type": "Param",
              "type_": "bool"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Specify the mimetype for embedded videos. Default will be guessed from file extension, if available."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "mimetype",
              "type": "Param",
              "type_": "unicode"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Width in pixels to which to constrain the video in HTML. If not supplied, defaults to the width of the video."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "width",
              "type": "Param",
              "type_": "int"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Height in pixels to which to constrain the video in html. If not supplied, defaults to the height of the video."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "height",
              "type": "Param",
              "type_": "int"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Attributes for the HTML "
                    },
                    {
                      "type": "inlineCode",
                      "value": "<video>"
                    },
                    {
                      "type": "text",
                      "value": " block. Default: "
                    },
                    {
                      "type": "inlineCode",
                      "value": "\"controls\""
                    },
                    {
                      "type": "text",
                      "value": " to get video controls. Other examples: "
                    },
                    {
                      "type": "inlineCode",
                      "value": "\"controls muted\""
                    },
                    {
                      "type": "text",
                      "value": " for muted video with controls, "
                    },
                    {
                      "type": "inlineCode",
                      "value": "\"loop autoplay\""
                    },
                    {
                      "type": "text",
                      "value": " for looping autoplaying video without controls."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "html_attributes",
              "type": "Param",
              "type_": "str"
            }
          ],
          "type": "Parameters"
        }
      ],
