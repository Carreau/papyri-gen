    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "This will create an IPython magic (line, cell or both) from a standalone function.  The functions should have the following signatures:"
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "For line magics: "
                    },
                    {
                      "domain": null,
                      "role": null,
                      "type": "Directive",
                      "value": "def f(line)"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            },
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "For cell magics: "
                    },
                    {
                      "domain": null,
                      "role": null,
                      "type": "Directive",
                      "value": "def f(line, cell)"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            },
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "For a function that does both: "
                    },
                    {
                      "domain": null,
                      "role": null,
                      "type": "Directive",
                      "value": "def f(line, cell=None)"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            }
          ],
          "ordered": false,
          "spread": false,
          "start": 1,
          "type": "list"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "In the latter case, the function will be called with "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "cell==None"
            },
            {
              "type": "text",
              "value": " when invoked as "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "%f"
            },
            {
              "type": "text",
              "value": ", and with cell as a string when invoked as "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "%%f"
            },
            {
              "type": "text",
              "value": "."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
