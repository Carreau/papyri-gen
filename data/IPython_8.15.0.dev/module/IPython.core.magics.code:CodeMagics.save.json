              "dd": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "%save [options] filename [history]"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "dt": {
                "children": [
                  {
                    "type": "text",
                    "value": "Usage:\\"
                  }
                ],
                "type": "paragraph"
              },
              "type": "DefListItem"
            }
          ],
          "type": "DefList"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "Options:"
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "children": [
                {
                  "type": "text",
                  "value": "-r: use 'raw' input.  By default, the 'processed' history is used,   so that magics are loaded in their transformed version to valid   Python.  If this option is given, the raw input as typed as the   command line is used instead."
                }
              ],
              "type": "paragraph"
            },
            {
              "children": [
                {
                  "type": "text",
                  "value": "-f: force overwrite.  If file exists, %save will prompt for overwrite   unless -f is given."
                }
              ],
              "type": "paragraph"
            },
            {
              "children": [
                {
                  "type": "text",
                  "value": "-a: append to the file instead of overwriting it."
                }
              ],
              "type": "paragraph"
            }
          ],
          "type": "blockquote"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "The history argument uses the same syntax as %history for input ranges, then saves the lines to the filename you specify."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "If no ranges are specified, saves history of the current session up to this point."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "It adds a '.py' extension to the file if you don't do so yourself, and it asks for confirmation before overwriting existing files."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "If "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "-r"
            },
            {
              "type": "text",
              "value": " option is used, the default extension is "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": ".ipy"
            },
            {
              "type": "text",
              "value": "."
            }
          ],
          "type": "paragraph"
        }
      ],
