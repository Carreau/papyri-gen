  "example_section_data": {
    "children": [
      {
        "type": "code",
        "value": "import pytest\nnp = pytest.importorskip(\"numpy\")"
      },
      {
        "type": "text",
        "value": "Generate a sound"
      },
      {
        "type": "code",
        "value": "import numpy as np\nframerate = 44100\nt = np.linspace(0,5,framerate*5)\ndata = np.sin(2*np.pi*220*t) + np.sin(2*np.pi*224*t)\nAudio(data, rate=framerate)"
      },
      {
        "type": "text",
        "value": "Can also do stereo or more channels"
      },
      {
        "type": "code",
        "value": "dataleft = np.sin(2*np.pi*220*t)\ndataright = np.sin(2*np.pi*224*t)\nAudio([dataleft, dataright], rate=framerate)"
      },
      {
        "type": "text",
        "value": "From URL:"
      },
      {
        "type": "code",
        "value": "Audio(\"http://www.nch.com.au/acm/8k16bitpcm.wav\")  # doctest: +SKIP\nAudio(url=\"http://www.w3schools.com/html/horse.ogg\")  # doctest: +SKIP"
      },
      {
        "type": "text",
        "value": "From a File:"
      },
      {
        "type": "code",
        "value": ">>> Audio('IPython/lib/tests/test.wav')  # doctest: +SKIP\n>>> Audio(filename='IPython/lib/tests/test.wav')  # doctest: +SKIP"
      },
      {
        "type": "text",
        "value": "From Bytes:"
      },
      {
        "type": "code",
        "value": "Audio(b'RAW_WAV_DATA..')  # doctest: +SKIP\nAudio(data=b'RAW_WAV_DATA..')  # doctest: +SKIP"
      }
    ],
