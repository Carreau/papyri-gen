    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "The first invocation of this will create a "
            },
            {
              "type": "inlineCode",
              "value": "terminal.embed.InteractiveShellEmbed"
            },
            {
              "type": "text",
              "value": " instance and then call it.  Consecutive calls just call the already created instance."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "If you don't want the kernel to initialize the namespace from the scope of the surrounding function, and/or you want to load full IPython configuration, you probably want "
            },
            {
              "anchor": null,
              "exists": true,
              "kind": "module",
              "reference": {
                "kind": "api",
                "module": "IPython",
                "path": "IPython:start_ipython",
                "type": "RefInfo",
                "version": "*"
              },
              "type": "Link",
              "value": "IPython.start_ipython()"
            },
            {
              "type": "text",
              "value": " instead."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "Here is a simple example      "
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "code",
          "value": "from IPython import embed\na = 10\nb = 20\nembed(header='First time')\nc = 30\nd = 40\nembed()"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
