            "children": [
              {
                "data": {
                  "value": "Calling this magic from clients that do not implement standard input, such as the ipython notebook interface, will reset the namespace without confirmation."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
