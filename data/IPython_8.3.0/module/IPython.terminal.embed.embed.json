                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "Config"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " in as the config argument."
                },
                "type": "Words"
              }
            ]
