                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "A command to be executed by the system, using "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "subprocess.Popen"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": ". If a string is passed, it will be run in the system shell. If a list is passed, it will be used directly as arguments."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "cmd",
