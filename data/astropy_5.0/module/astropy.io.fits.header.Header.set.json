    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "If the keyword does not already exist in the header, a new keyword is created in the specified position, or appended to the end of the header if no position is specified."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "This method is similar to "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "Header.update"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " prior to Astropy v0.1."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "children": [
                    {
                      "data": {
                        "value": "It should be noted that "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "value": [
                          "header.set(keyword, value)"
                        ]
                      },
                      "type": "Verbatim"
                    },
                    {
                      "data": {
                        "value": " and "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "value": [
                          "header.set(keyword, value, comment)"
                        ]
                      },
                      "type": "Verbatim"
                    },
                    {
                      "data": {
                        "value": " are equivalent to "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "value": [
                          "header[keyword] = value"
                        ]
                      },
                      "type": "Verbatim"
                    },
                    {
                      "data": {
                        "value": " and "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "value": [
                          "header[keyword] = (value, comment)"
                        ]
                      },
                      "type": "Verbatim"
                    },
                    {
                      "data": {
                        "value": " respectively."
                      },
                      "type": "Words"
                    }
                  ]
                },
                "type": "Paragraph"
              },
              {
                "data": {
                  "children": [
                    {
                      "data": {
                        "value": "New keywords can also be inserted relative to existing keywords using, for example::      "
                      },
                      "type": "Words"
                    }
                  ]
                },
                "type": "Paragraph"
              },
              {
                "data": {
                  "value": ">>> header.insert('NAXIS1', ('NAXIS', 2, 'Number of axes'))"
                },
                "type": "BlockVerbatim"
              },
              {
                "data": {
                  "children": [
                    {
                      "data": {
                        "value": "to insert before an existing keyword, or::      "
                      },
                      "type": "Words"
                    }
                  ]
                },
                "type": "Paragraph"
              },
              {
                "data": {
                  "value": ">>> header.insert('NAXIS', ('NAXIS1', 4096), after=True)"
                },
                "type": "BlockVerbatim"
              },
              {
                "data": {
                  "children": [
                    {
                      "data": {
                        "value": "to insert after an existing keyword."
                      },
                      "type": "Words"
                    }
                  ]
                },
                "type": "Paragraph"
              },
              {
                "data": {
                  "children": [
                    {
                      "data": {
                        "value": "The only advantage of using "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "domain": null,
                        "role": "meth",
                        "value": "Header.set"
                      },
                      "type": "Directive"
                    },
                    {
                      "data": {
                        "value": " is that it easily replaces the old usage of "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "domain": null,
                        "role": "meth",
                        "value": "Header.update"
                      },
                      "type": "Directive"
                    },
                    {
                      "data": {
                        "value": " both conceptually and in terms of function signature."
                      },
                      "type": "Words"
                    }
                  ]
                },
                "type": "Paragraph"
              }
            ],
            "kind": "note",
            "title": ""
          },
          "type": "Admonition"
        }
      ],
