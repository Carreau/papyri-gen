    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "Raise a ConfigObjError if the value cannot be safely quoted. If multiline is "
            },
            {
              "type": "inlineCode",
              "value": "True"
            },
            {
              "type": "text",
              "value": " (default) then use triple quotes if necessary."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Don't quote values that don't need it."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            },
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Recursively quote members of a list and return a comma joined list."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            },
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Multiline is "
                    },
                    {
                      "type": "inlineCode",
                      "value": "False"
                    },
                    {
                      "type": "text",
                      "value": " for lists."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            },
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Obey list syntax for empty and single member lists."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            }
          ],
          "ordered": false,
          "spread": false,
          "start": 1,
          "type": "list"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "If "
            },
            {
              "type": "inlineCode",
              "value": "list_values=False"
            },
            {
              "type": "text",
              "value": " then the value is only quoted if it contains a "
            },
            {
              "type": "inlineCode",
              "value": "\\n"
            },
            {
              "type": "text",
              "value": " (is multiline) or '#'."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "If "
            },
            {
              "type": "inlineCode",
              "value": "write_empty_values"
            },
            {
              "type": "text",
              "value": " is set, and the value is an empty string, it won't be quoted."
            }
          ],
          "type": "paragraph"
        }
      ],
