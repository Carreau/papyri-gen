    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "ImageDataDiff"
            },
            {
              "type": "text",
              "value": " objects have the following diff attributes:"
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "inlineCode",
                      "value": "diff_dimensions"
                    },
                    {
                      "type": "text",
                      "value": ": If the two arrays contain either a different number   of dimensions or different sizes in any dimension, this contains a   2-tuple of the shapes of each array.  Currently no further comparison is   performed on images that don't have the exact same dimensions."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            },
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "inlineCode",
                      "value": "diff_pixels"
                    },
                    {
                      "type": "text",
                      "value": ": If the two images contain any different pixels, this   contains a list of 2-tuples of the array index where the difference was   found, and another 2-tuple containing the different values.  For example,   if the pixel at (0, 0) contains different values this would look like        "
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "type": "code",
                  "value": "[(0, 0), (1.1, 2.2)]"
                },
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "where 1.1 and 2.2 are the values of that pixel in each array.  This   array only contains up to "
                    },
                    {
                      "type": "inlineCode",
                      "value": "self.numdiffs"
                    },
                    {
                      "type": "text",
                      "value": " differences, for storage   efficiency."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            },
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "inlineCode",
                      "value": "diff_total"
                    },
                    {
                      "type": "text",
                      "value": ": The total number of different pixels found between the   arrays.  Although "
                    },
                    {
                      "type": "inlineCode",
                      "value": "diff_pixels"
                    },
                    {
                      "type": "text",
                      "value": " does not necessarily contain all the   different pixel values, this can be used to get a count of the total   number of differences found."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            },
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "inlineCode",
                      "value": "diff_ratio"
                    },
                    {
                      "type": "text",
                      "value": ": Contains the ratio of "
                    },
                    {
                      "type": "inlineCode",
                      "value": "diff_total"
                    },
                    {
                      "type": "text",
                      "value": " to the total number   of pixels in the arrays."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            }
          ],
          "ordered": false,
          "spread": false,
          "start": 1,
          "type": "list"
        }
