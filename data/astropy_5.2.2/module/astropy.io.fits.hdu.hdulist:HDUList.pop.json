              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "An integer value of "
                    },
                    {
                      "type": "inlineCode",
                      "value": "index"
                    },
                    {
                      "type": "text",
                      "value": " indicates the position from which "
                    },
                    {
                      "type": "inlineCode",
                      "value": "pop()"
                    },
                    {
                      "type": "text",
                      "value": " removes and returns an HDU. A string value or a tuple of "
                    },
                    {
                      "type": "inlineCode",
                      "value": "(string, int)"
                    },
                    {
                      "type": "text",
                      "value": " functions as a key for identifying the HDU to be removed and returned. If "
                    },
                    {
                      "type": "inlineCode",
                      "value": "key"
                    },
                    {
                      "type": "text",
                      "value": " is a tuple, it is of the form "
                    },
                    {
                      "type": "inlineCode",
                      "value": "(key, ver)"
                    },
                    {
                      "type": "text",
                      "value": " where "
                    },
                    {
                      "type": "inlineCode",
                      "value": "ver"
                    },
                    {
                      "type": "text",
                      "value": " is an "
                    },
                    {
                      "type": "inlineCode",
                      "value": "EXTVER"
                    },
                    {
                      "type": "text",
                      "value": " value that must match the HDU being searched for."
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "If the key is ambiguous (e.g. there are multiple 'SCI' extensions) the first match is returned.  For a more precise match use the "
                    },
                    {
                      "type": "inlineCode",
                      "value": "(name, ver)"
                    },
                    {
                      "type": "text",
                      "value": " pair."
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "If even the "
                    },
                    {
                      "type": "inlineCode",
                      "value": "(name, ver)"
                    },
                    {
                      "type": "text",
                      "value": " pair is ambiguous the numeric index must be used to index the duplicate HDU."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "index",
              "type": "Param",
              "type_": "int, str, tuple of (string, int), optional"
            }
