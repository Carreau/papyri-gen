              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "A filename or an open file-like object from which a FITS header is to be read.  For open file handles the file pointer must be at the beginning of the header."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "fileobj",
              "type": "Param",
              "type_": "str, file-like"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "The string separating cards from each other, such as a newline.  By default there is no card separator (as is the case in a raw FITS file)."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "sep",
              "type": "Param",
              "type_": "str, optional"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "If True (the default) the header must end with an END card in order to be considered valid.  If an END card is not found an "
                    },
                    {
                      "domain": null,
                      "role": null,
                      "type": "Directive",
                      "value": "OSError"
                    },
                    {
                      "type": "text",
                      "value": " is raised."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "endcard",
              "type": "Param",
              "type_": "bool, optional"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "If True (the default) the header will be required to be padded out to a multiple of 2880, the FITS header block size.  Otherwise any padding, or lack thereof, is ignored."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "padding",
              "type": "Param",
              "type_": "bool, optional"
            }
          ],
