    },
    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "Although FITS keywords must be exclusively upper case, retrieving an item in a "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "Header"
            },
            {
              "type": "text",
              "value": " object is case insensitive."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
