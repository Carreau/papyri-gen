    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "The provided "
            },
            {
              "type": "inlineCode",
              "value": "url_key"
            },
            {
              "type": "text",
              "value": " will be the name used in the cache. The file should contain the contents of this URL, at least notionally (the URL may be temporarily or permanently unavailable). It is using "
            },
            {
              "type": "inlineCode",
              "value": "url_key"
            },
            {
              "type": "text",
              "value": " that users will request these contents from the cache. See "
            },
            {
              "type": "inlineCode",
              "value": "download_file"
            },
            {
              "type": "text",
              "value": " for details."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "If "
            },
            {
              "type": "inlineCode",
              "value": "url_key"
            },
            {
              "type": "text",
              "value": " already exists in the cache, it will be updated to point to these imported contents, and its old contents will be deleted from the cache."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
    "Methods": {
      "children": [],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
