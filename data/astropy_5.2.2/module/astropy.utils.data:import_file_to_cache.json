    "Parameters": {
      "children": [
        {
          "children": [
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "The key to index the file under. This should probably be the URL where the file was located, though if you obtained it from a mirror you should use the URL of the primary location."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "url_key",
              "type": "Param",
              "type_": "str"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "The file whose contents you want to import."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "filename",
              "type": "Param",
              "type_": "str"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Whether to remove the original file ("
                    },
                    {
                      "type": "inlineCode",
                      "value": "filename"
                    },
                    {
                      "type": "text",
                      "value": ") once import is complete."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "remove_original",
              "type": "Param",
              "type_": "bool"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "The package name to use to locate the download cache. i.e. for "
                    },
                    {
                      "type": "inlineCode",
                      "value": "pkgname='astropy'"
                    },
                    {
                      "type": "text",
                      "value": " the default cache location is "
                    },
                    {
                      "type": "inlineCode",
                      "value": "~/.astropy/cache"
                    },
                    {
                      "type": "text",
                      "value": "."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "pkgname",
              "type": "Param",
              "type_": "`str`, optional"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Whether or not to replace an existing object in the cache, if one exists. If replacement is not requested but the object exists, silently pass."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "replace",
              "type": "Param",
              "type_": "boolean, optional"
            }
          ],
