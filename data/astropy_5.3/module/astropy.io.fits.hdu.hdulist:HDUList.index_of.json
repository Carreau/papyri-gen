              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "The key identifying the HDU.  If "
                    },
                    {
                      "type": "inlineCode",
                      "value": "key"
                    },
                    {
                      "type": "text",
                      "value": " is a tuple, it is of the form "
                    },
                    {
                      "type": "inlineCode",
                      "value": "(name, ver)"
                    },
                    {
                      "type": "text",
                      "value": " where "
                    },
                    {
                      "type": "inlineCode",
                      "value": "ver"
                    },
                    {
                      "type": "text",
                      "value": " is an "
                    },
                    {
                      "type": "inlineCode",
                      "value": "EXTVER"
                    },
                    {
                      "type": "text",
                      "value": " value that must match the HDU being searched for."
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "If the key is ambiguous (e.g. there are multiple 'SCI' extensions) the first match is returned.  For a more precise match use the "
                    },
                    {
                      "type": "inlineCode",
                      "value": "(name, ver)"
                    },
                    {
                      "type": "text",
                      "value": " pair."
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "If even the "
                    },
                    {
                      "type": "inlineCode",
                      "value": "(name, ver)"
                    },
                    {
                      "type": "text",
                      "value": " pair is ambiguous (it shouldn't be but it's not impossible) the numeric index must be used to index the duplicate HDU."
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "When "
                    },
                    {
                      "type": "inlineCode",
                      "value": "key"
                    },
                    {
                      "type": "text",
                      "value": " is an HDU object, this function returns the index of that HDU object in the "
                    },
                    {
                      "type": "inlineCode",
                      "value": "HDUList"
                    },
                    {
                      "type": "text",
                      "value": "."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "key",
              "type": "Param",
              "type_": "int, str, tuple of (string, int) or BaseHDU"
            }
          ],
          "type": "Parameters"
        }
