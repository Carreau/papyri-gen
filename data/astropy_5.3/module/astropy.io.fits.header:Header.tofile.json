      "children": [
        {
          "children": [
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Either the pathname of a file, or an open file handle or file-like object."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "fileobj",
              "type": "Param",
              "type_": "path-like or file-like, optional"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "The character or string with which to separate cards.  By default there is no separator, but one could use "
                    },
                    {
                      "type": "inlineCode",
                      "value": "'\\\\n'"
                    },
                    {
                      "type": "text",
                      "value": ", for example, to separate each card with a new line"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "sep",
              "type": "Param",
              "type_": "str, optional"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "If "
                    },
                    {
                      "domain": null,
                      "role": null,
                      "type": "Directive",
                      "value": "True"
                    },
                    {
                      "type": "text",
                      "value": " (default) adds the END card to the end of the header string"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "endcard",
              "type": "Param",
              "type_": "bool, optional"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "If "
                    },
                    {
                      "domain": null,
                      "role": null,
                      "type": "Directive",
                      "value": "True"
                    },
                    {
                      "type": "text",
                      "value": " (default) pads the string with spaces out to the next multiple of 2880 characters"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "padding",
              "type": "Param",
              "type_": "bool, optional"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "If "
                    },
                    {
                      "type": "inlineCode",
                      "value": "True"
                    },
                    {
                      "type": "text",
                      "value": ", overwrite the output file if it exists. Raises an "
                    },
                    {
                      "type": "inlineCode",
                      "value": "OSError"
                    },
                    {
                      "type": "text",
                      "value": " if "
                    },
                    {
                      "type": "inlineCode",
                      "value": "False"
                    },
                    {
                      "type": "text",
                      "value": " and the output file exists. Default is "
                    },
                    {
                      "type": "inlineCode",
                      "value": "False"
                    },
                    {
