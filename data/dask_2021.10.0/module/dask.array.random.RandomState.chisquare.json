    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The variable obtained by summing the squares of "
                },
                "type": "Words"
              },
              {
                "data": {
                  "anchor": null,
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "df",
                    "version": null
                  },
                  "value": "df"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " independent, standard normally distributed random variables:"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "Q = \\sum_{i=0}^{\\mathtt{df}} X^2_i"
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "is chi-square distributed, denoted"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "Q \\sim \\chi^2_k."
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The probability density function of the chi-squared distribution is"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "p(x) = \\frac{(1/2)^{k/2}}{\\Gamma(k/2)}x^{k/2 - 1} e^{-x/2},"
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\Gamma"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is the gamma function,"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "\\Gamma(x) = \\int_0^{-\\infty} t^{x - 1} e^{-t} dt."
          },
          "type": "BlockMath"
        }
      ],
      "level": 0,
      "target": null,
      "title": null
    },
