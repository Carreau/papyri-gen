    "Notes": {
      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "The probability density function for the noncentral Chi-square distribution is"
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "P(x;df,nonc) = \\sum^{\\infty}_{i=0} \\frac { e^ { - nonc / 2} ( nonc / 2)^ { i}} { i!} P_ { Y_ { df+2i}} ( x), "
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "Y_{q}"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is the Chi-square with q degrees of freedom."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
