    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The probability density function of the uniform distribution is"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "p(x) = \\frac{1}{b - a}"
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "anywhere within the interval "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "[a, b)"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ", and zero elsewhere."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "When "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "high"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " == "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "low"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ", values of "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "low"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " will be returned. If "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "high"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " < "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "low"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ", the results are officially undefined and may eventually raise an error, i.e. do not rely on this function to behave when passed arguments satisfying that inequality condition. The "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "high"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " limit may be included in the returned array of floats due to floating-point rounding in the equation "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "low + (high-low) * random_sample()"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ". For example:"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": ">>> x = np.float32(5*0.99999999)  # doctest: +SKIP\n>>> x  # doctest: +SKIP\n5.0"
          },
          "type": "BlockVerbatim"
        }
