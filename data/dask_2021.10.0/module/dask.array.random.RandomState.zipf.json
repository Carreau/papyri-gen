        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "Samples are drawn from a Zipf distribution with specified parameter "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "a",
                    "version": null
                  },
                  "value": "a"
                },
