                    "path": "dask.array.reductions.sum",
                    "version": "2021.10.0"
                  },
                  "value": "sum"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " with the kwarg "
                },
                "type": "Words"
              },
              {
                "data": {
                  "anchor": null,
                  "exists": true,
