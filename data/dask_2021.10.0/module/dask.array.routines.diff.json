        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "The first difference is given by "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "out[i] = a[i+1] - a[i]"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " along the given axis, higher differences are calculated by using "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
