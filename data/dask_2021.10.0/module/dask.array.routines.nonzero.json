    "Notes": {
      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "While the nonzero values can be obtained with "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "a[nonzero(a)]"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ", it is recommended to use "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "x[x.astype(bool)]"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " or "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "x[x != 0]"
                  ]
                },
                "type": "Verbatim"
