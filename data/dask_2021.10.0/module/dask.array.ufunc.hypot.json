        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Given the \"legs\" of a right triangle, return its hypotenuse."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Equivalent to "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "sqrt(x1**2 + x2**2)"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ", element-wise.  If "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "x1"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " or "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "x2"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " is scalar_like (i.e., unambiguously cast-able to a scalar type), it is broadcast for use with each element of the other argument. (See Examples)"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
