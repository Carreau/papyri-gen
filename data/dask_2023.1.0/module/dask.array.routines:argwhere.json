    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "inlineCode",
              "value": "np.argwhere(a)"
            },
            {
              "type": "text",
              "value": " is almost the same as "
            },
            {
              "type": "inlineCode",
              "value": "np.transpose(np.nonzero(a))"
            },
            {
              "type": "text",
              "value": ", but produces a result of the correct shape for a 0D array."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "The output of "
            },
            {
              "type": "inlineCode",
              "value": "argwhere"
            },
            {
              "type": "text",
              "value": " is not suitable for indexing arrays. For this purpose use "
            },
            {
              "type": "inlineCode",
              "value": "nonzero(a)"
            },
            {
              "type": "text",
              "value": " instead."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
