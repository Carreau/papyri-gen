              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Array containing data to be averaged. If "
                    },
                    {
                      "anchor": null,
                      "exists": true,
                      "kind": "local",
                      "reference": {
                        "kind": "local",
                        "module": null,
                        "path": "a",
