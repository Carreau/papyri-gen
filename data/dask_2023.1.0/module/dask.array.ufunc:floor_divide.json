    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "Some inconsistencies with the Dask version may exist."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "Return the largest integer smaller or equal to the division of the inputs. It is equivalent to the Python "
            },
            {
              "type": "inlineCode",
              "value": "//"
            },
            {
              "type": "text",
              "value": " operator and pairs with the Python "
            },
            {
              "type": "inlineCode",
              "value": "%"
            },
            {
              "type": "text",
              "value": " ("
            },
            {
              "anchor": null,
              "exists": true,
              "kind": "module",
              "reference": {
                "kind": "module",
                "module": "dask",
                "path": "dask.array.ufunc:remainder",
                "type": "RefInfo",
                "version": "2023.1.0"
              },
              "type": "Link",
              "value": "remainder"
            },
            {
              "type": "text",
              "value": "), function so that "
            },
            {
              "type": "inlineCode",
              "value": "a = a % b + b * (a // b)"
            },
            {
              "type": "text",
              "value": " up to roundoff."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
