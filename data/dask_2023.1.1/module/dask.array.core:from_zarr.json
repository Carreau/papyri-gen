              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Location of the data. A URL can include a protocol specifier like s3:// for remote data. Can also be any MutableMapping instance, which should be serializable if used in multiple processes."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "url: Zarr Array or str or MutableMapping",
              "type": "Param",
              "type_": ""
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "If the location is a zarr group rather than an array, this is the subcomponent that should be loaded, something like "
                    },
                    {
                      "type": "inlineCode",
                      "value": "'foo/bar'"
                    },
                    {
                      "type": "text",
                      "value": "."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "component: str or None",
              "type": "Param",
              "type_": ""
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Any additional parameters for the storage backend (ignored for local paths)"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "storage_options: dict",
              "type": "Param",
              "type_": ""
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Passed to "
                    },
                    {
                      "type": "inlineCode",
                      "value": "dask.array.from_array"
                    },
                    {
                      "type": "text",
                      "value": ", allows setting the chunks on initialisation, if the chunking scheme in the on-disc dataset is not optimal for the calculations to follow."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "chunks: tuple of ints or tuples of ints",
              "type": "Param",
              "type_": ""
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "An optional keyname for the array.  Defaults to hashing the input"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "name",
              "type": "Param",
              "type_": "str, optional"
            },
            {
