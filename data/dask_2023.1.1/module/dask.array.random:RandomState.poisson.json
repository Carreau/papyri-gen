    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "The Poisson distribution"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "f(k; \\lambda)=\\frac{\\lambda^k e^{-\\lambda}}{k!}"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "For events with an expected separation "
            },
            {
              "type": "inlineMath",
              "value": "\\lambda"
            },
            {
              "type": "text",
              "value": " the Poisson distribution "
            },
            {
              "type": "inlineMath",
              "value": "f(k; \\lambda)"
            },
            {
              "type": "text",
              "value": " describes the probability of "
            },
            {
              "type": "inlineMath",
              "value": "k"
            },
            {
              "type": "text",
              "value": " events occurring within the observed interval "
            },
            {
              "type": "inlineMath",
              "value": "\\lambda"
            },
            {
              "type": "text",
              "value": "."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "Because the output is limited to the range of the C int64 type, a ValueError is raised when "
            },
            {
              "anchor": null,
              "exists": true,
              "kind": "local",
              "reference": {
                "kind": "local",
                "module": null,
                "path": "lam",
                "type": "RefInfo",
                "version": null
              },
              "type": "Link",
              "value": "lam"
            },
            {
              "type": "text",
              "value": " is within 10 sigma of the maximum representable value."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
