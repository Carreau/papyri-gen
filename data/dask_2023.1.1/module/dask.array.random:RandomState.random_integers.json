    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "To sample from N evenly spaced floating-point numbers between a and b, use    "
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "code",
          "value": "a + (b - a) * (np.random.random_integers(N) - 1) / (N - 1.)"
        }
      ],
      "level": 0,
