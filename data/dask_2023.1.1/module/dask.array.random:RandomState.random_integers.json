      },
      {
        "type": "text",
        "value": "Roll two six sided dice 1000 times and sum the results:"
      },
      {
        "type": "code",
        "value": "d1 = np.random.random_integers(1, 6, 1000)  # doctest: +SKIP\nd2 = np.random.random_integers(1, 6, 1000)  # doctest: +SKIP\ndsums = d1 + d2  # doctest: +SKIP"
      },
      {
        "type": "text",
        "value": "Display results as a histogram:"
      },
      {
        "type": "code",
        "value": "import matplotlib.pyplot as plt  # doctest: +SKIP\ncount, bins, ignored = plt.hist(dsums, 11, density=True)  # doctest: +SKIP\nplt.show()  # doctest: +SKIP"
      }
    ],
