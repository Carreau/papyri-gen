    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "NaN values are propagated, that is if at least one item is NaN, the corresponding max value will be NaN as well. To ignore NaN values (MATLAB behavior), please use nanmax."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "Don't use "
            },
            {
              "anchor": null,
              "exists": true,
              "kind": "local",
              "reference": {
                "kind": "local",
                "module": null,
                "path": "amax",
                "type": "RefInfo",
                "version": null
              },
              "type": "Link",
              "value": "amax"
            },
            {
              "type": "text",
              "value": " for element-wise comparison of 2 arrays; when "
            },
            {
              "type": "inlineCode",
              "value": "a.shape[0]"
            },
            {
              "type": "text",
              "value": " is 2, "
            },
            {
              "type": "inlineCode",
              "value": "maximum(a[0], a[1])"
            },
            {
              "type": "text",
              "value": " is faster than "
            },
            {
              "type": "inlineCode",
              "value": "amax(a, axis=0)"
            },
            {
              "type": "text",
              "value": "."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
