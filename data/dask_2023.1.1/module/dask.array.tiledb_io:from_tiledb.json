      {
        "type": "code",
        "value": "import tempfile, tiledb\nimport dask.array as da, numpy as np\nuri = tempfile.NamedTemporaryFile().name\n_ = tiledb.from_numpy(uri, np.arange(0,9).reshape(3,3))  # create a tiledb array\ntdb_ar = da.from_tiledb(uri)  # read back the array\ntdb_ar.shape"
      },
      {
        "type": "code",
        "value": "tdb_ar.mean().compute()"
      }
