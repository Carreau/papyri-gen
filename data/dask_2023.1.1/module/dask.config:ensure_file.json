    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "This tries to move a default configuration file to a default location if if does not already exist.  It also comments out that file by default."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "This is to be used by downstream modules (like dask.distributed) that may have default configuration files that they wish to include in the default configuration path."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
