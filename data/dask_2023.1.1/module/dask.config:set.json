    "children": [
      {
        "type": "code",
        "value": "import dask"
      },
      {
        "type": "text",
        "value": "Set ``'foo.bar'`` in a context, by providing a mapping."
      },
      {
        "type": "code",
        "value": "with dask.config.set({'foo.bar': 123}):\n    pass"
      },
      {
        "type": "text",
        "value": "Set ``'foo.bar'`` in a context, by providing a keyword argument."
      },
      {
        "type": "code",
        "value": "with dask.config.set(foo__bar=123):\n    pass"
      },
      {
        "type": "text",
        "value": "Set ``'foo.bar'`` globally."
      },
      {
        "type": "code",
        "value": "dask.config.set(foo__bar=123)  # doctest: +SKIP"
      }
    ],
