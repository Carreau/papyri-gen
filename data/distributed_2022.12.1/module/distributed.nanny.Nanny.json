    },
    "Parameters": {
      "children": [
        {
          "data": {
            "children": [
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Environment variables set at time of Nanny initialization will be ensured to be set in the Worker process as well. This argument allows to overwrite or otherwise set environment variables for the Worker. It is also possible to set environment variables using the option "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "distributed.nanny.environ"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": ". Precedence as follows"
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  },
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "children": [
                              {
                                "children": [
                                  {
                                    "data": {
                                      "children": [
                                        {
                                          "data": {
                                            "value": "Nanny arguments"
                                          },
                                          "type": "Words"
                                        }
                                      ]
                                    },
                                    "type": "Paragraph"
                                  }
                                ]
                              },
                              {
                                "children": [
                                  {
                                    "data": {
                                      "children": [
                                        {
                                          "data": {
                                            "value": "Existing environment variables"
                                          },
                                          "type": "Words"
                                        }
                                      ]
                                    },
                                    "type": "Paragraph"
                                  }
                                ]
                              },
                              {
                                "children": [
                                  {
                                    "data": {
                                      "children": [
                                        {
                                          "data": {
                                            "value": "Dask configuration"
                                          },
                                          "type": "Words"
                                        }
                                      ]
                                    },
                                    "type": "Paragraph"
                                  }
                                ]
                              }
                            ]
                          },
                          "type": "EnumeratedList"
                        }
                      ]
                    },
                    "type": "BlockQuote"
                  },
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "children": [
                              {
                                "data": {
                                  "value": "Some environment variables, like "
                                },
                                "type": "Words"
                              },
                              {
                                "data": {
                                  "value": [
                                    "OMP_NUM_THREADS"
                                  ]
                                },
                                "type": "Verbatim"
                              },
                              {
                                "data": {
                                  "value": ", must be set before importing numpy to have effect. Others, like "
                                },
                                "type": "Words"
                              },
                              {
                                "data": {
                                  "value": [
                                    "MALLOC_TRIM_THRESHOLD_"
                                  ]
                                },
                                "type": "Verbatim"
                              },
                              {
                                "data": {
                                  "value": " (see "
                                },
                                "type": "Words"
                              },
                              {
                                "data": {
                                  "domain": null,
                                  "role": "ref",
                                  "value": "memtrim"
                                },
                                "type": "Directive"
                              },
                              {
                                "data": {
                                  "value": "), must be set before starting the Linux process. Such variables would be ineffective if set here or in "
                                },
                                "type": "Words"
                              },
                              {
                                "data": {
                                  "value": [
                                    "distributed.nanny.environ"
                                  ]
                                },
                                "type": "Verbatim"
                              },
                              {
                                "data": {
                                  "value": "; they must be set in "
                                },
                                "type": "Words"
                              },
                              {
                                "data": {
                                  "value": [
                                    "distributed.nanny.pre-spawn-environ"
                                  ]
                                },
                                "type": "Verbatim"
                              },
                              {
                                "data": {
                                  "value": " so that they are set before spawning the subprocess, even if this means poisoning the process running the Nanny."
                                },
                                "type": "Words"
                              }
                            ]
                          },
                          "type": "Paragraph"
                        },
                        {
                          "data": {
                            "children": [
                              {
                                "data": {
                                  "value": "For the same reason, be warned that changing "
                                },
                                "type": "Words"
                              },
                              {
                                "data": {
                                  "value": [
                                    "distributed.worker.multiprocessing-method"
                                  ]
                                },
                                "type": "Verbatim"
                              },
                              {
                                "data": {
                                  "value": " from "
                                },
                                "type": "Words"
                              },
                              {
                                "data": {
                                  "value": [
                                    "spawn"
                                  ]
                                },
                                "type": "Verbatim"
                              },
                              {
                                "data": {
                                  "value": " to "
                                },
                                "type": "Words"
                              },
                              {
                                "data": {
                                  "value": [
                                    "fork"
                                  ]
                                },
                                "type": "Verbatim"
                              },
                              {
                                "data": {
                                  "value": " or "
                                },
                                "type": "Words"
                              },
                              {
                                "data": {
                                  "value": [
                                    "forkserver"
                                  ]
                                },
                                "type": "Verbatim"
                              },
                              {
                                "data": {
                                  "value": " may inhibit some environment variables; if you do, you should set the variables yourself in the shell before you start "
                                },
                                "type": "Words"
                              },
                              {
                                "data": {
                                  "value": [
                                    "dask-worker"
                                  ]
                                },
                                "type": "Verbatim"
                              },
                              {
                                "data": {
                                  "value": "."
                                },
                                "type": "Words"
                              }
                            ]
                          },
                          "type": "Paragraph"
                        }
                      ],
