          "data": {
            "children": [
              {
                "data": {
                  "value": "A drop-in replacement of the built-in "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "warnings.warn()"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " function for issuing warnings remotely from workers to clients."
                },
                "type": "Words"
              }
            ]
          },
