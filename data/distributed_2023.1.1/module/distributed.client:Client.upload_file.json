    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "This sends a local file up to all worker nodes.  This file is placed into the working directory of the running worker, see config option "
            },
            {
              "type": "inlineCode",
              "value": "temporary-directory"
            },
            {
              "type": "text",
              "value": " (defaults to "
            },
            {
              "type": "inlineCode",
              "value": "tempfile.gettempdir"
            },
            {
              "type": "text",
              "value": ")."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "This directory will be added to the Python's system path so any .py, .egg or .zip  files will be importable."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
