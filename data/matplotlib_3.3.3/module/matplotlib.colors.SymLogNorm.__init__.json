    "Parameters": {
      "children": [
        {
          "type": "Param",
          "data": {
            "param": "linthresh",
            "type_": "float",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "The range within which the plot is linear (to avoid having the plot go to infinity around zero)."
                      }
                    }
                  ],
                  "inner": []
                }
              }
            ]
          }
        },
        {
          "type": "Param",
          "data": {
            "param": "linscale",
            "type_": "float, default: 1",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "This allows the linear range (-*linthresh* to *linthresh*) to be stretched relative to the logarithmic range. Its value is the number of powers of *base* to use for each half of the linear range."
                      }
                    }
                  ],
                  "inner": []
                }
              },
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "For example, when *linscale* == 1.0 (the default) and "
                      }
                    },
                    {
                      "type": "Verbatim",
                      "data": {
                        "value": [
                          "base=10"
                        ]
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": ", then space used for the positive and negative halves of the linear range will be equal to a decade in the logarithmic."
                      }
                    }
                  ],
                  "inner": []
                }
              }
            ]
          }
        },
        {
          "type": "Param",
          "data": {
            "param": "base",
            "type_": "float, default: None",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "If not given, defaults to "
                      }
                    },
                    {
                      "type": "Verbatim",
                      "data": {
                        "value": [
                          "np.e"
                        ]
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": " (consistent with prior behavior) and warns."
                      }
                    }
                  ],
                  "inner": []
                }
              },
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "In v3.3 the default value will change to 10 to be consistent with "
                      }
                    },
                    {
                      "type": "Directive",
                      "data": {
                        "value": [
                          ".SymLogNorm"
                        ],
                        "domain": null,
                        "role": null
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": "."
                      }
                    }
                  ],
                  "inner": []
                }
              },
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "To suppress the warning pass *base* as a keyword argument."
                      }
                    }
                  ],
                  "inner": []
                }
              }
            ]
          }
        }
