                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "Mapping of tick frequencies to multiples allowed for that ticking. The default is ::"
                      }
                    }
                  ],
                  "inner": []
                }
              },
              {
                "type": "BlockVerbatim",
                "data": {
                  "lines": {
                    "_lines": [
                      {
                        "_line": "    self.intervald = {",
                        "_number": 3,
                        "_offset": 4
                      },
                      {
                        "_line": "        YEARLY  : [1, 2, 4, 5, 10, 20, 40, 50, 100, 200, 400, 500,",
                        "_number": 4,
                        "_offset": 4
                      },
                      {
                        "_line": "                   1000, 2000, 4000, 5000, 10000],",
                        "_number": 5,
                        "_offset": 4
                      },
                      {
                        "_line": "        MONTHLY : [1, 2, 3, 4, 6],",
                        "_number": 6,
                        "_offset": 4
                      },
                      {
                        "_line": "        DAILY   : [1, 2, 3, 7, 14, 21],",
                        "_number": 7,
                        "_offset": 4
                      },
                      {
                        "_line": "        HOURLY  : [1, 2, 3, 4, 6, 12],",
                        "_number": 8,
                        "_offset": 4
                      },
                      {
                        "_line": "        MINUTELY: [1, 5, 10, 15, 30],",
                        "_number": 9,
                        "_offset": 4
                      },
                      {
                        "_line": "        SECONDLY: [1, 5, 10, 15, 30],",
                        "_number": 10,
                        "_offset": 4
                      },
                      {
                        "_line": "        MICROSECONDLY: [1, 2, 5, 10, 20, 50, 100, 200, 500,",
                        "_number": 11,
                        "_offset": 4
                      },
                      {
                        "_line": "                        1000, 2000, 5000, 10000, 20000, 50000,",
                        "_number": 12,
                        "_offset": 4
                      },
                      {
                        "_line": "                        100000, 200000, 500000, 1000000],",
                        "_number": 13,
                        "_offset": 4
                      },
                      {
                        "_line": "    }",
                        "_number": 14,
                        "_offset": 4
                      },
                      {
                        "_line": "",
                        "_number": 15,
                        "_offset": 4
                      }
                    ]
                  }
                }
              },
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "where the keys are defined in "
                      }
                    },
                    {
                      "type": "Directive",
                      "data": {
                        "value": [
                          "dateutil.rrule"
                        ],
                        "domain": null,
                        "role": null
                      }
                    },
                    {
                      "type": "Words",
