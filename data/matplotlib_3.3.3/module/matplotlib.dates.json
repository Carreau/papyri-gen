            ],
            "inner": [
              {
                "type": "BlockVerbatim",
                "data": {
                  "lines": {
                    "_lines": [
                      {
                        "_line": "# import constants for the days of the week",
                        "_number": 0,
                        "_offset": 0
                      },
                      {
                        "_line": "from matplotlib.dates import MO, TU, WE, TH, FR, SA, SU",
                        "_number": 1,
                        "_offset": 0
                      },
                      {
                        "_line": "",
                        "_number": 2,
                        "_offset": 0
                      },
                      {
                        "_line": "# tick on mondays every week",
                        "_number": 3,
                        "_offset": 0
                      },
                      {
                        "_line": "loc = WeekdayLocator(byweekday=MO, tz=tz)",
                        "_number": 4,
                        "_offset": 0
                      },
                      {
                        "_line": "",
                        "_number": 5,
                        "_offset": 0
                      },
                      {
                        "_line": "# tick on mondays and saturdays",
                        "_number": 6,
                        "_offset": 0
                      },
                      {
                        "_line": "loc = WeekdayLocator(byweekday=(MO, SA))",
                        "_number": 7,
                        "_offset": 0
                      }
                    ]
                  }
                }
              }
            ]
