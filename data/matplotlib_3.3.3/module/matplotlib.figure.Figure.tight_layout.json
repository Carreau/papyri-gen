          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "To exclude an artist on the axes from the bounding box calculation that determines the subplot parameters (i.e. legend, or annotation), set "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "a.set",
                    "_",
                    "in",
                    "_",
                    "layout(False)"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " for that artist."
                }
              }
            ]
          }
        }
      ]
    },
