            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "Plot y versus x as lines and/or markers with attached errorbars."
                }
              }
            ]
          }
        }
      ]
    },
    "Extended Summary": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "*x*, *y* define the data locations, *xerr*, *yerr* define the errorbar sizes. By default, this draws the data markers/lines as well the errorbars. Use fmt='none' to draw errorbars without any data markers."
                }
              }
            ]
          }
        }
      ]
    },
