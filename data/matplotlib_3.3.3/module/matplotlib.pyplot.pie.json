    "Extended Summary": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "Make a pie chart of array *x*.  The fractional area of each wedge is given by "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "x/sum(x)"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ".  If "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "sum(x)",
                    " ",
                    "<",
                    " ",
                    "1"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ", then the values of *x* give the fractional area directly and the array will not be normalized. The resulting pie will have an empty wedge of size "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "1",
                    " ",
                    "-",
                    " ",
                    "sum(x)"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": "."
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "The wedges are plotted counterclockwise, by default starting from the x-axis."
                }
              }
            ],
            "inner": []
          }
        }
      ],
      "title": null
