    "Parameters": {
      "children": [
        {
          "type": "Param",
          "data": {
            "param": "xmax",
            "type_": "float",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "children": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "Determines how the number is converted into a percentage. *xmax* is the data value that corresponds to 100%. Percentages are computed as "
                      }
                    },
                    {
                      "type": "Verbatim",
                      "data": {
                        "value": [
                          "x",
                          " ",
                          "/",
                          " ",
                          "xmax",
                          " ",
                          "*",
                          " ",
                          "100"
                        ]
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": ". So if the data is already scaled to be percentages, *xmax* will be 100. Another common situation is where *xmax* is 1.0."
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "type": "Param",
          "data": {
            "param": "decimals",
            "type_": "None or int",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "children": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "The number of decimal places to place after the point. If *None* (the default), the number will be computed automatically."
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "type": "Param",
          "data": {
            "param": "symbol",
            "type_": "str or None",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "children": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "A string that will be appended to the label. It may be *None* or empty to indicate that no symbol should be used. LaTeX special characters are escaped in *symbol* whenever latex mode is enabled, unless *is_latex* is *True*."
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "type": "Param",
          "data": {
            "param": "is_latex",
            "type_": "bool",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "children": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "If *False*, reserved LaTeX characters in *symbol* will be escaped."
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      ]
