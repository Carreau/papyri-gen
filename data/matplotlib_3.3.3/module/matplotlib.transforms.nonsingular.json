                "data": {
                  "children": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "If True, swap *vmin*, *vmax* if *vmin* > *vmax*."
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      ]
