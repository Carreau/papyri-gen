    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "This assumes a mapping "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "f : [0, 1] \\rightarrow [0, 1]"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ". The returned data is an array of N values "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "y = f(x)"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " where x is sampled from [0, 1]."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "By default ("
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": {
                    "value": "gamma"
                  }
                },
                "type": "Emph"
              },
              {
                "data": {
                  "value": " = 1) x is equidistantly sampled from [0, 1]. The "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": {
                    "value": "gamma"
                  }
                },
                "type": "Emph"
              },
              {
                "data": {
                  "value": " correction factor "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\gamma"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " distorts this equidistant sampling by "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "x \\rightarrow x^\\gamma"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": "."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
    "Methods": {
