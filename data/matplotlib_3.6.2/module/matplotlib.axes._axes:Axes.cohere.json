    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "Coherence is the normalized cross spectral density:"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "C_{xy} = \\frac{|P_{xy}|^2}{P_{xx}P_{yy}}"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
