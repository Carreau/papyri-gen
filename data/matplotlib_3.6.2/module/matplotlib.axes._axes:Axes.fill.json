    "Parameters": {
      "children": [
        {
          "children": [
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Each polygon is defined by the lists of "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "x"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": " and "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "y"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": " positions of its nodes, optionally followed by a "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "color"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": " specifier. See "
                    },
                    {
                      "type": "inlineCode",
                      "value": "matplotlib.colors"
                    },
                    {
                      "type": "text",
                      "value": " for supported color specifiers. The standard color cycle is used for polygons without a color specifier."
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "You can plot multiple polygons by providing multiple "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "x"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": ", "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "y"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": ", "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "[color]"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": " groups."
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "For example, each of the following is legal      "
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "type": "code",
                  "value": "ax.fill(x, y)                    # a polygon with default color\nax.fill(x, y, \"b\")               # a blue polygon\nax.fill(x, y, x2, y2)            # two polygons\nax.fill(x, y, \"b\", x2, y2, \"r\")  # a blue and a red polygon"
                }
              ],
              "param": "*args",
              "type": "Param",
              "type_": "sequence of x, y, [color]"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "An object with labelled data. If given, provide the label names to plot in "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "x"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": " and "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "y"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": ", e.g.      "
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "type": "code",
                  "value": "ax.fill(\"time\", \"signal\",\n        data={\"time\": [0, 1, 2], \"signal\": [0, 1, 0]})"
                }
              ],
              "param": "data",
              "type": "Param",
              "type_": "indexable object, optional"
            }
          ],
          "type": "Parameters"
        }
      ],
