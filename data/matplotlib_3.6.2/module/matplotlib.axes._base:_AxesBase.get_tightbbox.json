            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "List of artists to include in the tight bounding box.  If "
                    },
                    {
                      "type": "inlineCode",
                      "value": "None"
                    },
                    {
                      "type": "text",
                      "value": " (default), then all artist children of the Axes are included in the tight bounding box."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "bbox_extra_artists",
              "type": "Param",
              "type_": "list of `.Artist` or ``None``"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "If "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "call_axes_locator"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": " is "
                    },
                    {
                      "type": "inlineCode",
                      "value": "False"
                    },
                    {
                      "type": "text",
                      "value": ", it does not call the "
                    },
                    {
                      "type": "inlineCode",
                      "value": "_axes_locator"
                    },
                    {
                      "type": "text",
                      "value": " attribute, which is necessary to get the correct bounding box. "
                    },
                    {
                      "type": "inlineCode",
                      "value": "call_axes_locator=False"
                    },
                    {
                      "type": "text",
                      "value": " can be used if the caller is only interested in the relative size of the tightbbox compared to the Axes bbox."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "call_axes_locator",
              "type": "Param",
              "type_": "bool, default: True"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "The bounding box will "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "not"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": " include the x-extent of the title and the xlabel, or the y-extent of the ylabel."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "for_layout_only",
              "type": "Param",
              "type_": "default: False"
