      "children": [
        {
          "children": [
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "If 'box', change the physical dimensions of the Axes. If 'datalim', change the "
                    },
                    {
                      "type": "inlineCode",
                      "value": "x"
                    },
                    {
                      "type": "text",
                      "value": " or "
                    },
                    {
                      "type": "inlineCode",
                      "value": "y"
                    },
                    {
                      "type": "text",
                      "value": " data limits."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "adjustable",
              "type": "Param",
              "type_": "{'box', 'datalim'}"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "If "
                    },
                    {
                      "type": "inlineCode",
                      "value": "True"
                    },
                    {
                      "type": "text",
                      "value": ", apply the settings to all shared Axes."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "share",
              "type": "Param",
              "type_": "bool, default: False"
            }
          ],
