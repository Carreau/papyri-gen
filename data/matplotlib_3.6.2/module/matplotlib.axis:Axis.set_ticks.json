    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "The mandatory expansion of the view limits is an intentional design choice to prevent the surprise of a non-visible tick. If you need other limits, you should set the limits explicitly after setting the ticks."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
