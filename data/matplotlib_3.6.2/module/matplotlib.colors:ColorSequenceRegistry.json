    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "The universal registry instance is "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "matplotlib.color_sequences"
            },
            {
              "type": "text",
              "value": ". There should be no need for users to instantiate "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": ".ColorSequenceRegistry"
            },
            {
              "type": "text",
              "value": " themselves."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "Read access uses a dict-like interface mapping names to lists of colors      "
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "code",
          "value": "import matplotlib as mpl\ncmap = mpl.color_sequences['tab10']"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "The returned lists are copies, so that their modification does not change the global definition of the color sequence."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "Additional color sequences can be added via "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": ".ColorSequenceRegistry.register"
            },
            {
              "type": "text",
              "value": ":      "
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "code",
          "value": "mpl.color_sequences.register('rgb', ['r', 'g', 'b'])"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
