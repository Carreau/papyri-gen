  "example_section_data": {
    "children": [
      {
        "type": "text",
        "value": "::"
      },
      {
        "type": "text",
        "value": "    # First create some toy data:\n    x = np.linspace(0, 2*np.pi, 400)\n    y = np.sin(x**2)"
      },
      {
        "type": "text",
        "value": "    # Create a figure\n    plt.figure()"
      },
      {
        "type": "text",
        "value": "    # Create a subplot\n    ax = fig.subplots()\n    ax.plot(x, y)\n    ax.set_title('Simple plot')"
      },
      {
        "type": "text",
        "value": "    # Create two subplots and unpack the output array immediately\n    ax1, ax2 = fig.subplots(1, 2, sharey=True)\n    ax1.plot(x, y)\n    ax1.set_title('Sharing Y axis')\n    ax2.scatter(x, y)"
      },
      {
        "type": "text",
        "value": "    # Create four polar Axes and access them through the returned array\n    axes = fig.subplots(2, 2, subplot_kw=dict(projection='polar'))\n    axes[0, 0].plot(x, y)\n    axes[1, 1].scatter(x, y)"
      },
      {
        "type": "text",
        "value": "    # Share a X axis with each column of subplots\n    fig.subplots(2, 2, sharex='col')"
      },
      {
        "type": "text",
        "value": "    # Share a Y axis with each row of subplots\n    fig.subplots(2, 2, sharey='row')"
      },
      {
        "type": "text",
        "value": "    # Share both X and Y axes with all subplots\n    fig.subplots(2, 2, sharex='all', sharey='all')"
      },
      {
        "type": "text",
        "value": "    # Note that this is the same as\n    fig.subplots(2, 2, sharex=True, sharey=True)"
      }
    ],
