          "children": [
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "The point (x, y) to check."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "point",
              "type": "Param",
              "type_": "(float, float)"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "If not "
                    },
                    {
                      "type": "inlineCode",
                      "value": "None"
                    },
                    {
                      "type": "text",
                      "value": ", "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "point"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": " will be compared to "
                    },
                    {
                      "type": "inlineCode",
                      "value": "self"
                    },
                    {
                      "type": "text",
                      "value": " transformed by "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "transform"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": "; i.e. for a correct check, "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "transform"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": " should transform the path into the coordinate system of "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "point"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": "."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "transform",
              "type": "Param",
              "type_": "`matplotlib.transforms.Transform`, optional"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Add an additional margin on the path in coordinates of "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "point"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": ". The path is extended tangentially by "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "radius/2"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": "; i.e. if you would draw the path with a linewidth of "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "radius"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": ", all points on the line would still be considered to be contained in the area. Conversely, negative values shrink the area: Points on the imaginary line will be considered outside the area."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "radius",
              "type": "Param",
              "type_": "float, default: 0"
            }
          ],
          "type": "Parameters"
        }
