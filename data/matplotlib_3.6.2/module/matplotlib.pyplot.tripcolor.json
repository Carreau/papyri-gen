                "data": {
                  "value": ", "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": {
                    "value": "y"
                  }
                },
                "type": "Emph"
              },
              {
                "data": {
                  "value": " and optionally the "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": {
                    "value": "triangles"
                  }
                },
                "type": "Emph"
              },
              {
                "data": {
                  "value": " and a "
