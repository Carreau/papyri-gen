        {
          "children": [
            {
              "type": "text",
              "value": "Newly created figures are passed to the "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "~.FigureCanvasBase.new_manager"
            },
            {
              "type": "text",
