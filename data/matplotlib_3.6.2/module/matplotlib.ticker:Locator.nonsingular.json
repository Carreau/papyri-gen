    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "This method gets called during autoscaling, with "
            },
            {
              "type": "inlineCode",
              "value": "(v0, v1)"
            },
            {
              "type": "text",
              "value": " set to the data limits on the axes if the axes contains any data, or "
            },
            {
              "type": "inlineCode",
              "value": "(-inf, +inf)"
            },
            {
              "type": "text",
              "value": " if not."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "If "
                    },
                    {
                      "type": "inlineCode",
                      "value": "v0 == v1"
                    },
                    {
                      "type": "text",
                      "value": " (possibly up to some floating point slop), this   method returns an expanded interval around this value."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            },
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "If "
                    },
                    {
                      "type": "inlineCode",
                      "value": "(v0, v1) == (-inf, +inf)"
                    },
                    {
                      "type": "text",
                      "value": ", this method returns appropriate   default view limits."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            },
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Otherwise, "
                    },
                    {
                      "type": "inlineCode",
                      "value": "(v0, v1)"
                    },
                    {
                      "type": "text",
                      "value": " is returned without modification."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            }
          ],
          "ordered": false,
