          "children": [
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "If x > 1/2, with x = 1-v, indicate if x should be displayed as $\\overline{v}$. The default is to display $1-v$."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "use_overline",
              "type": "Param",
              "type_": "bool, default: False"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "The string used to represent 1/2."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "one_half",
              "type": "Param",
              "type_": "str, default: r\"\\frac{1}{2}\""
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Indicate if the formatter is formatting minor ticks or not. Basically minor ticks are not labelled, except when only few ticks are provided, ticks with most space with neighbor ticks are labelled. See other parameters to change the default behavior."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "minor",
              "type": "Param",
              "type_": "bool, default: False"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Maximum number of locs for labelling some minor ticks. This parameter have no effect if minor is False."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "minor_threshold",
              "type": "Param",
              "type_": "int, default: 25"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Number of ticks which are labelled when the number of ticks is below the threshold."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "minor_number",
              "type": "Param",
              "type_": "int, default: 6"
            }
          ],
          "type": "Parameters"
        }
      ],
