      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "The file location is determined in the following order"
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "inlineCode",
                      "value": "$PWD/matplotlibrc"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            },
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "inlineCode",
                      "value": "$MATPLOTLIBRC"
                    },
                    {
                      "type": "text",
                      "value": " if it is not a directory"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            },
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "inlineCode",
                      "value": "$MATPLOTLIBRC/matplotlibrc"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            },
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "inlineCode",
                      "value": "$MPLCONFIGDIR/matplotlibrc"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            },
            {
              "children": [
                {
                  "children": [
                    {
                      "dd": [
                        {
                          "children": [
                            {
                              "children": [
                                {
                                  "children": [
                                    {
                                      "type": "inlineCode",
                                      "value": "$XDG_CONFIG_HOME/matplotlib/matplotlibrc"
                                    },
                                    {
                                      "type": "text",
                                      "value": " (if "
                                    },
                                    {
                                      "type": "inlineCode",
                                      "value": "$XDG_CONFIG_HOME"
                                    },
                                    {
                                      "type": "text",
                                      "value": "       is defined)"
                                    }
                                  ],
                                  "type": "paragraph"
                                }
                              ],
                              "spread": false,
                              "type": "listItem"
                            },
                            {
                              "children": [
                                {
                                  "children": [
                                    {
                                      "type": "text",
                                      "value": "or "
                                    },
                                    {
                                      "type": "inlineCode",
                                      "value": "$HOME/.config/matplotlib/matplotlibrc"
                                    },
                                    {
                                      "type": "text",
                                      "value": " (if "
                                    },
                                    {
                                      "type": "inlineCode",
                                      "value": "$XDG_CONFIG_HOME"
                                    },
                                    {
                                      "type": "text",
                                      "value": "       is not defined)"
                                    }
                                  ],
                                  "type": "paragraph"
                                }
                              ],
                              "spread": false,
                              "type": "listItem"
                            }
                          ],
                          "ordered": false,
                          "spread": false,
                          "start": 1,
                          "type": "list"
                        }
                      ],
                      "dt": {
                        "children": [
                          {
                            "type": "text",
                            "value": "On Linux,"
                          }
                        ],
                        "type": "paragraph"
                      },
                      "type": "DefListItem"
                    }
                  ],
                  "type": "DefList"
                }
              ],
              "spread": false,
              "type": "listItem"
            },
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "On other platforms,   - "
                    },
                    {
                      "type": "inlineCode",
                      "value": "$HOME/.matplotlib/matplotlibrc"
                    },
                    {
                      "type": "text",
                      "value": " if "
                    },
                    {
                      "type": "inlineCode",
