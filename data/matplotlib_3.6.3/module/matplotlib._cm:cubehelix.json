        {
          "children": [
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Gamma factor emphasizing either low intensity values (gamma < 1), or high intensity values (gamma > 1)."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "gamma",
              "type": "Param",
              "type_": "float, default: 1"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "The starting color."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "s",
              "type": "Param",
              "type_": "float, default: 0.5 (purple)"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "The number of r, g, b rotations in color that are made from the start to the end of the color scheme.  The default of -1.5 corresponds to -> B -> G -> R -> B."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "r",
              "type": "Param",
              "type_": "float, default: -1.5"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "The hue, i.e. how saturated the colors are. If this parameter is zero then the color scheme is purely a greyscale."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "h",
              "type": "Param",
              "type_": "float, default: 1"
            }
          ],
          "type": "Parameters"
        }
      ],
