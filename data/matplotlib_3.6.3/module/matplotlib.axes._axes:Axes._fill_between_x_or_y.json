                }
              ],
              "param": "{dep}2",
              "type": "Param",
              "type_": "array (length N) or scalar, default: 0"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Define "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "where"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": " to exclude some {dir} regions from being filled. The filled regions are defined by the coordinates "
                    },
                    {
                      "type": "inlineCode",
                      "value": "{ind}[where]"
                    },
                    {
                      "type": "text",
                      "value": ". More precisely, fill between "
                    },
                    {
                      "type": "inlineCode",
                      "value": "{ind}[i]"
                    },
                    {
                      "type": "text",
                      "value": " and "
                    },
                    {
                      "type": "inlineCode",
                      "value": "{ind}[i+1]"
                    },
                    {
                      "type": "text",
                      "value": " if "
                    },
                    {
                      "type": "inlineCode",
                      "value": "where[i] and where[i+1]"
                    },
                    {
                      "type": "text",
                      "value": ".  Note that this definition implies that an isolated "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "True"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": " value between two "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "False"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": " values in "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "where"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": " will not result in filling.  Both sides of the "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "True"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": " position remain unfilled due to the adjacent "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "False"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": " values."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "where",
              "type": "Param",
              "type_": "array of bool (length N), optional"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "This option is only relevant if "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "where"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": " is used and the two curves are crossing each other."
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Semantically, "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "where"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": " is often used for "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "{dep}1"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": " > "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "{dep}2"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": " or similar.  By default, the nodes of the polygon defining the filled region will only be placed at the positions in the "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "{ind}"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": " array. Such a polygon cannot describe the above semantics close to the intersection.  The {ind}-sections containing the intersection are simply clipped."
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Setting "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "interpolate"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": " to "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "True"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": " will calculate the actual intersection point and extend the filled region up to this point."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "interpolate",
              "type": "Param",
              "type_": "bool, default: False"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Define "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "step"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": " if the filling should be a step function, i.e. constant in between "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "{ind}"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": ".  The value determines where the step will occur:"
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "children": [
                    {
                      "children": [
                        {
                          "children": [
                            {
                              "type": "text",
                              "value": "'pre': The y value is continued constantly to the left from   every "
                            },
                            {
                              "children": [
                                {
                                  "type": "text",
                                  "value": "x"
                                }
                              ],
                              "type": "emphasis"
                            },
                            {
                              "type": "text",
                              "value": " position, i.e. the interval "
                            },
                            {
                              "type": "inlineCode",
                              "value": "(x[i-1], x[i]]"
                            },
                            {
                              "type": "text",
                              "value": " has the   value "
                            },
                            {
                              "type": "inlineCode",
                              "value": "y[i]"
                            },
                            {
                              "type": "text",
                              "value": "."
                            }
                          ],
                          "type": "paragraph"
                        }
                      ],
                      "spread": false,
                      "type": "listItem"
                    },
                    {
                      "children": [
                        {
                          "children": [
                            {
                              "type": "text",
                              "value": "'post': The y value is continued constantly to the right from   every "
                            },
                            {
                              "children": [
                                {
                                  "type": "text",
                                  "value": "x"
                                }
                              ],
                              "type": "emphasis"
                            },
                            {
                              "type": "text",
                              "value": " position, i.e. the interval "
                            },
                            {
                              "type": "inlineCode",
                              "value": "[x[i], x[i+1])"
                            },
                            {
                              "type": "text",
                              "value": " has the   value "
                            },
                            {
                              "type": "inlineCode",
                              "value": "y[i]"
                            },
                            {
                              "type": "text",
                              "value": "."
                            }
                          ],
                          "type": "paragraph"
                        }
                      ],
                      "spread": false,
                      "type": "listItem"
                    },
                    {
                      "children": [
                        {
                          "children": [
                            {
                              "type": "text",
                              "value": "'mid': Steps occur half-way between the "
                            },
                            {
                              "children": [
                                {
                                  "type": "text",
                                  "value": "x"
                                }
                              ],
                              "type": "emphasis"
                            },
                            {
                              "type": "text",
                              "value": " positions."
                            }
                          ],
                          "type": "paragraph"
                        }
                      ],
                      "spread": false,
                      "type": "listItem"
                    }
                  ],
                  "ordered": false,
                  "spread": false,
                  "start": 1,
                  "type": "list"
                }
              ],
