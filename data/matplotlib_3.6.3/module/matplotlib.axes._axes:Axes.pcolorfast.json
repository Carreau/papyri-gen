  "arbitrary": [],
  "content": {
    "Attributes": {
      "children": [],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "Call signature    "
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "code",
          "value": "ax.pcolorfast([X, Y], C, /, **kwargs)"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "This method is similar to "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "~.Axes.pcolor"
            },
            {
              "type": "text",
              "value": " and "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "~.Axes.pcolormesh"
            },
            {
              "type": "text",
              "value": ". It's designed to provide the fastest pcolor-type plotting with the Agg backend. To achieve this, it uses different algorithms internally depending on the complexity of the input grid (regular rectangular, non-regular rectangular or arbitrary quadrilateral)."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "children": [
                {
                  "type": "text",
                  "value": "warning "
                }
              ],
              "type": "admonitionTitle"
            },
            {
              "children": [
                {
                  "type": "text",
                  "value": "This method is experimental. Compared to "
                },
                {
                  "domain": null,
                  "role": null,
                  "type": "Directive",
                  "value": "~.Axes.pcolor"
                },
                {
                  "type": "text",
                  "value": " or "
                },
                {
                  "domain": null,
                  "role": null,
                  "type": "Directive",
                  "value": "~.Axes.pcolormesh"
                },
                {
                  "type": "text",
                  "value": " it has some limitations:"
                }
              ],
              "type": "paragraph"
            },
            {
              "children": [
                {
                  "children": [
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "It supports only flat shading (no outlines)"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ],
                  "spread": false,
                  "type": "listItem"
                },
                {
                  "children": [
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "It lacks support for log scaling of the axes."
                        }
                      ],
                      "type": "paragraph"
                    }
                  ],
                  "spread": false,
                  "type": "listItem"
                },
                {
                  "children": [
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "It does not have a pyplot wrapper."
                        }
                      ],
                      "type": "paragraph"
                    }
                  ],
                  "spread": false,
                  "type": "listItem"
                }
              ],
              "ordered": false,
              "spread": false,
              "start": 1,
              "type": "list"
            }
          ],
          "kind": "note",
          "type": "admonition"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
    "Methods": {
      "children": [],
      "level": 0,
      "target": null,
