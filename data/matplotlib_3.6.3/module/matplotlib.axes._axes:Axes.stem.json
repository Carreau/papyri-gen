    "Parameters": {
      "children": [
        {
          "children": [
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "For vertical stem plots, the x-positions of the stems. For horizontal stem plots, the y-positions of the stems."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "locs",
              "type": "Param",
              "type_": "array-like, default: (0, 1, ..., len(heads) - 1)"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "For vertical stem plots, the y-values of the stem heads. For horizontal stem plots, the x-values of the stem heads."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "heads",
              "type": "Param",
              "type_": "array-like"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "A string defining the color and/or linestyle of the vertical lines:"
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "=========  ============= Character  Line Style =========  ============= "
                    },
                    {
                      "type": "inlineCode",
                      "value": "'-'"
                    },
                    {
                      "type": "text",
                      "value": "    solid line "
                    },
                    {
                      "type": "inlineCode",
                      "value": "'--'"
                    },
                    {
                      "type": "text",
                      "value": "   dashed line "
                    },
                    {
                      "type": "inlineCode",
                      "value": "'-.'"
                    },
                    {
                      "type": "text",
                      "value": "   dash-dot line "
                    },
                    {
                      "type": "inlineCode",
                      "value": "':'"
                    },
                    {
                      "type": "text",
                      "value": "    dotted line =========  ============="
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Default: 'C0-', i.e. solid line with the first color of the color cycle."
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Note: Markers specified through this parameter (e.g. 'x') will be silently ignored (unless using "
                    },
                    {
                      "type": "inlineCode",
                      "value": "use_line_collection=False"
                    },
                    {
                      "type": "text",
                      "value": "). Instead, markers should be specified using "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "markerfmt"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": "."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "linefmt",
              "type": "Param",
              "type_": "str, optional"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "A string defining the color and/or shape of the markers at the stem heads. If the marker is not given, use the marker 'o', i.e. filled circles. If the color is not given, use the color from "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "linefmt"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": "."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "markerfmt",
              "type": "Param",
              "type_": "str, optional"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "A format string defining the properties of the baseline."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "basefmt",
              "type": "Param",
              "type_": "str, default: 'C3-' ('C2-' in classic mode)"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "If 'vertical', will produce a plot with stems oriented vertically, If 'horizontal', the stems will be oriented horizontally."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "orientation",
              "type": "Param",
              "type_": "{'vertical', 'horizontal'}, default: 'vertical'"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "The y/x-position of the baseline (depending on orientation)."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "bottom",
              "type": "Param",
              "type_": "float, default: 0"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "The label to use for the stems in legends."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "label",
              "type": "Param",
              "type_": "str, default: None"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "Deprecated since 3.6"
                        }
                      ],
                      "type": "emphasis"
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "If "
                    },
                    {
                      "type": "inlineCode",
                      "value": "True"
                    },
                    {
                      "type": "text",
                      "value": ", store and plot the stem lines as a "
                    },
                    {
                      "domain": null,
                      "role": null,
                      "type": "Directive",
                      "value": "~.collections.LineCollection"
                    },
                    {
                      "type": "text",
                      "value": " instead of individual lines, which significantly increases performance.  If "
                    },
                    {
                      "type": "inlineCode",
                      "value": "False"
                    },
                    {
                      "type": "text",
                      "value": ", defaults to the old behavior of using a list of "
                    },
                    {
                      "domain": null,
                      "role": null,
                      "type": "Directive",
                      "value": ".Line2D"
                    },
                    {
                      "type": "text",
                      "value": " objects."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "use_line_collection",
              "type": "Param",
              "type_": "bool, default: True"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "If given, all parameters also accept a string "
                    },
                    {
                      "type": "inlineCode",
                      "value": "s"
                    },
                    {
                      "type": "text",
                      "value": ", which is interpreted as "
                    },
                    {
                      "type": "inlineCode",
                      "value": "data[s]"
                    },
                    {
                      "type": "text",
                      "value": " (unless this raises an exception)."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "data",
              "type": "Param",
              "type_": "indexable object, optional"
            }
          ],
          "type": "Parameters"
        }
      ],
