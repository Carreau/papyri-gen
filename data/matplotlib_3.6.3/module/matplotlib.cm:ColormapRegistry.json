    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "The universal registry instance is "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "matplotlib.colormaps"
            },
            {
              "type": "text",
              "value": ". There should be no need for users to instantiate "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": ".ColormapRegistry"
            },
            {
              "type": "text",
              "value": " themselves."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "Read access uses a dict-like interface mapping names to "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": ".Colormap"
            },
            {
              "type": "text",
              "value": "\\s      "
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "code",
          "value": "import matplotlib as mpl\ncmap = mpl.colormaps['viridis']"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "Returned "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": ".Colormap"
            },
            {
              "type": "text",
              "value": "\\s are copies, so that their modification does not change the global definition of the colormap."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "Additional colormaps can be added via "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": ".ColormapRegistry.register"
            },
            {
              "type": "text",
              "value": ":      "
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "code",
          "value": "mpl.colormaps.register(my_colormap)"
        }
      ],
