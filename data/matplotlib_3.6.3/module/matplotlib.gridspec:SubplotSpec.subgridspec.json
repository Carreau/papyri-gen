    "children": [
      {
        "type": "text",
        "value": "Adding three subplots in the space occupied by a single subplot::"
      },
      {
        "type": "text",
        "value": "    fig = plt.figure()\n    gs0 = fig.add_gridspec(3, 1)\n    ax1 = fig.add_subplot(gs0[0])\n    ax2 = fig.add_subplot(gs0[1])\n    gssub = gs0[2].subgridspec(1, 3)\n    for i in range(3):\n        fig.add_subplot(gssub[0, i])"
      }
    ],
