    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "Wait until the user clicks "
            },
            {
              "children": [
                {
                  "type": "text",
                  "value": "n"
                }
              ],
              "type": "emphasis"
            },
            {
              "type": "text",
              "value": " times on the figure, and return the coordinates of each click in a list."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "There are three possible interactions:"
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Add a point."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            },
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Remove the most recently added point."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            },
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Stop the interaction and return the points added so far."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            }
          ],
          "ordered": false,
          "spread": false,
          "start": 1,
          "type": "list"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "The actions are assigned to mouse buttons via the arguments "
            },
            {
              "children": [
                {
                  "type": "text",
                  "value": "mouse_add"
                }
              ],
              "type": "emphasis"
            },
            {
              "type": "text",
              "value": ", "
            },
            {
              "children": [
                {
                  "type": "text",
                  "value": "mouse_pop"
                }
              ],
              "type": "emphasis"
            },
            {
              "type": "text",
              "value": " and "
            },
            {
              "children": [
                {
                  "type": "text",
                  "value": "mouse_stop"
                }
              ],
              "type": "emphasis"
            },
            {
              "type": "text",
              "value": "."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
    "Methods": {
      "children": [],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "The keyboard can also be used to select points in case your mouse does not have one or more of the buttons.  The delete and backspace keys act like right-clicking (i.e., remove last point), the enter key terminates input and any other key (not already used by the window manager) selects a point."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
    "Other Parameters": {
      "children": [],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
    "Parameters": {
      "children": [
        {
          "children": [
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Number of mouse clicks to accumulate. If negative, accumulate clicks until the input is terminated manually."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "n",
              "type": "Param",
              "type_": "int, default: 1"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Number of seconds to wait before timing out. If zero or negative will never time out."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "timeout",
              "type": "Param",
              "type_": "float, default: 30 seconds"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "If True, show a red cross at the location of each click."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "show_clicks",
              "type": "Param",
              "type_": "bool, default: True"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Mouse button used to add points."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "mouse_add",
              "type": "Param",
              "type_": "`.MouseButton` or None, default: `.MouseButton.LEFT`"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Mouse button used to remove the most recently added point."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "mouse_pop",
              "type": "Param",
              "type_": "`.MouseButton` or None, default: `.MouseButton.RIGHT`"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Mouse button used to stop input."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "mouse_stop",
              "type": "Param",
              "type_": "`.MouseButton` or None, default: `.MouseButton.MIDDLE`"
            }
          ],
          "type": "Parameters"
        }
      ],
