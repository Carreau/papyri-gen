      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "The following call       "
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "code",
          "value": "ax = subplot2grid((nrows, ncols), (row, col), rowspan, colspan)"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "is identical to       "
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "code",
          "value": "fig = gcf()\ngs = fig.add_gridspec(nrows, ncols)\nax = fig.add_subplot(gs[row:row+rowspan, col:col+colspan])"
        }
      ],
      "level": 0,
