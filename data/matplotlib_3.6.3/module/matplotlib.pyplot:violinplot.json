    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "Make a violin plot for each column of "
            },
            {
              "children": [
                {
                  "type": "text",
                  "value": "dataset"
                }
              ],
              "type": "emphasis"
            },
            {
              "type": "text",
              "value": " or each vector in sequence "
            },
            {
              "children": [
                {
                  "type": "text",
                  "value": "dataset"
                }
              ],
              "type": "emphasis"
            },
            {
              "type": "text",
              "value": ".  Each filled area extends to represent the entire data range, with optional lines at the mean, the median, the minimum, the maximum, and user-specified quantiles."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
