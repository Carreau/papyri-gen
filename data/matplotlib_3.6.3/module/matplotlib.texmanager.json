  "arbitrary": [
    {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "Support for embedded TeX expressions in Matplotlib."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "Requirements:"
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "LaTeX."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            },
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "\\*Agg backends: dvipng>=1.6."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            },
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "PS backend: PSfrag, dvips, and Ghostscript>=9.0."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            },
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "PDF and SVG backends: if LuaTeX is present, it will be used to speed up some   post-processing steps, but note that it is not used to parse the TeX string   itself (only LaTeX is supported)."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            }
          ],
          "ordered": false,
          "spread": false,
          "start": 1,
          "type": "list"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "To enable TeX rendering of all text in your Matplotlib figure, set "
            },
            {
              "type": "inlineCode",
              "value": "text.usetex"
            },
            {
              "type": "text",
              "value": " to True."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "TeX and dvipng/dvips processing results are cached in ~/.matplotlib/tex.cache for reuse between sessions."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "TexManager.get_rgba"
            },
            {
              "type": "text",
              "value": " can also be used to directly obtain raster output as RGBA NumPy arrays."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    }
