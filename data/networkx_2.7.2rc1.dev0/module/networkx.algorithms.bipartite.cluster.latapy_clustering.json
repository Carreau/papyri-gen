              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "c_{uv}"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " which can be:"
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "dot"
                },
                "type": "Directive"
              },
              {
                "data": {
