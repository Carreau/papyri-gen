            "inline": [
              {
                "data": {
                  "value": "The bipartie clustering coefficient is a measure of local density of connections defined as :"
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "c_u = \\frac { \\sum_ { v \\in N ( N ( u))} c_ { uv} } { |N ( N ( u))|} "
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "N(N(u))"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " are the second order neighbors of "
                },
                "type": "Words"
              },
              {
