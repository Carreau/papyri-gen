    "Extended Summary": {
      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "The weighted projected graph is the projection of the bipartite network B onto the specified nodes with weights representing the number of shared neighbors or the ratio between actual shared neighbors and possible shared neighbors if "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "ratio is True"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " . The nodes retain their attributes and are connected in the resulting graph if they have an edge to a common node in the original graph."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
    "Methods": {
      "children": [],
      "title": null
    },
    "Notes": {
      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "No attempt is made to verify that the input graph B is bipartite. The graph and node properties are (shallow) copied to the projected graph."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "See "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "bipartite documentation <networkx.algorithms.bipartite>"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " for further details on how bipartite graphs are handled in NetworkX."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
    "Other Parameters": {
      "children": [],
      "title": null
    },
