      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "Let "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "G=(V,E)"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " be a simple undirected graph with "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "n"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " nodes and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "m"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " edges, and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "A"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " denote the adjacency matrix of "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "G"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": "."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "Let "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "G(r)=(V,E(r))"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " be the graph resulting from removing all edges connected to node "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "r"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " but not the node itself."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "The adjacency matrix for "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "G(r)"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " is "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "A+E(r)"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": ",  where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "E(r)"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " has nonzeros only in row and column "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "r"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": "."
