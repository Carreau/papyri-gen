                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "The subraph betweenness of a node "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "r"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": "  is"
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "\\omega_ { r} = \\frac { 1} { C}\\sum_ { p}\\sum_ { q}\\frac { G_ { prq}} { G_ { pq}}, p\\neq q, q\\neq r, "
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "G_{prq}=(e^{A}_{pq} - (e^{A+E(r)})_{pq}"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": "  is the number of walks involving node r, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "G_{pq}=(e^{A})_{pq}"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " is the number of closed walks starting at node "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "module",
                  "reference": {
                    "kind": "module",
                    "module": "networkx",
                    "path": "networkx.generators.degree_seq.DegreeSequenceRandomGraph.p",
                    "version": "2.7.2rc1.dev0"
                  },
                  "value": "p"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " and ending at node "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "module",
                  "reference": {
                    "kind": "module",
                    "module": "networkx",
                    "path": "networkx.generators.degree_seq.DegreeSequenceRandomGraph.q",
                    "version": "2.7.2rc1.dev0"
                  },
                  "value": "q"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": ", and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "C=(n-1)^{2}-(n-1)"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " is a normalization factor equal to the number of terms in the sum."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "The resulting "
