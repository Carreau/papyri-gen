    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Communicability betweenness measure makes use of the number of walks connecting every pair of nodes as the basis of a betweenness centrality measure."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
    "Methods": {
      "children": [],
      "title": null
    },
    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Let "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "G=(V,E)"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " be a simple undirected graph with "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "n",
                    "version": null
                  },
                  "value": "n"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " nodes and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "m"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " edges, and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "A"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " denote the adjacency matrix of "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "G",
                    "version": null
                  },
                  "value": "G"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": "."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Let "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "G(r)=(V,E(r))"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " be the graph resulting from removing all edges connected to node "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "r",
                    "version": null
                  },
                  "value": "r"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " but not the node itself."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The adjacency matrix for "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "G(r)"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " is "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "A+E(r)"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": ",  where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "E(r)"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " has nonzeros only in row and column "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "r",
                    "version": null
                  },
                  "value": "r"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": "."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The subraph betweenness of a node "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "r",
                    "version": null
                  },
                  "value": "r"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": "  is"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "\\omega_{r} = \\frac{1}{C}\\sum_{p}\\sum_{q}\\frac{G_{prq}}{G_{pq}},\np\\neq q, q\\neq r,"
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "G_{prq}=(e^{A}_{pq} - (e^{A+E(r)})_{pq}"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": "  is the number of walks involving node r, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "G_{pq}=(e^{A})_{pq}"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " is the number of closed walks starting at node "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "p",
                    "version": null
                  },
                  "value": "p"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " and ending at node "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "module",
                  "reference": {
                    "kind": "module",
                    "module": "networkx",
                    "path": "networkx.generators.degree_seq.DegreeSequenceRandomGraph.q",
                    "version": "2.7.2rc1.dev0"
                  },
                  "value": "q"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": ", and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "C=(n-1)^{2}-(n-1)"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " is a normalization factor equal to the number of terms in the sum."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The resulting "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "\\omega_{r}"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " takes values between zero and one. The lower bound cannot be attained for a connected graph, and the upper bound is attained in the star graph."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
