          "data": {
            "children": [
              {
                "data": {
                  "value": "The Estrada Index is a topological index of folding or 3D \"compactness\" ()."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
    "Methods": {
      "children": [],
      "title": null
    },
    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Let "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "G=(V,E)"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " be a simple undirected graph with "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "n"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " nodes  and let "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "\\lambda_{1}\\leq\\lambda_{2}\\leq\\cdots\\lambda_{n}"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " be a non-increasing ordering of the eigenvalues of its adjacency matrix "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "A"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": ". The Estrada index is (, )"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "EE(G)=\\sum_{j=1}^n e^{\\lambda _j}."
          },
          "type": "BlockMath"
        }
      ],
      "title": null
    },
