    },
    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "For each node, the generalized degree shows how many edges of given triangle multiplicity the node is connected to. The triangle multiplicity of an edge is the number of triangles an edge participates in. The generalized degree of node "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "i"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " can be written as a vector "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\mathbf{k}_i=(k_i^{(0)}, \\dotsc, k_i^{(N-2)})"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "k_i^{(j)}"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is the number of edges attached to node "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "i"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " that participate in "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "j"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " triangles."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
    "Methods": {
      "children": [],
      "title": null
    },
    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "In a network of N nodes, the highest triangle multiplicty an edge can have is N-2."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The return value does not include a "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "module",
                  "reference": {
                    "kind": "module",
                    "module": "networkx",
                    "path": "networkx.utils.heaps.MinHeap.__nonzero__",
                    "version": "2.7.2rc1.dev0"
                  },
                  "value": "zero"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " entry if no edges of a particular triangle multiplicity are present."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The number of triangles node "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "i"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is attached to can be recovered from the generalized degree "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\mathbf{k}_i=(k_i^{(0)}, \\dotsc,\nk_i^{(N-2)})"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " by "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "(k_i^{(1)}+2k_i^{(2)}+\\dotsc +(N-2)k_i^{(N-2)})/2"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": "."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
