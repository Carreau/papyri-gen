    "Extended Summary": {
      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "For each node return the fraction of possible squares that exist at the node"
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "C_4 ( v) = \\frac { \\sum_ { u=1}^ { k_v} \\sum_ { w=u+1}^ { k_v} q_v ( u,w) } { \\sum_ { u=1}^ { k_v} \\sum_ { w=u+1}^ { k_v} [ a_v ( u,w) + q_v ( u,w)]}, "
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "q_v(u,w)"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " are the number of common neighbors of "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "u"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "w"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " other than "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "v"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " (ie squares), and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "a_v(u,w) = (k_u -\n(1+q_v(u,w)+\\theta_{uv})) + (k_w - (1+q_v(u,w)+\\theta_{uw}))"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ", where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "\\theta_{uw} = 1"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " if "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "u"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "w"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " are connected and 0 otherwise."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
