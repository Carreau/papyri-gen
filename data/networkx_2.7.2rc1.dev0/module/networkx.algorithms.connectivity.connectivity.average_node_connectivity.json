            "children": [
              {
                "data": {
                  "value": "The average connectivity "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "\\bar{\\kappa}"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " of a graph G is the average of local node connectivity over all pairs of nodes of G  ."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "\\bar{\\kappa}(G) = \\frac{\\sum_{u,v} \\kappa_{G}(u,v)}{{n \\choose 2}}"
          },
          "type": "BlockMath"
        }
      ],
      "title": null
