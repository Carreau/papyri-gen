                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Each k-edge-subgraph is a maximal set of nodes that defines a subgraph of G that is k-edge-connected."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "k_edge_subgraphs",
                "type_": "a generator of nx.Graphs that are k-edge-subgraphs"
              }
            ]
          },
