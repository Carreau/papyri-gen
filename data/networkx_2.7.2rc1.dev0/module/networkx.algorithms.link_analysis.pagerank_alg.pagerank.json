                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "If the algorithm fails to converge to the specified tolerance within the specified number of iterations of the power iteration method."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
