                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Node names of G and H can be changed by specifying the tuple rename=('G-','H-') (for example).  Node \"u\" in G is then renamed \"G-u\" and \"v\" in H is renamed \"H-v\"."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
