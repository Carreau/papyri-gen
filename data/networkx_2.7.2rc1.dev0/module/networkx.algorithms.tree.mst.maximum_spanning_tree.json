                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "An undirected graph. If "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "exists": true,
                            "kind": "local",
                            "reference": {
                              "kind": "local",
