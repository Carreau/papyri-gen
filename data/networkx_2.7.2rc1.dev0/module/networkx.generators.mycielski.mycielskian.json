    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The Mycielskian of graph preserves a graph's triangle free property while increasing the chromatic number by 1."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The Mycielski Operation on a graph, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "G=(V, E)"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ", constructs a new graph with "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "2|V| + 1"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " nodes and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "3|E| + |V|"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " edges."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The construction is as follows:"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Let "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "V = {0, ..., n-1}"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ". Construct another vertex set "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "U = {n, ..., 2n}"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " and a vertex, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "w"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": ". Construct a new graph, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "M",
                    "version": null
                  },
                  "value": "M"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": ", with vertices "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "U \\bigcup V \\bigcup w"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ". For edges, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "(u, v) \\in E"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " add edges "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "(u, v), (u, v + n)"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ", and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "(u + n, v)"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " to M. Finally, for all vertices "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "u \\in U"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ", add edge "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "(u, w)"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " to M."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The Mycielski Operation can be done multiple times by repeating the above process iteratively."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "More information can be found at https://en.wikipedia.org/wiki/Mycielskian"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
