                    {
                      "data": {
                        "value": "Represents the parameters for a shell, starting at the center shell.  Each element of the list must be of the form "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "domain": null,
                        "role": null,
                        "value": "(n, m,\nd)"
                      },
                      "type": "Directive"
                    },
                    {
                      "data": {
                        "value": ", where "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "domain": null,
                        "role": null,
                        "value": "n"
                      },
                      "type": "Directive"
                    },
                    {
                      "data": {
                        "value": " is the number of nodes in the shell, "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "domain": null,
                        "role": null,
                        "value": "m"
                      },
                      "type": "Directive"
                    },
                    {
                      "data": {
                        "value": " is the number of edges in the shell, and "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "domain": null,
                        "role": null,
                        "value": "d"
                      },
                      "type": "Directive"
                    },
                    {
                      "data": {
                        "value": " is the ratio of inter-shell (next) edges to intra-shell edges. If "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "domain": null,
                        "role": null,
                        "value": "d"
                      },
                      "type": "Directive"
                    },
                    {
                      "data": {
                        "value": " is zero, there will be no intra-shell edges, and if "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "domain": null,
                        "role": null,
                        "value": "d"
                      },
                      "type": "Directive"
                    },
                    {
                      "data": {
                        "value": " is one there will be all possible intra-shell edges."
                      },
                      "type": "Words"
                    }
