    "Extended Summary": {
      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "The graph directed combinatorial Laplacian is the matrix"
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "L = \\Phi - ( \\Phi P + P^T \\Phi) / 2 "
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "P"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " is the transition matrix of the graph and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "\\Phi"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " a matrix with the Perron vector of "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "P"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " in the diagonal and zeros elsewhere."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "Depending on the value of walk_type, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "P"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " can be the transition matrix induced by a random walk, a lazy random walk, or a random walk with teleportation (PageRank)."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
