    },
    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "For each node return the fraction of possible squares that exist at the node"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "C_4(v) = \\frac{ \\sum_{u=1}^{k_v}\n\\sum_{w=u+1}^{k_v} q_v(u,w) }{ \\sum_{u=1}^{k_v}\n\\sum_{w=u+1}^{k_v} [a_v(u,w) + q_v(u,w)]},"
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "q_v(u,w)"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " are the number of common neighbors of "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "u"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "w"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " other than "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "v"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " (ie squares), and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "a_v(u,w) = (k_u -\n(1+q_v(u,w)+\\theta_{uv})) + (k_w - (1+q_v(u,w)+\\theta_{uw}))"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ", where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\theta_{uw} = 1"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " if "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "u"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "w"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " are connected and 0 otherwise."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "level": 0,
      "title": null
    },
    "Methods": {
      "children": [],
      "level": 0,
      "title": null
    },
    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "While "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "C_3(v)"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " (triangle clustering) gives the probability that two neighbors of node v are connected with each other, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "C_4(v)"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is the probability that two neighbors of node v share a common neighbor different from v. This algorithm can be applied to both bipartite and unipartite networks."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "level": 0,
      "title": null
    },
