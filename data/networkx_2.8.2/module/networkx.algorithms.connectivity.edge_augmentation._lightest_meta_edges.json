    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Each node in the metagraph is a k-edge-connected component in the original graph.  We don't care about any edge within the same k-edge-connected component, so we ignore self edges.  We also are only intereseted in the minimum weight edge bridging each k-edge-connected component so, we group the edges by meta-edge and take the lightest in each group."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null
    },
