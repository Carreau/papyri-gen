    },
    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "An edge cover of a graph is a set of edges such that every node of the graph is incident to at least one edge of the set."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
