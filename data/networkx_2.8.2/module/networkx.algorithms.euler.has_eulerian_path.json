    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "An Eulerian path is a path in a graph which uses each edge of a graph exactly once. If "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "source"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " is specified, then this function checks whether an Eulerian path that starts at node "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "source"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " exists."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "dd": [
                  {
                    "data": {
                      "children": [
                        {
                          "children": [
                            {
                              "data": {
                                "children": [
                                  {
                                    "data": {
                                      "value": "at most one vertex has out_degree - in_degree = 1,"
                                    },
                                    "type": "Words"
                                  }
                                ]
                              },
                              "type": "Paragraph"
                            }
                          ]
                        },
                        {
                          "children": [
                            {
                              "data": {
                                "children": [
                                  {
                                    "data": {
                                      "value": "at most one vertex has in_degree - out_degree = 1,"
                                    },
                                    "type": "Words"
                                  }
                                ]
                              },
                              "type": "Paragraph"
                            }
                          ]
                        },
                        {
                          "children": [
                            {
                              "data": {
                                "children": [
                                  {
                                    "data": {
                                      "value": "every other vertex has equal in_degree and out_degree,"
                                    },
                                    "type": "Words"
                                  }
                                ]
                              },
                              "type": "Paragraph"
                            }
                          ]
                        },
                        {
                          "children": [
                            {
                              "data": {
                                "children": [
                                  {
                                    "data": {
                                      "value": "and all of its vertices belong to a single connected       component of the underlying undirected graph."
                                    },
                                    "type": "Words"
                                  }
                                ]
                              },
                              "type": "Paragraph"
                            }
                          ]
                        }
                      ]
                    },
                    "type": "BulletList"
                  }
                ],
                "dt": {
                  "children": [
                    {
                      "data": {
                        "value": "A directed graph has an Eulerian path iff:"
                      },
                      "type": "Words"
                    }
                  ]
                }
              }
            ]
          },
          "type": "DefList"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "If "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "source"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " is not None, an Eulerian path starting at "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "source"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " exists if no other node has out_degree - in_degree = 1. This is equivalent to either there exists an Eulerian circuit or "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "source"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " has out_degree - in_degree = 1 and the conditions above hold."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "dd": [
                  {
