                  }
                ],
                "dt": {
                  "children": [
                    {
                      "data": {
                        "value": "An undirected graph has an Eulerian path iff:"
                      },
                      "type": "Words"
                    }
                  ]
                }
              }
            ]
          },
          "type": "DefList"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "If "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
