          "data": {
            "children": [
              {
                "data": {
                  "value": "A matching is a subset of edges in which no node occurs more than once. A maximal matching cannot add more edges and still be a matching."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
