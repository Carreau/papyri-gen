    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Edge weight attributes must be numerical. Distances are calculated as sums of weighted edges traversed."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The list of predecessors contains more than one element only when there are more than one shortest paths to the key node."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
