    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "A double-edge swap removes two randomly chosen edges u-v and x-y and creates the new edges u-x and v-y:   "
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "u--v            u  v\n       becomes  |  |\nx--y            x  y"
          },
          "type": "BlockVerbatim"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "If either the edge u-x or v-y already exist no swap is performed and another attempt is made to find a suitable edge pair."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null
    },
