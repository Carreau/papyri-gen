                "type": "Link"
              },
              {
                "data": {
                  "value": "."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": ">>> import random\n>>> import math\n>>> n = 100\n>>> pos = {i: (random.gauss(0, 2), random.gauss(0, 2)) for i in range(n)}\n>>> p_dist = lambda dist: math.exp(-dist)\n>>> G = nx.soft_random_geometric_graph(n, 0.2, pos=pos, p_dist=p_dist)"
          },
          "type": "BlockVerbatim"
        }
