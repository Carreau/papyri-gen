          "data": {
            "children": [
              {
                "data": {
                  "value": "Robins and Alexander  defined bipartite clustering coefficient as four times the number of four cycles "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "C_4"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " divided by the number of three paths "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "L_3"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " in a bipartite graph:"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "CC_4 = \\frac{4 * C_4}{L_3}"
          },
          "type": "BlockMath"
        }
