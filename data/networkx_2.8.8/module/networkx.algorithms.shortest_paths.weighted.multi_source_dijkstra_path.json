    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Compute shortest path between any of the source nodes and all other reachable nodes for a weighted graph."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null
    },
    "Methods": {
      "children": [],
      "level": 0,
      "target": null,
      "title": null
    },
    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Edge weight attributes must be numerical. Distances are calculated as sums of weighted edges traversed."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The weight function can be used to hide edges by returning None. So "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "weight = lambda u, v, d: 1 if d['color']==\"red\" else None"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " will find the shortest red path."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null
    },
