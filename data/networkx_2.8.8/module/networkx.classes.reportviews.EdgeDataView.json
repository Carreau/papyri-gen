                "data": {
                  "anchor": null,
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "default",
                    "version": null
                  },
                  "value": "default"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " control what edge data is reported. The default "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "data is False"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " reports only node-tuples for each edge. If "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "data is True"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " the entire edge data dict is returned. Otherwise "
                },
                "type": "Words"
              },
              {
                "data": {
                  "anchor": null,
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
