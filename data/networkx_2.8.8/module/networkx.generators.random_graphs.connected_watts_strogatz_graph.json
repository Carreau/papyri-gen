          "data": {
            "children": [
              {
                "data": {
                  "value": "Attempts to generate a connected graph by repeated generation of Watts\u2013Strogatz small-world graphs.  An exception is raised if the maximum number of tries is exceeded."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null
    },
    "Methods": {
