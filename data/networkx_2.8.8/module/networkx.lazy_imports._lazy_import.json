                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Proxy module. Can be used like any regularly imported module. Actual loading of the module occurs upon first attribute request."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
