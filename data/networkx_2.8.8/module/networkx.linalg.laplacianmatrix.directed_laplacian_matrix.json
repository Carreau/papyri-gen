    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The graph directed Laplacian is the matrix"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "L = I - (\\Phi^{1/2} P \\Phi^{-1/2} + \\Phi^{-1/2} P^T \\Phi^{1/2} ) / 2"
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "I"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " is the identity matrix, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "P"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " is the transition matrix of the graph, and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "\\Phi"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " a matrix with the Perron vector of "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "P"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " in the diagonal and zeros elsewhere ."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Depending on the value of walk_type, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "P"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " can be the transition matrix induced by a random walk, a lazy random walk, or a random walk with teleportation (PageRank)."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null
    },
    "Methods": {
      "children": [],
      "level": 0,
      "target": null,
      "title": null
    },
