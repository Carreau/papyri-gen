            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "If None, every node has weight 1. If a string, use this node attribute as the node weight. A node without this attribute is assumed to have weight 1."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "weight",
              "type": "Param",
              "type_": "string, optional (default = None)"
            }
          ],
