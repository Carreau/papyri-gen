      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "The smallest edge cover can be found in polynomial time by finding a maximum matching and extending it greedily so that all nodes are covered."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
