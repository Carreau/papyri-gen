    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "Group in-degree centrality of a group of nodes $S$ is the fraction of non-group members connected to group members by incoming edges."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
    "Methods": {
      "children": [],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "The number of nodes in the group must be a maximum of n - 1 where "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "n"
            },
            {
              "type": "text",
              "value": " is the total number of nodes in the graph."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "G.neighbors(i)"
            },
            {
              "type": "text",
              "value": " gives nodes with an outward edge from i, in a DiGraph, so for group in-degree centrality, the reverse graph is used."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
