    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "The trophic difference $x_ij$ for each edge is defined in Johnson et al.  as:"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "x_ij = s_j - s_i"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "Where $s_i$ is the trophic level of node $i$."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
