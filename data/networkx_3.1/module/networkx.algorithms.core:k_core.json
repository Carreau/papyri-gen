      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "A k-core is a maximal subgraph that contains nodes of degree k or more."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
