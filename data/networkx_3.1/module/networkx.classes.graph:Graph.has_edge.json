    "children": [
      {
        "type": "code",
        "value": "G = nx.path_graph(4)  # or DiGraph, MultiGraph, MultiDiGraph, etc\nG.has_edge(0, 1)  # using two nodes"
      },
      {
        "type": "code",
        "value": "e = (0, 1)\nG.has_edge(*e)  #  e is a 2-tuple (u, v)"
      },
      {
        "type": "code",
        "value": "e = (0, 1, {\"weight\": 7})\nG.has_edge(*e[:2])  # e is a 3-tuple (u, v, data_dictionary)"
      },
      {
        "type": "text",
        "value": "The following syntax are equivalent:"
      },
      {
        "type": "code",
        "value": "G.has_edge(0, 1)"
      },
