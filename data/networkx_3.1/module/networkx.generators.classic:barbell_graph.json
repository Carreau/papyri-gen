    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "Two identical complete graphs $K_{m1}$ form the left and right bells, and are connected by a path $P_{m2}$."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "dd": [
                {
                  "children": [
                    {
                      "domain": null,
                      "role": null,
                      "type": "Directive",
                      "value": "0, ..., m1-1"
                    },
                    {
                      "type": "text",
                      "value": " for the left barbell,     "
                    },
                    {
                      "domain": null,
                      "role": null,
                      "type": "Directive",
                      "value": "m1, ..., m1+m2-1"
                    },
                    {
                      "type": "text",
                      "value": " for the path,     and "
                    },
                    {
                      "domain": null,
                      "role": null,
                      "type": "Directive",
                      "value": "m1+m2, ..., 2*m1+m2-1"
                    },
                    {
                      "type": "text",
                      "value": " for the right barbell."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "dt": {
                "children": [
                  {
                    "type": "text",
                    "value": "The "
                  },
                  {
                    "domain": null,
                    "role": null,
                    "type": "Directive",
                    "value": "2*m1+m2"
                  },
                  {
                    "type": "text",
                    "value": "  nodes are numbered"
                  }
                ],
                "type": "paragraph"
              },
              "type": "DefListItem"
            }
          ],
          "type": "DefList"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "The 3 subgraphs are joined via the edges "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "(m1-1, m1)"
            },
            {
              "type": "text",
              "value": " and "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "(m1+m2-1, m1+m2)"
            },
            {
              "type": "text",
              "value": ". If "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "m2=0"
            },
            {
              "type": "text",
              "value": ", this is merely two complete graphs joined together."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "This graph is an extremal example in David Aldous and Jim Fill's e-text on Random Walks on Graphs."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
