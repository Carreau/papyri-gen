          "children": [
            {
              "type": "text",
              "value": "The Dorogovtsev-Goltsev-Mendes  procedure produces a scale-free graph deterministically with the following properties for a given "
            },
            {
              "anchor": null,
              "exists": true,
              "kind": "local",
              "reference": {
                "kind": "local",
                "module": null,
                "path": "n",
                "type": "RefInfo",
                "version": null
              },
              "type": "Link",
              "value": "n"
            },
            {
              "type": "text",
              "value": ": - Total number of nodes = "
            },
            {
              "type": "inlineCode",
              "value": "3 * (3**n + 1) / 2"
            },
            {
              "type": "text",
              "value": " - Total number of edges = "
            },
            {
              "type": "inlineCode",
              "value": "3 ** (n + 1)"
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
