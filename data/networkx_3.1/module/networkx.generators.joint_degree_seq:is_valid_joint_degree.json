    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "A "
            },
            {
              "children": [
                {
                  "type": "text",
                  "value": "joint degree dictionary"
                }
              ],
              "type": "emphasis"
            },
            {
              "type": "text",
              "value": " is a dictionary of dictionaries, in which entry "
            },
            {
              "type": "inlineCode",
              "value": "joint_degrees[k][l]"
            },
            {
              "type": "text",
              "value": " is an integer representing the number of edges joining nodes of degree "
            },
            {
              "children": [
                {
                  "type": "text",
                  "value": "k"
                }
              ],
              "type": "emphasis"
            },
            {
              "type": "text",
              "value": " with nodes of degree "
            },
            {
              "children": [
                {
                  "type": "text",
                  "value": "l"
                }
              ],
              "type": "emphasis"
            },
            {
              "type": "text",
              "value": ". Such a dictionary is realizable as a simple graph if and only if the following conditions are satisfied."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "each entry must be an integer,"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            },
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "the total number of nodes of degree "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "k"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": ", computed by   "
                    },
                    {
                      "type": "inlineCode",
                      "value": "sum(joint_degrees[k].values()) / k"
                    },
                    {
                      "type": "text",
                      "value": ", must be an integer,"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            },
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "the total number of edges joining nodes of degree "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "k"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": " with   nodes of degree "
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "l"
                        }
                      ],
                      "type": "emphasis"
                    },
                    {
                      "type": "text",
                      "value": " cannot exceed the total number of possible edges,"
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            },
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "each diagonal entry "
                    },
                    {
                      "type": "inlineCode",
                      "value": "joint_degrees[k][k]"
                    },
                    {
                      "type": "text",
                      "value": " must be even (this is   a convention assumed by the "
                    },
                    {
                      "type": "inlineCode",
                      "value": "joint_degree_graph"
                    },
                    {
                      "type": "text",
                      "value": " function)."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            }
          ],
