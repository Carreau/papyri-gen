              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Represents the parameters for a shell, starting at the center shell.  Each element of the list must be of the form "
                    },
                    {
                      "domain": null,
                      "role": null,
                      "type": "Directive",
                      "value": "(n, m,\nd)"
                    },
                    {
                      "type": "text",
                      "value": ", where "
                    },
                    {
                      "domain": null,
                      "role": null,
                      "type": "Directive",
                      "value": "n"
                    },
                    {
                      "type": "text",
                      "value": " is the number of nodes in the shell, "
                    },
                    {
                      "domain": null,
                      "role": null,
                      "type": "Directive",
                      "value": "m"
                    },
                    {
                      "type": "text",
                      "value": " is the number of edges in the shell, and "
                    },
                    {
                      "domain": null,
                      "role": null,
                      "type": "Directive",
                      "value": "d"
                    },
                    {
                      "type": "text",
                      "value": " is the ratio of inter-shell (next) edges to intra-shell edges. If "
                    },
                    {
                      "domain": null,
                      "role": null,
                      "type": "Directive",
                      "value": "d"
                    },
                    {
                      "type": "text",
                      "value": " is zero, there will be no intra-shell edges, and if "
                    },
                    {
                      "domain": null,
                      "role": null,
                      "type": "Directive",
                      "value": "d"
                    },
                    {
                      "type": "text",
                      "value": " is one there will be all possible intra-shell edges."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "constructor",
              "type": "Param",
              "type_": "list of three-tuples"
            },
            {
