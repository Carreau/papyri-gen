    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "The normalized graph Laplacian is the matrix"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "N = D^{-1/2} L D^{-1/2}"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "where "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "L"
            },
            {
              "type": "text",
              "value": " is the graph Laplacian and "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "D"
            },
            {
              "type": "text",
              "value": " is the diagonal matrix of node degrees ."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
    "Methods": {
      "children": [],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "For MultiGraph, the edges weights are summed. See "
            },
            {
              "type": "inlineCode",
              "value": "to_numpy_array"
            },
            {
              "type": "text",
              "value": " for other options."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "If the Graph contains selfloops, D is defined as "
            },
            {
              "type": "inlineCode",
              "value": "diag(sum(A, 1))"
            },
            {
              "type": "text",
              "value": ", where A is the adjacency matrix ."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
