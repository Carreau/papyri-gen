          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "Perform an indirect partition along the given axis using the algorithm specified by the "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "kind"
                  ],
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " keyword. It returns an array of indices of the same shape as "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "a"
                  ],
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " that index data along the given axis in partitioned order."
                }
              }
            ]
          }
        }
      ]
    },
