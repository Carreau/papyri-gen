          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "Packs the elements of a binary-valued array into bits in a uint8 array."
                }
              }
            ]
          }
        }
      ]
    },
    "Extended Summary": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "The result is padded to full bytes by inserting zero bits at the end."
                }
              }
            ]
          }
        }
      ]
    },
    "Parameters": {
      "children": [
        {
          "type": "Param",
          "data": {
            "param": "a",
            "type_": "array_like",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "children": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "An array of integers or booleans whose elements should be packed to bits."
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "type": "Param",
          "data": {
            "param": "axis",
            "type_": "int, optional",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "children": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "The dimension over which bit-packing is done. "
                      }
                    },
                    {
                      "type": "Verbatim",
                      "data": {
                        "value": [
                          "None"
                        ]
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": " implies packing the flattened array."
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "type": "Param",
          "data": {
            "param": "bitorder",
            "type_": "{'big', 'little'}, optional",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "children": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "The order of the input bits. 'big' will mimic bin(val), "
                      }
                    },
                    {
                      "type": "Verbatim",
                      "data": {
                        "value": [
                          "[0,",
                          " ",
                          "0,",
                          " ",
                          "0,",
                          " ",
                          "0,",
                          " ",
                          "0,",
                          " ",
                          "0,",
                          " ",
                          "1,",
                          " ",
                          "1]",
                          " ",
                          "=>",
                          " ",
                          "3",
                          " ",
                          "=",
                          " ",
                          "0b00000011"
                        ]
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": ", 'little' will reverse the order so "
                      }
                    },
                    {
                      "type": "Verbatim",
                      "data": {
                        "value": [
                          "[1,",
                          " ",
                          "1,",
                          " ",
                          "0,",
                          " ",
                          "0,",
                          " ",
                          "0,",
                          " ",
                          "0,",
                          " ",
                          "0,",
                          " ",
                          "0]",
                          " ",
                          "=>",
                          " ",
                          "3"
                        ]
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": ". Defaults to 'big'."
                      }
                    }
                  ]
                }
              },
