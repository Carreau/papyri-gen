      "title": null
    },
    "Extended Summary": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "Using an instance of "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "errstate"
                  ],
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " as a context manager allows statements in that context to execute with a known error handling behavior. Upon entering the context the error handling is set with "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "seterr"
                  ],
                  "domain": null,
                  "role": null
