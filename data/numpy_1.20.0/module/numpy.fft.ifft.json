            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "This function computes the inverse of the one-dimensional *n*-point discrete Fourier transform computed by "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "fft"
                  ],
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ".  In other words, "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "ifft(fft(a))",
                    " ",
                    "==",
                    " ",
                    "a"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " to within numerical accuracy. For a general description of the algorithm and definitions, see "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "numpy.fft"
                  ],
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": "."
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "The input should be ordered in the same way as is returned by "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "fft"
                  ],
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ", i.e.,"
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "* "
                }
              },
              {
                "type": "Verbatim",
                "data": {
