          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "A dictionary-like object with lazy-loading of files in the zipped archive provided on construction."
                }
              }
            ]
          }
        }
      ]
    },
    "Extended Summary": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "NpzFile"
                  ],
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " is used to load files in the NumPy "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    ".npz"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " data archive format. It assumes that files in the archive have a "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    ".npy"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " extension, other files are ignored."
                }
              }
            ]
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "The arrays and file strings are lazily loaded on either getitem access using "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "obj['key']"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " or attribute lookup using "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "obj.f.key"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ". A list of all files (without "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    ".npy"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " extensions) can be obtained with "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "obj.files"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " and the ZipFile object itself using "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "obj.zip"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": "."
                }
              }
            ]
          }
        }
