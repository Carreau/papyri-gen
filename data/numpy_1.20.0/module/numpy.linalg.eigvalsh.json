            "type_": "(..., M,) ndarray",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "children": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "The eigenvalues in ascending order, each repeated according to its multiplicity."
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      ]
