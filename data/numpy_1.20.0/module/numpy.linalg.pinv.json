    "Notes": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "The pseudo-inverse of a matrix A, denoted "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "A^+"
                  ],
                  "domain": null,
                  "role": "math"
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ", is defined as: \"the matrix that 'solves' [the least-squares problem] "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "Ax",
                    " ",
                    "=",
                    " ",
                    "b"
                  ],
                  "domain": null,
                  "role": "math"
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ",\" i.e., if "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "\\bar{x}"
                  ],
                  "domain": null,
                  "role": "math"
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " is said solution, then "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "A^+"
                  ],
                  "domain": null,
                  "role": "math"
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " is that matrix such that "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "\\bar{x}",
                    " ",
                    "=",
                    " ",
                    "A^+b"
                  ],
                  "domain": null,
                  "role": "math"
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": "."
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "It can be shown that if "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "Q",
                    "_",
                    "1",
                    " ",
                    "\\Sigma",
                    " ",
                    "Q",
                    "_",
                    "2^T",
                    " ",
                    "=",
                    " ",
                    "A"
                  ],
                  "domain": null,
                  "role": "math"
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " is the singular value decomposition of A, then "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "A^+",
                    " ",
                    "=",
                    " ",
                    "Q",
                    "_",
                    "2",
                    " ",
                    "\\Sigma^+",
                    " ",
                    "Q",
                    "_",
                    "1^T"
                  ],
                  "domain": null,
                  "role": "math"
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ", where "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "Q",
                    "_",
                    "{1,2}"
                  ],
                  "domain": null,
                  "role": "math"
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " are orthogonal matrices, "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "\\Sigma"
                  ],
                  "domain": null,
                  "role": "math"
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " is a diagonal matrix consisting of A's so-called singular values, (followed, typically, by zeros), and then "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "\\Sigma^+"
                  ],
                  "domain": null,
                  "role": "math"
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " is simply the diagonal matrix consisting of the reciprocals of A's singular values (again, followed by zeros). [1]_"
                }
              }
            ],
            "inner": []
          }
        }
      ],
      "title": null
    },
