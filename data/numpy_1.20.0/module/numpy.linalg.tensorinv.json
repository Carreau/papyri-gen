                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": "'s tensordot inverse, shape "
                      }
                    },
                    {
                      "type": "Verbatim",
                      "data": {
                        "value": [
                          "a.shape[ind",
                          ":",
                          "]",
                          " ",
