{
  "_content": {
    "Summary": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "Compute the 'inverse' of an N-dimensional array."
                }
              }
            ]
          }
        }
      ]
    },
    "Extended Summary": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "The result is an inverse for "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "a"
                  ],
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " relative to the tensordot operation "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "tensordot(a,",
                    " ",
                    "b,",
                    " ",
                    "ind)"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ", i. e., up to floating-point accuracy, "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "tensordot(tensorinv(a),",
                    " ",
                    "a,",
                    " ",
                    "ind)"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " is the \"identity\" tensor for the tensordot operation."
                }
              }
            ]
          }
        }
      ]
    },
    "Parameters": {
      "children": [
        {
          "type": "Param",
          "data": {
            "param": "a",
            "type_": "array_like",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "children": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "Tensor to 'invert'. Its shape must be 'square', i. e., "
                      }
                    },
                    {
                      "type": "Verbatim",
                      "data": {
                        "value": [
                          "prod(a.shape[",
                          ":",
                          "ind])",
                          " ",
                          "==",
                          " ",
                          "prod(a.shape[ind",
                          ":",
                          "])"
                        ]
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": "."
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "type": "Param",
          "data": {
            "param": "ind",
            "type_": "int, optional",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "children": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "Number of first indices that are involved in the inverse sum. Must be a positive integer, default is 2."
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      ]
    },
    "Returns": {
