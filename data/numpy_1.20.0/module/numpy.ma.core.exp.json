                    " ",
                    "\\log",
                    "_",
                    "e",
                    " ",
                    "y"
                  ],
                  "domain": null,
                  "role": "math"
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ", then "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "e^x",
                    " ",
                    "=",
                    " ",
                    "y"
                  ],
                  "domain": null,
                  "role": "math"
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ". For real input, "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "exp(x)"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " is always positive."
                }
              }
            ]
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "For complex arguments, "
