            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "The columns of the output matrix are powers of the input vector. The order of the powers is determined by the "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "increasing"
                  ],
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " boolean argument. Specifically, when "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "increasing"
                  ],
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " is False, the "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "i"
                  ],
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": "-th output column is the input vector raised element-wise to the power of "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "N",
                    " ",
                    "-",
                    " ",
                    "i",
                    " ",
                    "-",
                    " ",
                    "1"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ". Such a matrix with a geometric progression in each row is named for Alexandre- Theophile Vandermonde."
                }
              }
            ]
          }
        }
      ]
    },
