              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "roots"
                  ],
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ". If a zero has multiplicity n, then it must appear in "
