        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "in Laguerre form, where the "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "r",
                    "_",
                    "n"
                  ],
                  "domain": null,
