            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "SeedSequence mixes sources of entropy in a reproducible way to set the initial state for independent and very probably non-overlapping BitGenerators."
                }
              }
            ],
            "inner": []
          }
        }
      ],
      "title": null
