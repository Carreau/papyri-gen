                    },
                    {
                      "type": "Verbatim",
                      "data": {
                        "value": [
                          "None"
                        ]
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": " (the default).  If "
                      }
                    },
                    {
                      "type": "Directive",
                      "data": {
                        "value": [
                          "seed"
                        ],
                        "domain": null,
                        "role": null
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": " is "
                      }
                    },
                    {
                      "type": "Verbatim",
                      "data": {
                        "value": [
                          "None"
                        ]
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": ", then the "
                      }
                    },
                    {
                      "type": "Directive",
                      "data": {
                        "value": [
                          "MT19937"
                        ],
                        "domain": null,
                        "role": null
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": " BitGenerator is initialized by reading data from "
                      }
                    },
                    {
                      "type": "Verbatim",
                      "data": {
                        "value": [
                          "/dev/urandom"
                        ]
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": " (or the Windows analogue) if available or seed from the clock otherwise."
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      ]
