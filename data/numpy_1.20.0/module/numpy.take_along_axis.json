        },
        {
          "type": "BlockVerbatim",
          "data": {
            "lines": {
              "_lines": [
                {
                  "_line": "    Ni, M, Nk = a.shape[:axis], a.shape[axis], a.shape[axis+1:]",
                  "_number": 3,
                  "_offset": 4
                },
                {
                  "_line": "    J = indices.shape[axis]  # Need not equal M",
                  "_number": 4,
                  "_offset": 4
                },
                {
                  "_line": "    out = np.empty(Ni + (J,) + Nk)",
                  "_number": 5,
                  "_offset": 4
                },
                {
                  "_line": "",
                  "_number": 6,
                  "_offset": 4
                },
                {
                  "_line": "    for ii in ndindex(Ni):",
                  "_number": 7,
                  "_offset": 4
                },
                {
                  "_line": "        for kk in ndindex(Nk):",
                  "_number": 8,
                  "_offset": 4
                },
                {
                  "_line": "            a_1d       = a      [ii + s_[:,] + kk]",
                  "_number": 9,
                  "_offset": 4
                },
                {
                  "_line": "            indices_1d = indices[ii + s_[:,] + kk]",
                  "_number": 10,
                  "_offset": 4
                },
                {
                  "_line": "            out_1d     = out    [ii + s_[:,] + kk]",
                  "_number": 11,
                  "_offset": 4
                },
                {
                  "_line": "            for j in range(J):",
                  "_number": 12,
                  "_offset": 4
                },
                {
                  "_line": "                out_1d[j] = a_1d[indices_1d[j]]",
                  "_number": 13,
                  "_offset": 4
                },
                {
                  "_line": "",
                  "_number": 14,
                  "_offset": 4
                }
              ]
            }
          }
        },
