                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " should be replaced by "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "NPY_ARRAY_INOUT_FARRAY2"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ". Arrays created with these new flags will then have the "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "WRITEBACKIFCOPY"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " semantics."
                },
                "type": "Words"
              }
            ]
