    "Extended Summary": {
      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "This function computes the correlation as generally defined in signal processing texts::      "
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "c_{av}[k] = sum_n a[n+k] * conj(v[n])"
          },
          "type": "BlockVerbatim"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "with a and v sequences being zero-padded where necessary and conj being the conjugate."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
    "Methods": {
      "children": [],
      "title": null
    },
    "Notes": {
      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "The definition of correlation above is not unique and sometimes correlation may be defined differently. Another common definition is::      "
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "c'_{av}[k] = sum_n a[n] conj(v[n+k])"
          },
          "type": "BlockVerbatim"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "which is related to "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "c_{av}[k]"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " by "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "c'_{av}[k] = c_{av}[-k]"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": "."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "exists": true,
                  "kind": "module",
                  "reference": {
                    "kind": "module",
                    "module": "numpy",
