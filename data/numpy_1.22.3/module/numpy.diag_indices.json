        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "This returns a tuple of indices that can be used to access the main diagonal of an array "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "a"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " with "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "a.ndim >= 2"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " dimensions and shape (n, n, ..., n). For "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "a.ndim = 2"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " this is the usual diagonal, for "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "a.ndim > 2"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " this is the set of indices to access "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "a[i, i, ..., i]"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " for "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "i = [0..n-1]"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": "."
                },
                "type": "Words"
              }
            ]
          },
