    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The first difference is given by "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "out[i] = a[i+1] - a[i]"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " along the given axis, higher differences are calculated by using "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "module",
                  "reference": {
                    "kind": "module",
                    "module": "numpy",
                    "path": "numpy.diff",
                    "version": "1.22.3"
                  },
                  "value": "diff"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " recursively."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
    "Methods": {
      "children": [],
      "title": null
    },
    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Type is preserved for boolean arrays, so the result will contain "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "False"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " when consecutive elements are the same and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "True"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " when they differ."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "For unsigned integer arrays, the results will also be unsigned. This should not be surprising, as the result is consistent with calculating the difference directly:"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": ">>> u8_arr = np.array([1, 0], dtype=np.uint8)\n>>> np.diff(u8_arr)\narray([255], dtype=uint8)\n>>> u8_arr[1,...] - u8_arr[0,...]\n255"
          },
          "type": "BlockVerbatim"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "If this is not desirable, then the array should be cast to a larger integer type first:"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": ">>> i16_arr = u8_arr.astype(np.int16)\n>>> np.diff(i16_arr)\narray([-1], dtype=int16)"
          },
          "type": "BlockVerbatim"
        }
      ],
      "title": null
    },
