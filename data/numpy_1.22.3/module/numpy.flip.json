    "Notes": {
      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "flip(m, 0) is equivalent to flipud(m)."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "flip(m, 1) is equivalent to fliplr(m)."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "flip(m, n) corresponds to "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "m[...,::-1,...]"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " with "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "::-1"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " at position n."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "inline": [
              {
