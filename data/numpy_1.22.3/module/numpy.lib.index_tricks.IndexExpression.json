    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "inline": [
                  {
                    "data": {
                      "value": "Use one of the two predefined instances `index_exp` or `s_` rather than directly using `IndexExpression`. "
                    },
                    "type": "Words"
                  }
                ],
                "inner": []
              }
            ],
            "kind": "note",
            "title": ""
          },
          "type": "Admonition"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "For any index combination, including slicing and axis insertion, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "a[indices]"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " is the same as "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "a[np.index_exp[indices]]"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " for any array "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "a"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": ". However, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "np.index_exp[indices]"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " can be used anywhere in Python code and returns a tuple of slice objects that can be used in the construction of complex index expressions."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
    "Methods": {
      "children": [],
      "title": null
    },
    "Notes": {
      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "You can do all this with "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "slice()"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " plus a few special objects, but there's a lot to remember and this version is simpler because it uses the standard array indexing syntax."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
