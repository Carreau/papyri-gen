                      "type": "Link"
                    },
                    {
                      "data": {
                        "value": "): "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "value": [
                          "converters = {3: lambda s: float(s.strip() or 0)}"
                        ]
                      },
                      "type": "Verbatim"
                    },
                    {
                      "data": {
                        "value": ". Default: None."
                      },
                      "type": "Words"
