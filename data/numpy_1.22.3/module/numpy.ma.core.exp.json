    "Notes": {
      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "The irrational number "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "e"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " is also known as Euler's number.  It is approximately 2.718281, and is the base of the natural logarithm, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "ln"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " (this means that, if "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "x = \\ln y = \\log_e y"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ", then "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "e^x = y"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ". For real input, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "exp(x)"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " is always positive."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "For complex arguments, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "x = a + ib"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ", we can write "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "e^x = e^a e^{ib}"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ".  The first term, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "e^a"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ", is already known (it is the real argument, described above).  The second term, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "e^{ib}"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ", is "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "\\cos b + i \\sin b"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ", a function with magnitude 1 and a periodic phase."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
