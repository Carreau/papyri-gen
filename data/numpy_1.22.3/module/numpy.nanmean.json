    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Returns the average of the array elements.  The average is taken over the flattened array by default, otherwise over the specified axis. "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "module",
                  "reference": {
                    "kind": "module",
                    "module": "numpy",
                    "path": "numpy.float64",
                    "version": "1.22.3"
                  },
                  "value": "float64"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " intermediate and return values are used for integer inputs."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "For all-NaN slices, NaN is returned and a "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "RuntimeWarning"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " is raised."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [],
            "kind": "versionadded",
            "title": "1.8.0"
          },
          "type": "Admonition"
        }
      ],
      "title": null
    },
    "Methods": {
      "children": [],
      "title": null
    },
    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The arithmetic mean is the sum of the non-NaN elements along the axis divided by the number of non-NaN elements."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Note that for floating-point input, the mean is computed using the same precision the input has.  Depending on the input data, this can cause the results to be inaccurate, especially for "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "module",
                  "reference": {
                    "kind": "module",
                    "module": "numpy",
                    "path": "numpy.float32",
                    "version": "1.22.3"
                  },
                  "value": "float32"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": ".  Specifying a higher-precision accumulator using the "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "module",
                  "reference": {
                    "kind": "module",
                    "module": "numpy",
                    "path": "numpy.dtype",
                    "version": "1.22.3"
                  },
                  "value": "dtype"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " keyword can alleviate this issue."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
