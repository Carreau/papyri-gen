                        },
                        {
                          "data": {
                            "value": [
                              "[0, 0, 0, 0, 0, 0, 1, 1] => 3 = 0b00000011"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": ", 'little' will reverse the order so "
                          },
