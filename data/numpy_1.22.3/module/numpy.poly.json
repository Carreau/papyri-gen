            ]
          },
          "type": "BlockQuote"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "content": {
                    "value": "I"
                  }
                },
                "type": "Strong"
              },
              {
                "data": {
                  "value": " is the "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "n"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": "-by-"
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
