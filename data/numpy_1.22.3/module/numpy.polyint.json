    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "inline": [
                  {
                    "data": {
                      "value": "This forms part of the old polynomial API. Since version 1.4, the new polynomial API defined in `numpy.polynomial` is preferred. A summary of the differences can be found in the : doc : `transition guide < / reference / routines.polynomials>`. "
                    },
                    "type": "Words"
                  }
                ],
                "inner": []
              }
            ],
            "kind": "note",
            "title": ""
          },
          "type": "Admonition"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "The returned order "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "m",
                    "version": null
                  },
                  "value": "m"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " antiderivative "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "P"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " of polynomial "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "p",
                    "version": null
                  },
                  "value": "p"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " satisfies "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "\\frac{d^m}{dx^m}P(x) = p(x)"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " and is defined up to "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "m - 1"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " integration constants "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "k",
                    "version": null
                  },
                  "value": "k"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": ". The constants determine the low-order polynomial part"
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "\\frac { k_ { m - 1}} { 0!} x^0 + \\ldots + \\frac { k_0} { ( m - 1)!}x^ { m - 1} "
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "of "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "P"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " so that "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "P^{(j)}(0) = k_{m-j-1}"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": "."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        }
      ],
