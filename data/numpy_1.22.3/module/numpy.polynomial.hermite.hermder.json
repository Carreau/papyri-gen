    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Returns the Hermite series coefficients "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "c",
                    "version": null
                  },
                  "value": "c"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " differentiated "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "m",
                    "version": null
                  },
                  "value": "m"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " times along "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "axis"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": ".  At each iteration the result is multiplied by "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "scl"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " (the scaling factor is for use in a linear change of variable). The argument "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "c",
                    "version": null
                  },
                  "value": "c"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " is an array of coefficients from low to high degree along each axis, e.g., [1,2,3] represents the series "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "1*H_0 + 2*H_1 + 3*H_2"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " while [[1,2],[1,2]] represents "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "1*H_0(x)*H_0(y) + 1*H_1(x)*H_0(y) +\n2*H_0(x)*H_1(y) + 2*H_1(x)*H_1(y)"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " if axis=0 is "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "x"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " and axis=1 is "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "y"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": "."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
    "Methods": {
      "children": [],
      "title": null
    },
    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "In general, the result of differentiating a Hermite series does not resemble the same operation on a power series. Thus the result of this function may be \"unintuitive,\" albeit correct; see Examples section below."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
    "Other Parameters": {
      "children": [],
      "title": null
    },
    "Parameters": {
      "children": [
        {
          "data": {
            "children": [
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Array of Hermite series coefficients. If "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "exists": true,
                            "kind": "local",
                            "reference": {
                              "kind": "local",
