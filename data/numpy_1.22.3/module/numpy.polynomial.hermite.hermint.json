    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Returns the Hermite series coefficients "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "c",
                    "version": null
                  },
                  "value": "c"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " integrated "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "m",
                    "version": null
                  },
                  "value": "m"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " times from "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "lbnd"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " along "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "axis"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": ". At each iteration the resulting series is "
                },
                "type": "Words"
              },
              {
                "data": {
                  "content": {
                    "value": "multiplied"
                  }
                },
                "type": "Strong"
              },
              {
                "data": {
                  "value": " by "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "scl"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " and an integration constant, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "k",
                    "version": null
                  },
                  "value": "k"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": ", is added. The scaling factor is for use in a linear change of variable.  (\"Buyer beware\": note that, depending on what one is doing, one may want "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "scl"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " to be the reciprocal of what one might expect; for more information, see the Notes section below.)  The argument "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "c",
                    "version": null
                  },
                  "value": "c"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " is an array of coefficients from low to high degree along each axis, e.g., [1,2,3] represents the series "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "H_0 + 2*H_1 + 3*H_2"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " while [[1,2],[1,2]] represents "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "1*H_0(x)*H_0(y) + 1*H_1(x)*H_0(y) + 2*H_0(x)*H_1(y) +\n2*H_1(x)*H_1(y)"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " if axis=0 is "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "x"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " and axis=1 is "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "y"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": "."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
    "Methods": {
      "children": [],
      "title": null
    },
    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Note that the result of each integration is "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": {
                    "value": "multiplied"
                  }
                },
                "type": "Emph"
              },
              {
                "data": {
                  "value": " by "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "scl"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": ". Why is this important to note?  Say one is making a linear change of variable "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "u = ax + b"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " in an integral relative to "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "x",
                    "version": null
                  },
                  "value": "x"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": ".  Then "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "dx = du/a"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ", so one will need to set "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "scl"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " equal to "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "1/a"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " - perhaps not what one would have first thought."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Also note that, in general, the result of integrating a C-series needs to be \"reprojected\" onto the C-series basis set.  Thus, typically, the result of this function is \"unintuitive,\" albeit correct; see Examples section below."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
    "Other Parameters": {
      "children": [],
      "title": null
    },
    "Parameters": {
      "children": [
        {
          "data": {
            "children": [
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Array of Hermite series coefficients. If c is multidimensional the different axis correspond to different variables with the degree in each axis given by the corresponding index."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "c",
                "type_": "array_like"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Order of integration, must be positive. (Default: 1)"
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "m",
                "type_": "int, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Integration constant(s).  The value of the first integral at "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "lbnd"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": " is the first value in the list, the value of the second integral at "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "lbnd"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": " is the second value, etc.  If "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "k == []"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": " (the default), all constants are set to zero.  If "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "m == 1"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": ", a single scalar can be given instead of a list."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "k",
                "type_": "{[], list, scalar}, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "The lower bound of the integral. (Default: 0)"
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "lbnd",
                "type_": "scalar, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Following each integration the result is "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": {
                              "value": "multiplied"
                            }
                          },
                          "type": "Emph"
                        },
                        {
                          "data": {
                            "value": " by "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "domain": null,
                            "role": null,
                            "value": "scl"
                          },
                          "type": "Directive"
                        },
                        {
                          "data": {
                            "value": " before the integration constant is added. (Default: 1)"
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "scl",
                "type_": "scalar, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Axis over which the integral is taken. (Default: 0)."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  },
                  {
                    "data": {
