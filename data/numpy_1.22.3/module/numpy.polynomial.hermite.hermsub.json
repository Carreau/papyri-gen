            "children": [
              {
                "data": {
                  "value": "Returns the difference of two Hermite series "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "c1"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " - "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "c2"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": ".  The sequences of coefficients are from lowest order term to highest, i.e., [1,2,3] represents the series "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "P_0 + 2*P_1 + 3*P_2"
