    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Returns the pseudo-Vandermonde matrix of degrees "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "deg"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " and sample points "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "(x, y)"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": ". The pseudo-Vandermonde matrix is defined by"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "V[..., (deg[1] + 1)*i + j] = L_i(x) * L_j(y),"
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "0 <= i <= deg[0]"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "0 <= j <= deg[1]"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": ". The leading indices of "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "V"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " index the points "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "(x, y)"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " and the last index encodes the degrees of the Legendre polynomials."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "If "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "V = legvander2d(x, y, [xdeg, ydeg])"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ", then the columns of "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "V"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " correspond to the elements of a 2-D coefficient array "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "c"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " of shape (xdeg + 1, ydeg + 1) in the order"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "c_{00}, c_{01}, c_{02} ... , c_{10}, c_{11}, c_{12} ..."
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "np.dot(V, c.flat)"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "legval2d(x, y, c)"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " will be the same up to roundoff. This equivalence is useful both for least squares fitting and for the evaluation of a large number of 2-D Legendre series of the same degrees and sample points."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
