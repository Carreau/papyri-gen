                      "children": [
                        {
                          "data": {
                            "value": "The shape of the returned matrix is "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "x.shape + (order,)"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": ", where "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": "order = (deg[0]+1)*(deg[1]+1)"
                          },
                          "type": "Math"
                        },
                        {
                          "data": {
                            "value": ".  The dtype will be the same as the converted "
                          },
