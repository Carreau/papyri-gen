        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "Returns the pseudo-Vandermonde matrix of degrees "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "deg",
                    "version": null
                  },
                  "value": "deg"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " and sample points "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "(x, y, z)"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": ". If "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "l, m, n"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " are the given degrees in "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "x, y, z"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": ", then The pseudo-Vandermonde matrix is defined by"
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "V [ ..., ( m+1) ( n+1)i + ( n+1)j + k] = L_i ( x)*L_j ( y)*L_k ( z), "
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "0 <= i <= l"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": ", "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "0 <= j <= m"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": ", and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "0 <= j <= n"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": ".  The leading indices of "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "V"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " index the points "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "(x, y, z)"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " and the last index encodes the degrees of the Legendre polynomials."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "If "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "V = legvander3d(x, y, z, [xdeg, ydeg, zdeg])"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ", then the columns of "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "V"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " correspond to the elements of a 3-D coefficient array "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "c"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " of shape (xdeg + 1, ydeg + 1, zdeg + 1) in the order"
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "c_ { 000}, c_ { 001}, c_ { 002},... , c_ { 010}, c_ { 011}, c_ { 012},... "
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "np.dot(V, c.flat)"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "legval3d(x, y, z, c)"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " will be the same up to roundoff. This equivalence is useful both for least squares fitting and for the evaluation of a large number of 3-D Legendre series of the same degrees and sample points."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        }
      ],
