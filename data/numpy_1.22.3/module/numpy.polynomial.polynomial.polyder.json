              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "scl"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " (the scaling factor is for use in a linear change of variable).  The argument "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
