    "Extended Summary": {
      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "Return the coefficients of the polynomial"
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "p ( x) = ( x - r_0) * ( x - r_1) * ... * ( x - r_n), "
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "where the "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "r_n"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " are the roots specified in "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "roots",
                    "version": null
                  },
                  "value": "roots"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": ".  If a zero has multiplicity n, then it must appear in "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "roots",
                    "version": null
                  },
                  "value": "roots"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " n times. For instance, if 2 is a root of multiplicity three and 3 is a root of multiplicity 2, then "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "roots",
                    "version": null
                  },
                  "value": "roots"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " looks something like [2, 2, 2, 3, 3]. The roots can appear in any order."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "If the returned coefficients are "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "c"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": ", then"
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "p ( x) = c_0 + c_1 * x + ... + x^n "
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "The coefficient of the last term is 1 for monic polynomials in this form."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        }
