    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Returns the polynomial coefficients "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "c",
                    "version": null
                  },
                  "value": "c"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " integrated "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "m",
                    "version": null
                  },
                  "value": "m"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " times from "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "lbnd"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " along "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "axis"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": ".  At each iteration the resulting series is "
                },
                "type": "Words"
              },
              {
                "data": {
                  "content": {
                    "value": "multiplied"
                  }
                },
                "type": "Strong"
              },
              {
                "data": {
                  "value": " by "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "scl"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " and an integration constant, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "k",
                    "version": null
                  },
                  "value": "k"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": ", is added. The scaling factor is for use in a linear change of variable.  (\"Buyer beware\": note that, depending on what one is doing, one may want "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "scl"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " to be the reciprocal of what one might expect; for more information, see the Notes section below.) The argument "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "c",
                    "version": null
                  },
                  "value": "c"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " is an array of coefficients, from low to high degree along each axis, e.g., [1,2,3] represents the polynomial "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "1 + 2*x + 3*x**2"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " while [[1,2],[1,2]] represents "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "1 + 1*x + 2*y + 2*x*y"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " if axis=0 is "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "x"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " and axis=1 is "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "y"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": "."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
    "Methods": {
      "children": [],
      "title": null
    },
    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Note that the result of each integration is "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": {
                    "value": "multiplied"
                  }
                },
                "type": "Emph"
              },
              {
                "data": {
                  "value": " by "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "scl"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": ".  Why is this important to note?  Say one is making a linear change of variable "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "u = ax + b"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " in an integral relative to "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "x",
                    "version": null
                  },
                  "value": "x"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": ". Then "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "dx = du/a"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ", so one will need to set "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "scl"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " equal to "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "1/a"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " - perhaps not what one would have first thought."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
