      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "This function is useful to be sure that an array with the correct flags is returned for passing to compiled code (perhaps through ctypes)."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
    "Methods": {
