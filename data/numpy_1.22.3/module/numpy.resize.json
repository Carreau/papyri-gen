            "children": [
              {
                "data": {
                  "value": "If the new array is larger than the original array, then the new array is filled with repeated copies of "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "a",
