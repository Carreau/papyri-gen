                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "p",
                    "version": null
                  },
                  "value": "p"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " is n+1 then the polynomial is described by::    "
                },
                "type": "Words"
              }
