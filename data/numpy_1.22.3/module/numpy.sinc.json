    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The sinc function is "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\sin(\\pi x)/(\\pi x)"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": "."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "children": [
                    {
                      "data": {
                        "value": "Note the normalization factor of "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "value": [
                          "pi"
                        ]
                      },
                      "type": "Verbatim"
                    },
                    {
                      "data": {
                        "value": " used in the definition. This is the most commonly used definition in signal processing. Use "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "value": [
                          "sinc(x / np.pi)"
                        ]
                      },
                      "type": "Verbatim"
                    },
                    {
                      "data": {
                        "value": " to obtain the unnormalized sinc function "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "domain": null,
                        "role": "math",
                        "value": "\\sin(x)/(x)"
                      },
                      "type": "Directive"
                    },
                    {
                      "data": {
                        "value": " that is more common in mathematics."
                      },
                      "type": "Words"
                    }
                  ]
                },
                "type": "Paragraph"
              }
            ],
            "kind": "note",
            "title": ""
          },
          "type": "Admonition"
        }
      ],
      "title": null
    },
    "Methods": {
      "children": [],
      "title": null
    },
    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": [
                    "sinc(0)"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " is the limit value 1."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The name sinc is short for \"sine cardinal\" or \"sinus cardinalis\"."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The sinc function is used in various signal processing applications, including in anti-aliasing, in the construction of a Lanczos resampling filter, and in interpolation."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "For bandlimited interpolation of discrete-time signals, the ideal interpolation kernel is proportional to the sinc function."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
