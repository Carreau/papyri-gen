    "Extended Summary": {
      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "Warnings can be slippery, because, whenever a warning is triggered, Python adds a "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "__warningregistry__"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " member to the "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": {
                    "value": "calling"
                  }
                },
                "type": "Emph"
              },
              {
                "data": {
                  "value": " module.  This makes it impossible to retrigger the warning in this module, whatever you put in the warnings filters.  This context manager accepts a sequence of "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "modules",
                    "version": null
                  },
                  "value": "modules"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " as a keyword argument to its constructor and:"
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "value": [
                  {
                    "data": {
                      "inline": [
                        {
                          "data": {
                            "value": "stores and removes any "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "__warningregistry__"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": " entries in given "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "exists": true,
                            "kind": "local",
                            "reference": {
                              "kind": "local",
                              "module": null,
                              "path": "modules",
                              "version": null
                            },
                            "value": "modules"
                          },
                          "type": "Link"
                        },
                        {
                          "data": {
                            "value": "   on entry;"
                          },
                          "type": "Words"
                        }
                      ],
                      "inner": []
                    },
                    "type": "Paragraph"
                  }
                ]
              },
              {
                "value": [
                  {
                    "data": {
                      "inline": [
                        {
                          "data": {
                            "value": "resets "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "__warningregistry__"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": " to its previous state on exit."
                          },
                          "type": "Words"
                        }
                      ],
                      "inner": []
                    },
                    "type": "Paragraph"
                  }
                ]
              }
            ]
          },
          "type": "BulletList"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "This makes it possible to trigger any warning afresh inside the context manager without disturbing the state of warnings outside."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "For compatibility with Python 3.0, please consider all arguments to be keyword-only."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
