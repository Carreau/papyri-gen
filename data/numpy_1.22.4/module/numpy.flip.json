    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "flip(m, 0) is equivalent to flipud(m)."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "flip(m, 1) is equivalent to fliplr(m)."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "flip(m, n) corresponds to "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "m[...,::-1,...]"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " with "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "::-1"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " at position n."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "flip(m) corresponds to "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "m[::-1,::-1,...,::-1]"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " with "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "::-1"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " at all positions."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "flip(m, (0, 1)) corresponds to "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "m[::-1,::-1,...]"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " with "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "::-1"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " at position 0 and position 1."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "level": 0,
      "title": null
    },
