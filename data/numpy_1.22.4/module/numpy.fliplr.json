            "children": [
              {
                "data": {
                  "value": "Equivalent to "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "m[:,::-1]"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " or "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "np.flip(m, axis=1)"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ". Requires the array to be at least 2-D."
