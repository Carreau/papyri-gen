          "data": {
            "children": [
              {
                "data": {
                  "value": "For a 2-D array, this flips the entries in each row in the left/right direction. Columns are preserved, but appear in a different order than before."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null
