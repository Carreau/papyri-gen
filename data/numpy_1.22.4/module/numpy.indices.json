        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The output shape in the dense case is obtained by prepending the number of dimensions in front of the tuple of dimensions, i.e. if "
                },
                "type": "Words"
              },
              {
                "data": {
                  "anchor": null,
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "dimensions",
                    "version": null
                  },
                  "value": "dimensions"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " is a tuple "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "(r0, ..., rN-1)"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " of length "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "N"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ", the output shape is "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "(N, r0, ..., rN-1)"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": "."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The subarrays "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "grid[k]"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " contains the N-D array of indices along the "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "k-th"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " axis. Explicitly:      "
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "grid[k, i0, i1, ..., iN-1] = ik"
