          "data": {
            "children": [
              {
                "data": {
                  "value": "Returns the sum of two Chebyshev series "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "c1"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " + "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "c2"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": ".  The arguments are sequences of coefficients ordered from lowest order term to highest, i.e., [1,2,3] represents the series "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "T_0 + 2*T_1 + 3*T_2"
                  ]
                },
                "type": "Verbatim"
