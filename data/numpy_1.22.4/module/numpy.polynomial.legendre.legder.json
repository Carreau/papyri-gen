    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Returns the Legendre series coefficients "
                },
                "type": "Words"
              },
              {
                "data": {
                  "anchor": null,
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "c",
                    "version": null
                  },
                  "value": "c"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " differentiated "
                },
                "type": "Words"
              },
              {
                "data": {
                  "anchor": null,
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "m",
                    "version": null
                  },
                  "value": "m"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " times along "
                },
                "type": "Words"
              },
              {
                "data": {
                  "anchor": null,
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "axis",
                    "version": null
                  },
                  "value": "axis"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": ".  At each iteration the result is multiplied by "
                },
                "type": "Words"
              },
              {
                "data": {
                  "anchor": null,
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "scl",
                    "version": null
                  },
                  "value": "scl"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " (the scaling factor is for use in a linear change of variable). The argument "
                },
                "type": "Words"
              },
              {
                "data": {
                  "anchor": null,
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "c",
                    "version": null
                  },
                  "value": "c"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " is an array of coefficients from low to high degree along each axis, e.g., [1,2,3] represents the series "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "1*L_0 + 2*L_1 + 3*L_2"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " while [[1,2],[1,2]] represents "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "1*L_0(x)*L_0(y) + 1*L_1(x)*L_0(y) +\n2*L_0(x)*L_1(y) + 2*L_1(x)*L_1(y)"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " if axis=0 is "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "x"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " and axis=1 is "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "y"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": "."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null
    },
