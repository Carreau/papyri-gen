          "data": {
            "children": [
              {
                "data": {
                  "value": "Return the roots (a.k.a. \"zeros\") of the polynomial"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "p(x) = \\sum_i c[i] * x^i."
          },
          "type": "BlockMath"
        }
      ],
      "level": 0,
      "target": null,
      "title": null
    },
