    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "Uses the cygpath utility (part of the Base install) to do the actual conversion.  Falls back to returning the original path if this fails."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "Handles the default "
            },
            {
              "type": "inlineCode",
              "value": "/cygdrive"
            },
            {
              "type": "text",
              "value": " mount prefix as well as the "
            },
            {
              "type": "inlineCode",
              "value": "/proc/cygdrive"
            },
            {
              "type": "text",
              "value": " portable prefix, custom cygdrive prefixes such as "
            },
            {
              "type": "inlineCode",
              "value": "/"
            },
            {
              "type": "text",
              "value": " or "
            },
            {
              "type": "inlineCode",
              "value": "/mnt"
            },
            {
              "type": "text",
              "value": ", and absolute paths such as "
            },
            {
              "type": "inlineCode",
              "value": "/usr/src/"
            },
            {
              "type": "text",
              "value": " or "
            },
            {
              "type": "inlineCode",
              "value": "/home/username"
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
