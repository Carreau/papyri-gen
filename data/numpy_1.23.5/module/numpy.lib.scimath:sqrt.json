  "example_section_data": {
    "children": [
      {
        "type": "text",
        "value": "For real, non-negative inputs this works just like `numpy.sqrt`:"
      },
      {
        "type": "code",
        "value": "np.emath.sqrt(1)"
      },
      {
        "type": "code",
        "value": "np.emath.sqrt([1, 4])"
      },
      {
        "type": "text",
        "value": "But it automatically handles negative inputs:"
      },
      {
        "type": "code",
        "value": "np.emath.sqrt(-1)"
      },
      {
        "type": "code",
        "value": "np.emath.sqrt([-1,4])"
      },
      {
        "type": "text",
        "value": "Different results are expected because:\nfloating point 0.0 and -0.0 are distinct."
      },
      {
        "type": "text",
        "value": "For more control, explicitly use complex() as follows:"
      },
      {
        "type": "code",
        "value": "np.emath.sqrt(complex(-4.0, 0.0))"
      },
      {
        "type": "code",
        "value": "np.emath.sqrt(complex(-4.0, -0.0))"
      }
    ],
