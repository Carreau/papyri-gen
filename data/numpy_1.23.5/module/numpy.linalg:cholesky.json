            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "A"
            },
            {
              "type": "text",
              "value": " is both Hermitian/symmetric and positive-definite)."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "First, we solve for "
            },
            {
              "type": "inlineMath",
              "value": "\\mathbf{y}"
            },
            {
              "type": "text",
              "value": " in"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "L \\mathbf{y} = \\mathbf{b},"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "and then for "
            },
            {
              "type": "inlineMath",
              "value": "\\mathbf{x}"
            },
            {
              "type": "text",
              "value": " in"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "L.H \\mathbf{x} = \\mathbf{y}."
        }
