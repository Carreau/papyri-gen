    "children": [
      {
        "type": "code",
        "value": "import numpy.ma as ma\na = np.arange(5)\na"
      },
      {
        "type": "code",
        "value": "a = ma.masked_where(a < 3, a)\na"
      },
      {
        "type": "code",
        "value": "ma.set_fill_value(a, -999)\na"
      },
      {
        "type": "text",
        "value": "Nothing happens if `a` is not a masked array."
      },
      {
        "type": "code",
        "value": "a = list(range(5))\na"
      },
      {
        "type": "code",
        "value": "ma.set_fill_value(a, 100)\na"
      },
