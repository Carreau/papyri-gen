      {
        "type": "text",
        "value": "Print sine of one angle:"
      },
      {
        "type": "code",
        "value": "np.sin(np.pi/2.)"
      },
      {
        "type": "text",
        "value": "Print sines of an array of angles given in degrees:"
      },
      {
        "type": "code",
        "value": "np.sin(np.array((0., 30., 45., 60., 90.)) * np.pi / 180. )"
      },
      {
        "type": "text",
        "value": "Plot the sine function:"
      },
      {
        "type": "code",
        "value": "import matplotlib.pylab as plt\nx = np.linspace(-np.pi, np.pi, 201)\nplt.plot(x, np.sin(x))\nplt.xlabel('Angle [rad]')\nplt.ylabel('sin(x)')\nplt.axis('tight')\nplt.show()"
      }
