    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "Returns the difference of two Chebyshev series "
            },
            {
              "anchor": null,
              "exists": true,
              "kind": "local",
              "reference": {
                "kind": "local",
                "module": null,
                "path": "c1",
                "type": "RefInfo",
                "version": null
              },
              "type": "Link",
              "value": "c1"
            },
            {
              "type": "text",
              "value": " - "
            },
            {
              "anchor": null,
              "exists": true,
              "kind": "local",
              "reference": {
                "kind": "local",
                "module": null,
                "path": "c2",
                "type": "RefInfo",
                "version": null
              },
              "type": "Link",
              "value": "c2"
            },
            {
              "type": "text",
              "value": ".  The sequences of coefficients are from lowest order term to highest, i.e., [1,2,3] represents the series "
            },
            {
              "type": "inlineCode",
              "value": "T_0 + 2*T_1 + 3*T_2"
            },
            {
              "type": "text",
              "value": "."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
