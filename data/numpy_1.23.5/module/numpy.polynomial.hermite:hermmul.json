    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "Returns the product of two Hermite series "
            },
            {
              "anchor": null,
              "exists": true,
              "kind": "local",
              "reference": {
                "kind": "local",
                "module": null,
                "path": "c1",
                "type": "RefInfo",
                "version": null
              },
              "type": "Link",
              "value": "c1"
            },
            {
              "type": "text",
              "value": " * "
            },
            {
              "anchor": null,
              "exists": true,
              "kind": "local",
              "reference": {
                "kind": "local",
                "module": null,
                "path": "c2",
                "type": "RefInfo",
                "version": null
              },
              "type": "Link",
              "value": "c2"
            },
            {
              "type": "text",
              "value": ".  The arguments are sequences of coefficients, from lowest order \"term\" to highest, e.g., [1,2,3] represents the series "
            },
            {
              "type": "inlineCode",
              "value": "P_0 + 2*P_1 + 3*P_2"
            },
            {
              "type": "text",
              "value": "."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
    "Methods": {
      "children": [],
      "level": 0,
