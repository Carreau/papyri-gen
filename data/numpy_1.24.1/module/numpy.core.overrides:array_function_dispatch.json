              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Function that when called like "
                    },
                    {
                      "type": "inlineCode",
                      "value": "dispatcher(*args, **kwargs)"
                    },
                    {
                      "type": "text",
                      "value": " with arguments from the NumPy function call returns an iterable of array-like arguments to check for "
                    },
                    {
                      "type": "inlineCode",
                      "value": "__array_function__"
                    },
                    {
                      "type": "text",
                      "value": "."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "dispatcher",
              "type": "Param",
              "type_": "callable"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "__module__ attribute to set on new function, e.g., "
                    },
                    {
                      "type": "inlineCode",
                      "value": "module='numpy'"
                    },
                    {
                      "type": "text",
                      "value": ". By default, module is copied from the decorated function."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "module",
              "type": "Param",
              "type_": "str, optional"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "If True, verify the that the signature of the dispatcher and decorated function signatures match exactly: all required and optional arguments should appear in order with the same names, but the default values for all optional arguments should be "
                    },
                    {
                      "type": "inlineCode",
                      "value": "None"
                    },
                    {
                      "type": "text",
                      "value": ". Only disable verification if the dispatcher's signature needs to deviate for some particular reason, e.g., because the function has a signature like "
                    },
                    {
                      "type": "inlineCode",
                      "value": "func(*args, **kwargs)"
                    },
                    {
                      "type": "text",
                      "value": "."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "verify",
              "type": "Param",
              "type_": "bool, optional"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "If True, copy docs from the dispatcher function onto the dispatched function, rather than from the implementation. This is useful for functions defined in C, which otherwise don't have docstrings."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "docs_from_dispatcher",
              "type": "Param",
              "type_": "bool, optional"
            }
