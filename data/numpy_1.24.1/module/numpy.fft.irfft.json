      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Returns the real valued "
                },
                "type": "Words"
              },
              {
                "data": {
                  "anchor": null,
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
