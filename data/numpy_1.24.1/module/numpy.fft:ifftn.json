      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "See "
            },
            {
              "anchor": null,
              "exists": true,
              "kind": "module",
              "reference": {
                "kind": "module",
                "module": "numpy",
                "path": "numpy.fft",
