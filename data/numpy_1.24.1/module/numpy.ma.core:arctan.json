    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "The inverse of tan, so that if "
            },
            {
              "type": "inlineCode",
              "value": "y = tan(x)"
            },
            {
              "type": "text",
              "value": " then "
            },
            {
              "type": "inlineCode",
              "value": "x = arctan(y)"
            },
            {
              "type": "text",
              "value": "."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
    "Methods": {
