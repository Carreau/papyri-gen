  "example_section_data": {
    "children": [
      {
        "type": "text",
        "value": "    >>> x = np.array([[1,2,3],[4,5,6]], order='F')"
      },
      {
        "type": "text",
        "value": "    >>> y = x.copy()"
      },
      {
        "type": "text",
        "value": "    >>> x.fill(0)"
      },
      {
        "type": "text",
        "value": "    >>> x\n    array([[0, 0, 0],\n           [0, 0, 0]])"
      },
      {
        "type": "text",
        "value": "    >>> y\n    array([[1, 2, 3],\n           [4, 5, 6]])"
      },
      {
        "type": "text",
        "value": "    >>> y.flags['C_CONTIGUOUS']\n    True"
      }
    ],
