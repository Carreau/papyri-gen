      "children": [
        {
          "children": [
            {
              "children": [
                {
                  "type": "text",
                  "value": "This function is the preferred method for creating an array copy.  The     function "
                },
                {
                  "type": "inlineCode",
                  "value": "numpy.copy"
                },
                {
                  "type": "text",
                  "value": " is similar, but it defaults to using order 'K',     and will not pass sub-classes through by default."
                }
              ],
              "type": "paragraph"
            }
          ],
          "type": "blockquote"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
