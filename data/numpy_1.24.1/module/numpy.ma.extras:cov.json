    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "Except for the handling of missing data this function does the same as "
            },
            {
              "anchor": null,
              "exists": true,
              "kind": "module",
              "reference": {
                "kind": "api",
                "module": "numpy",
                "path": "numpy:cov",
                "type": "RefInfo",
                "version": "*"
              },
              "type": "Link",
              "value": "numpy.cov"
            },
            {
              "type": "text",
              "value": ". For more details and examples, see "
            },
            {
              "anchor": null,
              "exists": true,
              "kind": "module",
              "reference": {
                "kind": "api",
                "module": "numpy",
                "path": "numpy:cov",
                "type": "RefInfo",
                "version": "*"
              },
              "type": "Link",
              "value": "numpy.cov"
            },
            {
              "type": "text",
              "value": "."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "By default, masked values are recognized as such. If "
            },
            {
              "anchor": null,
              "exists": true,
              "kind": "local",
              "reference": {
                "kind": "local",
                "module": null,
                "path": "x",
                "type": "RefInfo",
                "version": null
              },
              "type": "Link",
              "value": "x"
            },
            {
              "type": "text",
              "value": " and "
            },
            {
              "anchor": null,
              "exists": true,
              "kind": "local",
              "reference": {
                "kind": "local",
                "module": null,
                "path": "y",
                "type": "RefInfo",
                "version": null
              },
              "type": "Link",
              "value": "y"
            },
            {
              "type": "text",
              "value": " have the same shape, a common mask is allocated: if "
            },
            {
              "type": "inlineCode",
              "value": "x[i,j]"
            },
            {
              "type": "text",
              "value": " is masked, then "
            },
            {
              "type": "inlineCode",
              "value": "y[i,j]"
            },
            {
              "type": "text",
              "value": " will also be masked. Setting "
            },
            {
              "anchor": null,
              "exists": true,
              "kind": "local",
              "reference": {
                "kind": "local",
                "module": null,
                "path": "allow_masked",
                "type": "RefInfo",
                "version": null
              },
              "type": "Link",
              "value": "allow_masked"
            },
            {
              "type": "text",
              "value": " to False will raise an exception if values are missing in either of the input arrays."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
