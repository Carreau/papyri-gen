      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "Given a vector "
            },
            {
              "type": "inlineCode",
              "value": "V"
            },
            {
              "type": "text",
              "value": " with "
            },
            {
              "type": "inlineCode",
              "value": "N"
            },
            {
              "type": "text",
              "value": " non masked values, the median of "
            },
            {
              "type": "inlineCode",
              "value": "V"
            },
            {
              "type": "text",
              "value": " is the middle value of a sorted copy of "
            },
            {
              "type": "inlineCode",
              "value": "V"
            },
            {
              "type": "text",
              "value": " ("
            },
            {
              "type": "inlineCode",
              "value": "Vs"
            },
            {
              "type": "text",
              "value": ") - i.e. "
            },
            {
              "type": "inlineCode",
              "value": "Vs[(N-1)/2]"
            },
            {
              "type": "text",
              "value": ", when "
            },
            {
              "type": "inlineCode",
              "value": "N"
            },
            {
              "type": "text",
              "value": " is odd, or "
            },
            {
              "type": "inlineCode",
              "value": "{Vs[N/2 - 1] + Vs[N/2]}/2"
            },
            {
              "type": "text",
              "value": " when "
            },
            {
              "type": "inlineCode",
              "value": "N"
            },
            {
              "type": "text",
              "value": " is even."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
