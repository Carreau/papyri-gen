    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Returns the quotient-with-remainder of two Chebyshev series "
                },
                "type": "Words"
              },
              {
                "data": {
                  "anchor": null,
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "c1",
                    "version": null
                  },
                  "value": "c1"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " / "
                },
                "type": "Words"
              },
              {
                "data": {
                  "anchor": null,
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "c2",
                    "version": null
                  },
                  "value": "c2"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": ".  The arguments are sequences of coefficients from lowest order \"term\" to highest, e.g., [1,2,3] represents the series "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "T_0 + 2*T_1 + 3*T_2"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
