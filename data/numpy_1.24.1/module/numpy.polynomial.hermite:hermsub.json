          "children": [
            {
              "type": "text",
              "value": "Returns the difference of two Hermite series "
            },
            {
              "anchor": null,
              "exists": true,
              "kind": "local",
              "reference": {
                "kind": "local",
                "module": null,
                "path": "c1",
                "type": "RefInfo",
                "version": null
              },
              "type": "Link",
