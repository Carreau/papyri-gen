  "example_section_data": {
    "children": [
      {
        "type": "text",
        "value": "Convert a list into an array:"
      },
      {
        "type": "code",
        "value": "a = [1, 2]\nnp.asanyarray(a)"
      },
      {
        "type": "text",
        "value": "Instances of `ndarray` subclasses are passed through as-is:"
      },
      {
        "type": "code",
        "value": "a = np.array([(1.0, 2), (3.0, 4)], dtype='f4,i4').view(np.recarray)\nnp.asanyarray(a) is a"
      }
    ],
