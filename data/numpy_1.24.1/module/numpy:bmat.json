      {
        "type": "code",
        "value": "np.bmat([[A, B], [C, D]])"
      },
      {
        "type": "code",
        "value": "np.bmat(np.r_[np.c_[A, B], np.c_[C, D]])"
      },
      {
        "type": "code",
