                      "children": [
                        {
                          "type": "text",
                          "value": "deprecated 1.13.0"
                        }
                      ],
                      "type": "admonitionTitle"
                    },
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "Passing an axis where "
                        },
                        {
                          "type": "inlineCode",
                          "value": "axis > a.ndim"
                        },
                        {
                          "type": "text",
                          "value": " will be treated as "
                        },
                        {
                          "type": "inlineCode",
                          "value": "axis == a.ndim"
                        },
                        {
                          "type": "text",
                          "value": ", and passing "
                        },
                        {
                          "type": "inlineCode",
                          "value": "axis < -a.ndim - 1"
                        },
                        {
                          "type": "text",
                          "value": " will be treated as "
                        },
                        {
                          "type": "inlineCode",
                          "value": "axis == 0"
                        },
                        {
                          "type": "text",
                          "value": ". This behavior is deprecated."
                        }
