    },
    "Methods": {
      "children": [],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "flip(m, 0) is equivalent to flipud(m)."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "flip(m, 1) is equivalent to fliplr(m)."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "flip(m, n) corresponds to "
            },
            {
              "type": "inlineCode",
              "value": "m[...,::-1,...]"
            },
            {
              "type": "text",
              "value": " with "
            },
            {
              "type": "inlineCode",
              "value": "::-1"
            },
            {
              "type": "text",
              "value": " at position n."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "flip(m) corresponds to "
            },
            {
              "type": "inlineCode",
              "value": "m[::-1,::-1,...,::-1]"
            },
            {
              "type": "text",
              "value": " with "
            },
            {
              "type": "inlineCode",
              "value": "::-1"
            },
            {
              "type": "text",
              "value": " at all positions."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "flip(m, (0, 1)) corresponds to "
            },
            {
              "type": "inlineCode",
              "value": "m[::-1,::-1,...]"
            },
            {
              "type": "text",
              "value": " with "
            },
            {
              "type": "inlineCode",
              "value": "::-1"
            },
            {
              "type": "text",
              "value": " at position 0 and position 1."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
    "Other Parameters": {
      "children": [],
      "level": 0,
      "target": null,
