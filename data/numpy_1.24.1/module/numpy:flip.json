      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "The shape of the array is preserved, but the elements are reordered."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "children": [
                {
                  "type": "text",
                  "value": "versionadded 1.12.0"
                }
              ],
              "type": "admonitionTitle"
            }
          ],
