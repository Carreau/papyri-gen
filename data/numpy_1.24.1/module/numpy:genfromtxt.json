      },
      {
        "type": "code",
        "value": "s = StringIO(u\"1,1.3,abcde\")\ndata = np.genfromtxt(s, dtype=[('myint','i8'),('myfloat','f8'),\n('mystring','S5')], delimiter=\",\")\ndata"
