  "example_section_data": {
    "children": [
      {
        "type": "text",
        "value": "`issubdtype` can be used to check the type of arrays:"
      },
      {
        "type": "code",
        "value": "ints = np.array([1, 2, 3], dtype=np.int32)\nnp.issubdtype(ints.dtype, np.integer)"
      },
      {
        "type": "code",
        "value": "np.issubdtype(ints.dtype, np.floating)"
      },
      {
        "type": "code",
        "value": "floats = np.array([1, 2, 3], dtype=np.float32)\nnp.issubdtype(floats.dtype, np.integer)"
      },
      {
        "type": "code",
        "value": "np.issubdtype(floats.dtype, np.floating)"
      },
      {
        "type": "text",
        "value": "Similar types of different sizes are not subdtypes of each other:"
      },
      {
        "type": "code",
        "value": "np.issubdtype(np.float64, np.float32)"
      },
      {
        "type": "code",
        "value": "np.issubdtype(np.float32, np.float64)"
      },
      {
        "type": "text",
        "value": "but both are subtypes of `floating`:"
      },
      {
        "type": "code",
        "value": "np.issubdtype(np.float64, np.floating)"
      },
      {
        "type": "code",
        "value": "np.issubdtype(np.float32, np.floating)"
      },
      {
        "type": "text",
        "value": "For convenience, dtype-like objects are allowed too:"
      },
      {
        "type": "code",
        "value": "np.issubdtype('S1', np.string_)"
      },
      {
        "type": "code",
        "value": "np.issubdtype('i4', np.signedinteger)"
      }
    ],
    "level": 0,
    "target": null,
    "title": null,
    "type": "Section"
  },
