    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "Logspace is equivalent to the code"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "code",
          "value": ">>> y = np.linspace(start, stop, num=num, endpoint=endpoint)\n... # doctest: +SKIP\n>>> power(base, y).astype(dtype)\n... # doctest: +SKIP"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
