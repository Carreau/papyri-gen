    "Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "Return the cumulative product of array elements over a given axis treating Not a Numbers (NaNs) as one.  The cumulative product does not change when NaNs are encountered and leading NaNs are replaced by ones."
            }
          ],
          "type": "paragraph"
        }
