    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "Both "
            },
            {
              "anchor": null,
              "exists": true,
              "kind": "local",
              "reference": {
                "kind": "local",
                "module": null,
                "path": "u",
                "type": "RefInfo",
                "version": null
              },
              "type": "Link",
              "value": "u"
            },
            {
              "type": "text",
              "value": " and "
            },
            {
              "anchor": null,
              "exists": true,
              "kind": "local",
              "reference": {
                "kind": "local",
                "module": null,
                "path": "v",
                "type": "RefInfo",
                "version": null
              },
              "type": "Link",
              "value": "v"
            },
            {
              "type": "text",
              "value": " must be 0-d or 1-d (ndim = 0 or 1), but "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "u.ndim"
            },
            {
              "type": "text",
              "value": " need not equal "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "v.ndim"
            },
            {
              "type": "text",
              "value": ". In other words, all four possible combinations - "
            },
            {
              "type": "inlineCode",
              "value": "u.ndim = v.ndim = 0"
            },
            {
              "type": "text",
              "value": ", "
            },
            {
              "type": "inlineCode",
              "value": "u.ndim = v.ndim = 1"
            },
            {
              "type": "text",
              "value": ", "
            },
            {
              "type": "inlineCode",
              "value": "u.ndim = 1, v.ndim = 0"
            },
            {
              "type": "text",
              "value": ", and "
            },
            {
              "type": "inlineCode",
              "value": "u.ndim = 0, v.ndim = 1"
            },
            {
              "type": "text",
              "value": " - work."
            }
          ],
          "type": "paragraph"
        }
