        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "If the Timestamp has a timezone, flooring will take place relative to the local (\"wall\") time and re-localized to the same timezone. When flooring near daylight savings time, use "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "nonexistent"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "ambiguous"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " to control the re-localization behavior."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
    "Other Parameters": {
      "children": [],
      "title": null
    },
