                  }
                ],
                "param": "ts_input",
                "type_": "datetime-like, str, int, float"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Offset which Timestamp will have."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "freq",
                "type_": "str, DateOffset"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Time zone for time which Timestamp will have."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "tz",
                "type_": "str, pytz.timezone, dateutil.tz.tzfile or None"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Unit used for conversion if ts_input is of type int or float. The valid values are 'D', 'h', 'm', 's', 'ms', 'us', and 'ns'. For example, 's' means seconds and 'ms' means milliseconds."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "unit",
                "type_": "str"
              },
              {
                "desc": [],
                "param": "year, month, day",
                "type_": "int"
              },
              {
                "desc": [],
                "param": "hour, minute, second, microsecond",
                "type_": "int, optional, default 0"
              },
              {
                "desc": [],
                "param": "nanosecond",
                "type_": "int, optional, default 0"
              },
              {
                "desc": [],
                "param": "tzinfo",
                "type_": "datetime.tzinfo, optional, default None"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Due to daylight saving time, one wall clock time can occur twice when shifting from summer to winter time; fold describes whether the datetime-like corresponds  to the first (0) or the second time (1) the wall clock hits the ambiguous time."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  },
                  {
                    "data": {
                      "children": [],
                      "kind": "versionadded",
                      "title": "1.1.0"
                    },
                    "type": "Admonition"
                  }
                ],
                "param": "fold",
                "type_": "{0, 1}, default None, keyword-only"
              }
            ]
          },
