    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "children": [
                    {
                      "data": {
                        "value": "For some cases, a name cannot be converted to a valid Python identifier.     In that case "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "value": [
                          "tokenize_string"
                        ]
                      },
                      "type": "Verbatim"
                    },
                    {
                      "data": {
                        "value": " raises a SyntaxError.     In that case, we just return the name unmodified."
                      },
                      "type": "Words"
                    }
                  ]
                },
                "type": "Paragraph"
              },
              {
                "data": {
                  "children": [
                    {
                      "data": {
                        "value": "If this name was used in the query string (this makes the query call impossible)     an error will be raised by "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "value": [
                          "tokenize_backtick_quoted_string"
                        ]
                      },
                      "type": "Verbatim"
                    },
                    {
                      "data": {
                        "value": " instead,     which is not caught and propagates to the user level."
                      },
                      "type": "Words"
                    }
                  ]
                },
                "type": "Paragraph"
              }
            ]
          },
          "type": "BlockQuote"
        }
