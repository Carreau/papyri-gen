  "example_section_data": {
    "children": [
      {
        "data": {
          "value": [
            ">>> df = pd.DataFrame({\"A\": [1, 2, 3], \"B\": [4, 5, 6]})"
          ]
        },
        "type": "BlockQuote"
      },
      {
        "data": {
          "value": [
            "Change the row labels."
          ]
        },
        "type": "BlockQuote"
      },
      {
        "data": {
          "value": [
            ">>> df.set_axis(['a', 'b', 'c'], axis='index')",
            "   A  B",
            "a  1  4",
            "b  2  5",
            "c  3  6"
          ]
        },
        "type": "BlockQuote"
      },
      {
        "data": {
          "value": [
            "Change the column labels."
          ]
        },
        "type": "BlockQuote"
      },
      {
        "data": {
          "value": [
            ">>> df.set_axis(['I', 'II'], axis='columns')",
            "   I  II",
            "0  1   4",
            "1  2   5",
            "2  3   6"
          ]
        },
        "type": "BlockQuote"
      },
      {
        "data": {
          "value": [
            "Now, update the labels inplace."
          ]
        },
        "type": "BlockQuote"
      },
      {
        "data": {
          "value": [
            ">>> df.set_axis(['i', 'ii'], axis='columns', inplace=True)",
            ">>> df",
            "   i  ii",
            "0  1   4",
            "1  2   5",
            "2  3   6"
          ]
        },
        "type": "BlockQuote"
      }
