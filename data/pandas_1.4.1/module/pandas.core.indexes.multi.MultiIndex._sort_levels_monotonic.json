    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Create a new MultiIndex from the current to monotonically sorted items IN the levels. This does not actually make the entire MultiIndex monotonic, JUST the levels."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The resulting MultiIndex will have the same outward appearance, meaning the same .values and ordering. It will also be .equals() to the original."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
