      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Out of bounds values will be NA in the resulting Categorical object"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
