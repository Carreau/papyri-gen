  "example_section_data": {
    "children": [
      {
        "data": {
          "value": [
            ".. plot::",
            "    :context: close-figs"
          ]
        },
        "type": "BlockQuote"
      },
      {
        "data": {
          "value": [
            ">>> s = pd.Series([1, 3, 2])",
            ">>> s.plot.line()",
            "<AxesSubplot:ylabel='Density'>"
          ]
        },
        "type": "BlockQuote"
      },
      {
        "data": {
          "value": [
            ".. plot::",
            "    :context: close-figs"
          ]
        },
        "type": "BlockQuote"
      },
      {
        "data": {
          "value": [
            "The following example shows the populations for some animals",
            "over the years."
          ]
        },
        "type": "BlockQuote"
      },
      {
        "data": {
          "value": [
            ">>> df = pd.DataFrame({",
            "...    'pig': [20, 18, 489, 675, 1776],",
            "...    'horse': [4, 25, 281, 600, 1900]",
            "...    }, index=[1990, 1997, 2003, 2009, 2014])",
            ">>> lines = df.plot.line()"
          ]
        },
        "type": "BlockQuote"
      },
      {
        "data": {
          "value": [
            ".. plot::",
            "   :context: close-figs"
          ]
        },
        "type": "BlockQuote"
      },
      {
        "data": {
          "value": [
            "An example with subplots, so an array of axes is returned."
          ]
        },
        "type": "BlockQuote"
      },
      {
        "data": {
          "value": [
            ">>> axes = df.plot.line(subplots=True)",
            ">>> type(axes)",
            "<class 'numpy.ndarray'>"
          ]
        },
        "type": "BlockQuote"
      },
      {
        "data": {
          "value": [
            ".. plot::",
            "   :context: close-figs"
          ]
        },
        "type": "BlockQuote"
      },
      {
        "data": {
          "value": [
            "Let's repeat the same example, but specifying colors for",
            "each column (in this case, for each animal)."
          ]
        },
        "type": "BlockQuote"
      },
      {
        "data": {
          "value": [
            ">>> axes = df.plot.line(",
            "...     subplots=True, color={\"pig\": \"pink\", \"horse\": \"#742802\"}",
            "... )"
          ]
        },
        "type": "BlockQuote"
      },
      {
        "data": {
          "value": [
            ".. plot::",
            "    :context: close-figs"
          ]
        },
        "type": "BlockQuote"
      },
      {
        "data": {
          "value": [
            "The following example shows the relationship between both",
            "populations."
          ]
        },
        "type": "BlockQuote"
      },
      {
        "data": {
          "value": [
            ">>> lines = df.plot.line(x='pig', y='horse')"
          ]
        },
        "type": "BlockQuote"
      }
