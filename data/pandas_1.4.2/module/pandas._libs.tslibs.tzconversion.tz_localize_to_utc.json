                                "children": [
                                  {
                                    "data": {
                                      "value": "bool-ndarray where True signifies a DST time, False signifies a   non-DST time (note that this flag is only applicable for ambiguous   times, but the array must have the same length as vals)"
                                    },
                                    "type": "Words"
                                  }
                                ]
                              },
                              "type": "Paragraph"
                            }
