      "level": 0,
      "title": null
    },
    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Series are instantiated with "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "dtype=float64"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " by default. While "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "numpy.ndarray.view()"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " will return a view with the same data type as the original array, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "Series.view()"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " (without specified dtype) will try using "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "float64"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " and may fail if the original data type size in bytes is not the same."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
