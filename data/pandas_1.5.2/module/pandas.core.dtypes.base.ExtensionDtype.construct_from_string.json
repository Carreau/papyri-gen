    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "This is useful mainly for data types that accept parameters. For example, a period dtype accepts a frequency parameter that can be set as "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "period[H]"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " (where H means hourly frequency)."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "By default, in the abstract class, just the name of the type is expected. But subclasses can overwrite this method to accept parameters."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
