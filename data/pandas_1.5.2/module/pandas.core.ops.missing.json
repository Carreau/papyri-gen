                "data": {
                  "children": [
                    {
                      "children": [
                        {
                          "data": {
                            "children": [
                              {
                                "data": {
                                  "value": "np.array([-1, 0, 1], dtype=dtype1) % np.array([0, 0, 0], dtype=dtype2)        gives precisely the same results as the // operation."
                                },
                                "type": "Words"
                              }
                            ]
                          },
