                            {
                              "data": {
                                "children": [
                                  {
                                    "data": {
                                      "value": "'pad' / 'ffill': propagate last valid observation forward to next   valid"
                                    },
                                    "type": "Words"
                                  }
                                ]
                              },
                              "type": "Paragraph"
                            }
                          ]
                        },
                        {
                          "children": [
                            {
                              "data": {
                                "children": [
                                  {
                                    "data": {
                                      "value": "'backfill' / 'bfill': use NEXT valid observation to fill."
                                    },
                                    "type": "Words"
                                  }
                                ]
                              },
                              "type": "Paragraph"
                            }
                          ]
                        }
                      ]
                    },
                    "type": "BulletList"
                  }
                ],
                "param": "method",
                "type_": "{'backfill'/'bfill', 'pad'/'ffill'}, default None"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
