                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Any valid string path is acceptable. Only supports the local file system, remote URLs and file-like objects are not supported."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  },
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "If you want to pass in a path object, pandas accepts any "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "os.PathLike"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": "."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  },
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Alternatively, pandas accepts an open "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "pandas.HDFStore"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": " object."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "path_or_buf",
                "type_": "str, path object, pandas.HDFStore"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "The group identifier in the store. Can be omitted if the HDF file contains a single pandas object."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "key",
                "type_": "object, optional"
