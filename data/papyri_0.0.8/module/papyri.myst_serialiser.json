  "arbitrary": [
    {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "Unlike the current serializer (miniserde), that we use where enums/classes are externally tagged,"
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "Myst Json spec (https://spec.myst.tools/spec/overview) is internally tagged."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "Nodes also always carry a tag for the type, even if the container/parent can only store one type."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "This is a prototype of serializer that respect this layout."
            }
          ],
          "type": "paragraph"
        }
      ],
