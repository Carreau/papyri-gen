            {
              "data": {
                "value": "Given a linkage matrix "
              },
              "type": "Words"
            },
            {
              "data": {
                "value": [
                  "Z"
                ]
              },
              "type": "Verbatim"
            },
            {
              "data": {
                "value": ", "
