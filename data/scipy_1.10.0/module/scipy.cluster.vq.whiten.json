          "data": {
            "children": [
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Each row of the array is an observation.  The columns are the features seen during each observation."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  },
                  {
                    "data": {
                      "value": ">>> #         f0    f1    f2\n>>> obs = [[  1.,   1.,   1.],  #o0\n...        [  2.,   2.,   2.],  #o1\n...        [  3.,   3.,   3.],  #o2\n...        [  4.,   4.,   4.]]  #o3"
                    },
                    "type": "BlockVerbatim"
                  }
                ],
                "param": "obs",
                "type_": "ndarray"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Whether to check that the input matrices contain only finite numbers. Disabling may give a performance gain, but may result in problems (crashes, non-termination) if the inputs do contain infinities or NaNs. Default: True"
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "check_finite",
                "type_": "bool, optional"
