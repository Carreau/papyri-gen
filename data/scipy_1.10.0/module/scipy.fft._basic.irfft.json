              },
              {
                "data": {
                  "anchor": null,
                  "exists": true,
                  "kind": "module",
                  "reference": {
                    "kind": "module",
                    "module": "scipy",
                    "path": "scipy.fft._basic.rfft",
                    "version": "1.10.0"
                  },
                  "value": "rfft"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": ". In other words, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "irfft(rfft(x), len(x)) == x"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " to within numerical accuracy. (See Notes below for why "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "len(a)"
