    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The returned complex array contains "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "y(0), y(1),..., y(n-1)"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ", where"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": [
                    "y(j) = (x * exp(-2*pi*sqrt(-1)*j*np.arange(n)/n)).sum()"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": "."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "level": 0,
