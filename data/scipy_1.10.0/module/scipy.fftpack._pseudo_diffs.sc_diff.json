            "children": [
              {
                "data": {
                  "value": [
                    "sc_diff(cs_diff(x,a,b),b,a) == x"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " For even "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "len(x)"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ", the Nyquist mode of x is taken as zero."
                },
                "type": "Words"
