    },
    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The decomposition is:      "
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "A = P L U"
          },
          "type": "BlockVerbatim"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "where P is a permutation matrix, L lower triangular with unit diagonal elements, and U upper triangular."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null
    },
    "Methods": {
      "children": [],
      "level": 0,
      "target": null,
      "title": null
    },
    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "This is a wrapper to the "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "*GETRF"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " routines from LAPACK. Unlike "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "lu"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ", it outputs the L and U factors into a single array and returns pivot indices instead of a permutation matrix."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null
    },
