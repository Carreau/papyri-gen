                },
                "type": "Link"
              },
              {
                "data": {
                  "value": ", respectively, as:      "
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "P = numpy.hstack([numpy.eye(proj.shape[0]), proj])[:,numpy.argsort(idx)]"
          },
          "type": "BlockVerbatim"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The original matrix can then be reconstructed from its skeleton matrix "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "B"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " via:      "
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "numpy.dot(B, P)"
          },
          "type": "BlockVerbatim"
        },
        {
