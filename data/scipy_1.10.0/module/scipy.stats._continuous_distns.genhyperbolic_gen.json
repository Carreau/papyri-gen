    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The probability density function for "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "genhyperbolic"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " is:"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "f(x, p, a, b) =\n    \\frac{(a^2 - b^2)^{p/2}}\n    {\\sqrt{2\\pi}a^{p-0.5}\n    K_p\\Big(\\sqrt{a^2 - b^2}\\Big)}\n    e^{bx} \\times \\frac{K_{p - 1/2}\n    (a \\sqrt{1 + x^2})}\n    {(\\sqrt{1 + x^2})^{1/2 - p}}"
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "for "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "x, p \\in ( - \\infty; \\infty)"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ", "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "|b| < a"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " if "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "p \\ge 0"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ", "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "|b| \\le a"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " if "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "p < 0"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ". "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "K_{p}(.)"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " denotes the modified Bessel function of the second kind and order "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "p"
                },
                "type": "Math"
              },
              {
                "data": {
