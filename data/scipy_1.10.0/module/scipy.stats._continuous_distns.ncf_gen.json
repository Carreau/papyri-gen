    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The probability density function for "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "ncf"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " is:"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "f(x, n_1, n_2, \\lambda) =\n    \\exp\\left(\\frac{\\lambda}{2} +\n              \\lambda n_1 \\frac{x}{2(n_1 x + n_2)}\n        \\right)\n    n_1^{n_1/2} n_2^{n_2/2} x^{n_1/2 - 1} \\\\\n    (n_2 + n_1 x)^{-(n_1 + n_2)/2}\n    \\gamma(n_1/2) \\gamma(1 + n_2/2) \\\\\n    \\frac{L^{\\frac{n_1}{2}-1}_{n_2/2}\n        \\left(-\\lambda n_1 \\frac{x}{2(n_1 x + n_2)}\\right)}\n    {B(n_1/2, n_2/2)\n        \\gamma\\left(\\frac{n_1 + n_2}{2}\\right)}"
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "for "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "n_1, n_2 > 0"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ", "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\lambda \\ge 0"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ".  Here "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "n_1"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is the degrees of freedom in the numerator, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "n_2"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " the degrees of freedom in the denominator, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\lambda"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " the non-centrality parameter, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\gamma"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is the logarithm of the Gamma function, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "L_n^k"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is a generalized Laguerre polynomial and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "B"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is the beta function."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
