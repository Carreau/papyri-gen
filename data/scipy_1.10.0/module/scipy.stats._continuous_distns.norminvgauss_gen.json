    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The probability density function for "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "norminvgauss"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " is:"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "f(x, a, b) = \\frac{a \\, K_1(a \\sqrt{1 + x^2})}{\\pi \\sqrt{1 + x^2}} \\,\n             \\exp(\\sqrt{a^2 - b^2} + b x)"
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "x"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is a real number, the parameter "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "a"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is the tail heaviness and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "b"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is the asymmetry parameter satisfying "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "a > 0"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "|b| <= a"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ". "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "K_1"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is the modified Bessel function of second kind ("
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "scipy.special.k1"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": ")."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "%(after_notes)s"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "A normal inverse Gaussian random variable "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "Y"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " with parameters "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "a"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "b"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " can be expressed as a normal mean-variance mixture: "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "Y = b * V + sqrt(V) * X"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "X"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " is "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "norm(0,1)"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "V"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " is "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "invgauss(mu=1/sqrt(a**2 - b**2))"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": ". This representation is used to generate random variates."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Another common parametrization of the distribution (see Equation 2.1 in ) is given by the following expression of the pdf:"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "g(x, \\alpha, \\beta, \\delta, \\mu) =\n\\frac{\\alpha\\delta K_1\\left(\\alpha\\sqrt{\\delta^2 + (x - \\mu)^2}\\right)}\n{\\pi \\sqrt{\\delta^2 + (x - \\mu)^2}} \\,\ne^{\\delta \\sqrt{\\alpha^2 - \\beta^2} + \\beta (x - \\mu)}"
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "In SciPy, this corresponds to "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "a = alpha * delta, b = beta * delta, loc = mu, scale=delta"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": "."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
