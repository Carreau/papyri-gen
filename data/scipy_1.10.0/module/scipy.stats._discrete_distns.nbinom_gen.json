    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Negative binomial distribution describes a sequence of i.i.d. Bernoulli trials, repeated until a predefined, non-random number of successes occurs."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The probability mass function of the number of failures for "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "nbinom"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " is:"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "f(k) = \\binom{k+n-1}{n-1} p^n (1-p)^k"
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "for "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "k \\ge 0"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ", "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "0 < p \\leq 1"
                },
                "type": "Math"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "nbinom"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " takes "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "n"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "p"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " as shape parameters where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "n"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is the number of successes, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "p"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is the probability of a single success, and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "1-p"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is the probability of a single failure."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Another common parameterization of the negative binomial distribution is in terms of the mean number of failures "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\mu"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " to achieve "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "n"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " successes. The mean "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\mu"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is related to the probability of success as"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "p = \\frac{n}{n + \\mu}"
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The number of successes "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "n"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " may also be specified in terms of a \"dispersion\", \"heterogeneity\", or \"aggregation\" parameter "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\alpha"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ", which relates the mean "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\mu"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " to the variance "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\sigma^2"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ", e.g. "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\sigma^2 = \\mu + \\alpha \\mu^2"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ". Regardless of the convention used for "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\alpha"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ","
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "p &= \\frac{\\mu}{\\sigma^2} \\\\\nn &= \\frac{\\mu^2}{\\sigma^2 - \\mu}"
          },
          "type": "BlockMath"
        },
