        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Probability distribution of the difference of two correlated or uncorrelated Poisson random variables."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Let "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "k_1"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "k_2"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " be two Poisson-distributed r.v. with expected values "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\lambda_1"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\lambda_2"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ". Then, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "k_1 - k_2"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " follows a Skellam distribution with parameters "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\mu_1 = \\lambda_1 - \\rho \\sqrt{\\lambda_1 \\lambda_2}"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\mu_2 = \\lambda_2 - \\rho \\sqrt{\\lambda_1 \\lambda_2}"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ", where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\rho"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is the correlation coefficient between "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "k_1"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "k_2"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ". If the two Poisson-distributed r.v. are independent then "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\rho = 0"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": "."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Parameters "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\mu_1"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\mu_2"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " must be strictly positive."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
