    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The nth k-statistic k_n is the unique symmetric unbiased estimator of the nth cumulant kappa_n."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null
    },
    "Methods": {
      "children": [],
      "level": 0,
      "target": null,
      "title": null
    },
    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "For a sample size n, the first few k-statistics are given by:"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "k_{1} = \\mu\nk_{2} = \\frac{n}{n-1} m_{2}\nk_{3} = \\frac{ n^{2} } {(n-1) (n-2)} m_{3}\nk_{4} = \\frac{ n^{2} [(n + 1)m_{4} - 3(n - 1) m^2_{2}]} {(n-1) (n-2) (n-3)}"
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\mu"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is the sample mean, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "m_2"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is the sample variance, and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "m_i"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is the i-th sample central moment."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Beginning in SciPy 1.9, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "np.matrix"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " inputs (not recommended for new code) are converted to "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "np.ndarray"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " before the calculation is performed. In this case, the output will be a scalar or "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "np.ndarray"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " of appropriate shape rather than a 2D "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "np.matrix"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ". Similarly, while masked elements of masked arrays are ignored, the output will be a scalar or "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "np.ndarray"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " rather than a masked array with "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "mask=False"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": "."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null
    },
    "Other Parameters": {
      "children": [],
