                    "path": "x",
                    "version": null
                  },
                  "value": "x"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " increases, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "anchor": null,
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "y",
                    "version": null
                  },
                  "value": "y"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " decreases."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Missing values are discarded pair-wise: if a value is missing in "
                },
                "type": "Words"
              },
