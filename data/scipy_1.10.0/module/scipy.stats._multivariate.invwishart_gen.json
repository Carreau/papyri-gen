        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The scale matrix "
                },
                "type": "Words"
              },
              {
                "data": {
                  "anchor": null,
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "scale",
                    "version": null
                  },
                  "value": "scale"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " must be a symmetric positive definite matrix. Singular matrices, including the symmetric positive semi-definite case, are not supported. Symmetry is not checked; only the lower triangular portion is used."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The inverse Wishart distribution is often denoted"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "W_p^{-1}(\\nu, \\Psi)"
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\nu"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is the degrees of freedom and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\Psi"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is the "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "p \\times p"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " scale matrix."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The probability density function for "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "invwishart"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " has support over positive definite matrices "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "S"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": "; if "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "S \\sim W^{-1}_p(\\nu, \\Sigma)"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ", then its PDF is given by:"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "f(S) = \\frac{|\\Sigma|^\\frac{\\nu}{2}}{2^{ \\frac{\\nu p}{2} }\n       |S|^{\\frac{\\nu + p + 1}{2}} \\Gamma_p \\left(\\frac{\\nu}{2} \\right)}\n       \\exp\\left( -tr(\\Sigma S^{-1}) / 2 \\right)"
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "If "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "S \\sim W_p^{-1}(\\nu, \\Psi)"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " (inverse Wishart) then "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "S^{-1} \\sim W_p(\\nu, \\Psi^{-1})"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " (Wishart)."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "If the scale matrix is 1-dimensional and equal to one, then the inverse Wishart distribution "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "W_1(\\nu, 1)"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " collapses to the inverse Gamma distribution with parameters shape = "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\frac{\\nu}{2}"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " and scale = "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\frac{1}{2}"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": "."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [],
            "kind": "versionadded",
            "title": "0.16.0"
          },
          "type": "Admonition"
        }
