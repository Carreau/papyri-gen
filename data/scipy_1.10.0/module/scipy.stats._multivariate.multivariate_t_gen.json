              },
              {
                "data": {
                  "value": " is the "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "p"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": "-dimensional location, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\boldsymbol{\\Sigma}"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " the "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "p \\times p"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": "-dimensional shape matrix, and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\nu"
                },
                "type": "Math"
              },
              {
