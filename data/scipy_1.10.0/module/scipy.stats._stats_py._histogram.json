                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "numbins",
                "type_": "int, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "The lower and upper values for the range of the histogram. If no value is given, a range slightly larger than the range of the values in a is used. Specifically "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "(a.min() - s, a.max() + s)"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": ", where "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "s = (1/2)(a.max() - a.min()) / (numbins - 1)"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": "."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "defaultlimits",
                "type_": "tuple (lower, upper), optional"
              },
