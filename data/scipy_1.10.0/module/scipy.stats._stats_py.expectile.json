    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Expectiles are a generalization of the expectation in the same way as quantiles are a generalization of the median. The expectile at level "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "alpha = 0.5"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " is the mean (average). See Notes for more details."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null
    },
    "Methods": {
      "children": [],
      "level": 0,
      "target": null,
      "title": null
    },
    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "In general, the expectile at level "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\alpha"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " of a random variable "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "X"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " with cumulative distribution function (CDF) "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "F"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is given by the unique solution "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "t"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " of:"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "\\alpha E((X - t)_+) = (1 - \\alpha) E((t - X)_+) \\,."
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Here, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "(x)_+ = \\max(0, x)"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is the positive part of "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "x"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ". This equation can be equivalently written as:"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "\\alpha \\int_t^\\infty (x - t)\\mathrm{d}F(x)\n= (1 - \\alpha) \\int_{-\\infty}^t (t - x)\\mathrm{d}F(x) \\,."
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The empirical expectile at level "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\alpha"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " ("
                },
                "type": "Words"
              },
              {
                "data": {
                  "anchor": null,
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "alpha",
                    "version": null
                  },
                  "value": "alpha"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": ") of a sample "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "a_i"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " (the array "
                },
                "type": "Words"
              },
              {
                "data": {
                  "anchor": null,
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "a",
                    "version": null
                  },
                  "value": "a"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": ") is defined by plugging in the empirical CDF of "
                },
                "type": "Words"
              },
              {
                "data": {
                  "anchor": null,
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "a",
                    "version": null
                  },
                  "value": "a"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": ". Given sample or case weights "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "w"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " (the array "
                },
                "type": "Words"
              },
              {
                "data": {
                  "anchor": null,
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "weights",
                    "version": null
                  },
                  "value": "weights"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": "), it reads "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "F_a(x) = \\frac{1}{\\sum_i a_i} \\sum_i w_i 1_{a_i \\leq x}"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " with indicator function "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "1_{A}"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ". This leads to the definition of the empirical expectile at level "
                },
                "type": "Words"
              },
              {
                "data": {
                  "anchor": null,
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "alpha",
                    "version": null
                  },
                  "value": "alpha"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " as the unique solution "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "t"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " of:"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "\\alpha \\sum_{i=1}^n w_i (a_i - t)_+ =\n    (1 - \\alpha) \\sum_{i=1}^n w_i (t - a_i)_+ \\,."
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "For "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\alpha=0.5"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ", this simplifies to the weighted average. Furthermore, the larger "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\alpha"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ", the larger the value of the expectile."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "As a final remark, the expectile at level "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\alpha"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " can also be written as a minimization problem. One often used choice is"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "\\operatorname{argmin}_t\nE(\\lvert 1_{t\\geq X} - \\alpha\\rvert(t - X)^2) \\,."
          },
          "type": "BlockMath"
        }
      ],
      "level": 0,
      "target": null,
      "title": null
    },
