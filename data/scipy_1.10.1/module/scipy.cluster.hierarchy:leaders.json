    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "Returns the root nodes in a hierarchical clustering corresponding to a cut defined by a flat cluster assignment vector "
            },
            {
              "type": "inlineCode",
              "value": "T"
            },
            {
              "type": "text",
              "value": ". See the "
            },
            {
              "type": "inlineCode",
              "value": "fcluster"
            },
            {
              "type": "text",
              "value": " function for more information on the format of "
            },
            {
              "type": "inlineCode",
              "value": "T"
            },
            {
              "type": "text",
              "value": "."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "For each flat cluster "
            },
            {
              "type": "inlineMath",
              "value": "j"
            },
            {
              "type": "text",
              "value": " of the "
            },
            {
              "type": "inlineMath",
              "value": "k"
            },
            {
              "type": "text",
              "value": " flat clusters represented in the n-sized flat cluster assignment vector "
            },
            {
              "type": "inlineCode",
              "value": "T"
            },
            {
              "type": "text",
              "value": ", this function finds the lowest cluster node "
            },
            {
              "type": "inlineMath",
              "value": "i"
            },
            {
              "type": "text",
              "value": " in the linkage tree Z, such that:"
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "children": [
                {
                  "children": [
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "leaf descendants belong only to flat cluster j     (i.e., "
                        },
                        {
                          "type": "inlineCode",
                          "value": "T[p]==j"
                        },
                        {
                          "type": "text",
                          "value": " for all "
                        },
                        {
                          "type": "inlineMath",
                          "value": "p"
                        },
                        {
                          "type": "text",
                          "value": " in "
                        },
                        {
                          "type": "inlineMath",
                          "value": "S(i)"
                        },
                        {
                          "type": "text",
                          "value": ", where     "
                        },
                        {
                          "type": "inlineMath",
                          "value": "S(i)"
                        },
                        {
                          "type": "text",
                          "value": " is the set of leaf ids of descendant leaf nodes     with cluster node "
                        },
                        {
                          "type": "inlineMath",
                          "value": "i"
                        },
                        {
                          "type": "text",
                          "value": ")"
                        }
                      ],
                      "type": "paragraph"
                    }
                  ],
                  "spread": false,
                  "type": "listItem"
                },
                {
                  "children": [
                    {
                      "children": [
                        {
                          "type": "text",
                          "value": "there does not exist a leaf that is not a descendant with     "
                        },
                        {
                          "type": "inlineMath",
                          "value": "i"
                        },
                        {
                          "type": "text",
                          "value": " that also belongs to cluster "
                        },
                        {
                          "type": "inlineMath",
                          "value": "j"
                        },
                        {
                          "type": "text",
                          "value": "     (i.e., "
                        },
                        {
                          "type": "inlineCode",
                          "value": "T[q]!=j"
                        },
                        {
                          "type": "text",
                          "value": " for all "
                        },
                        {
                          "type": "inlineMath",
                          "value": "q"
                        },
                        {
                          "type": "text",
                          "value": " not in "
                        },
                        {
                          "type": "inlineMath",
                          "value": "S(i)"
                        },
                        {
                          "type": "text",
                          "value": "). If     this condition is violated, "
                        },
                        {
                          "type": "inlineCode",
                          "value": "T"
                        },
                        {
                          "type": "text",
                          "value": " is not a valid cluster     assignment vector, and an exception will be thrown."
                        }
                      ],
                      "type": "paragraph"
                    }
                  ],
                  "spread": false,
                  "type": "listItem"
                }
              ],
              "ordered": false,
              "spread": false,
              "start": 1,
              "type": "list"
            }
          ],
          "type": "blockquote"
        }
      ],
      "level": 0,
