      {
        "type": "code",
        "value": "from scipy.cluster.hierarchy import ward, num_obs_linkage\nfrom scipy.spatial.distance import pdist"
      },
      {
        "type": "code",
        "value": "X = [[0, 0], [0, 1], [1, 0],\n     [0, 4], [0, 3], [1, 4],\n     [4, 0], [3, 0], [4, 1],\n     [4, 4], [3, 4], [4, 3]]"
