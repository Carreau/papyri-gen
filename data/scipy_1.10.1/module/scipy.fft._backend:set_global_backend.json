    "Parameters": {
      "children": [
        {
          "children": [
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "The backend to use. Can either be a "
                    },
                    {
                      "type": "inlineCode",
                      "value": "str"
                    },
                    {
                      "type": "text",
                      "value": " containing the name of a known backend {'scipy'} or an object that implements the uarray protocol."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "backend",
              "type": "Param",
              "type_": "{object, 'scipy'}"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Whether to coerce input types when trying this backend."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "coerce",
              "type": "Param",
              "type_": "bool"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "If "
                    },
                    {
                      "type": "inlineCode",
                      "value": "True"
                    },
                    {
                      "type": "text",
                      "value": ", no more backends will be tried if this fails. Implied by "
                    },
                    {
                      "type": "inlineCode",
                      "value": "coerce=True"
                    },
                    {
                      "type": "text",
                      "value": "."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "only",
              "type": "Param",
              "type_": "bool"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "If "
                    },
                    {
                      "type": "inlineCode",
                      "value": "True"
                    },
                    {
                      "type": "text",
                      "value": ", the global backend is tried after registered backends."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "try_last",
              "type": "Param",
              "type_": "bool"
