    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "This utility method replaces the default backend for permanent use. It will be tried in the list of backends automatically, unless the "
            },
            {
              "type": "inlineCode",
              "value": "only"
            },
            {
              "type": "text",
              "value": " flag is set on a backend. This will be the first tried backend outside the "
            },
            {
              "type": "inlineCode",
              "value": "set_backend"
            },
            {
              "type": "text",
              "value": " context manager."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
    "Methods": {
      "children": [],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "This will overwrite the previously set global backend, which, by default, is the SciPy implementation."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
    "Other Parameters": {
