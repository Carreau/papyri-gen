    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "This function computes a discrete version of the Hankel transform"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "a(r) = \\int_{0}^{\\infty} \\! A(k) \\, J_\\mu(kr) \\, r \\, dk \\;,"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "where "
            },
            {
              "type": "inlineMath",
              "value": "J_\\mu"
            },
            {
              "type": "text",
              "value": " is the Bessel function of order "
            },
            {
              "type": "inlineMath",
              "value": "\\mu"
            },
            {
              "type": "text",
              "value": ".  The index "
            },
            {
              "type": "inlineMath",
              "value": "\\mu"
            },
            {
              "type": "text",
              "value": " may be any real number, positive or negative."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "See "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "fht"
            },
            {
              "type": "text",
              "value": " for further details."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
