        {
          "children": [
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "The returned real array contains    "
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "type": "code",
                  "value": "[y(0),Re(y(1)),Im(y(1)),...,Re(y(n/2))]              if n is even\n[y(0),Re(y(1)),Im(y(1)),...,Re(y(n/2)),Im(y(n/2))]   if n is odd"
                },
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "where    "
                    }
                  ],
                  "type": "paragraph"
                },
                {
                  "type": "code",
                  "value": "y(j) = sum[k=0..n-1] x[k] * exp(-sqrt(-1)*j*k*2*pi/n)\nj = 0..n-1"
                }
              ],
              "param": "z",
              "type": "Param",
              "type_": "real ndarray"
            }
