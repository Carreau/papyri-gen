      "type": "Section"
    },
    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "inlineCode",
              "value": "sc_diff(cs_diff(x,a,b),b,a) == x"
            },
            {
              "type": "text",
              "value": " For even "
            },
            {
              "type": "inlineCode",
              "value": "len(x)"
            },
            {
              "type": "text",
              "value": ", the Nyquist mode of x is taken as zero."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
