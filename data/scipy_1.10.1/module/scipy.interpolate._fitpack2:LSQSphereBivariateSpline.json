                    {
                      "type": "inlineCode",
                      "value": "[0, pi]"
                    },
                    {
                      "type": "text",
                      "value": ", and phi must lie within the interval "
                    },
                    {
                      "type": "inlineCode",
                      "value": "[0, 2pi]"
                    },
                    {
                      "type": "text",
                      "value": "."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "theta, phi, r",
              "type": "Param",
              "type_": "array_like"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Strictly ordered 1-D sequences of knots coordinates. Coordinates must satisfy "
                    },
                    {
                      "type": "inlineCode",
                      "value": "0 < tt[i] < pi"
                    },
                    {
                      "type": "text",
                      "value": ", "
                    },
                    {
                      "type": "inlineCode",
                      "value": "0 < tp[i] < 2*pi"
                    },
                    {
                      "type": "text",
                      "value": "."
