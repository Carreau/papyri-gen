    "children": [
      {
        "type": "code",
        "value": "from scipy import ndimage\nimport numpy as np\na = np.arange(12.).reshape((4, 3))\na"
      },
      {
        "type": "code",
        "value": "ndimage.map_coordinates(a, [[0.5, 2], [0.5, 1]], order=1)"
      },
      {
        "type": "text",
        "value": "Above, the interpolated value of a[0.5, 0.5] gives output[0], while\na[2, 1] is output[1]."
      },
      {
        "type": "code",
        "value": "inds = np.array([[0.5, 2], [0.5, 4]])\nndimage.map_coordinates(a, inds, order=1, cval=-33.3)"
      },
      {
