  "example_section_data": {
    "children": [
      {
        "type": "code",
        "value": "import numpy as np\na = np.array([[1, 2, 0, 0],\n              [5, 3, 0, 4],\n              [0, 0, 0, 7],\n              [9, 3, 0, 0]])\nfrom scipy import ndimage\nlbl, nlbl = ndimage.label(a)\nlbls = np.arange(1, nlbl+1)\nndimage.labeled_comprehension(a, lbl, lbls, np.mean, float, 0)"
      },
      {
        "type": "text",
        "value": "Falling back to `default`:"
      },
      {
        "type": "code",
        "value": "lbls = np.arange(1, nlbl+2)\nndimage.labeled_comprehension(a, lbl, lbls, np.mean, float, -1)"
      },
      {
        "type": "text",
        "value": "Passing positions:"
      },
      {
        "type": "code",
        "value": "def fn(val, pos):\n    print(\"fn says: %s : %s\" % (val, pos))\n    return (val.sum()) if (pos.sum() % 2 == 0) else (-val.sum())"
      },
      {
        "type": "code",
        "value": "ndimage.labeled_comprehension(a, lbl, lbls, fn, float, 0, True)"
      }
    ],
