      {
        "type": "code",
        "value": "from scipy import ndimage\nimport numpy as np\na = np.zeros((7,7), dtype=int)\na[1, 1] = 1; a[2:4, 2:4] = 1; a[4:6, 4:6] = 1\na"
      },
      {
        "type": "code",
        "value": "structure1 = np.array([[1, 0, 0], [0, 1, 1], [0, 1, 1]])\nstructure1"
      },
      {
        "type": "code",
        "value": "# Find the matches of structure1 in the array a\nndimage.binary_hit_or_miss(a, structure1=structure1).astype(int)"
      },
      {
        "type": "code",
        "value": "# Change the origin of the filter\n# origin1=1 is equivalent to origin1=(1,1) here\nndimage.binary_hit_or_miss(a, structure1=structure1,\\\norigin1=1).astype(int)"
      }
