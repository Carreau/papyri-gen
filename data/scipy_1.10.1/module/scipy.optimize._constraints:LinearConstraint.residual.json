    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "For a linear constraint of the form      "
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "code",
          "value": "lb <= A@x <= ub"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "the lower and upper residuals between "
            },
            {
              "type": "inlineCode",
              "value": "A@x"
            },
            {
              "type": "text",
              "value": " and the limits are values "
            },
            {
              "type": "inlineCode",
              "value": "sl"
            },
            {
              "type": "text",
              "value": " and "
            },
            {
              "type": "inlineCode",
              "value": "sb"
            },
            {
              "type": "text",
              "value": " such that      "
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "code",
          "value": "lb + sl == A@x == ub - sb"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "When all elements of "
            },
            {
              "type": "inlineCode",
              "value": "sl"
            },
            {
              "type": "text",
              "value": " and "
            },
            {
              "type": "inlineCode",
              "value": "sb"
            },
            {
              "type": "text",
              "value": " are positive, all elements of the constraint are satisfied; a negative element in "
            },
            {
              "type": "inlineCode",
              "value": "sl"
            },
            {
              "type": "text",
              "value": " or "
            },
            {
              "type": "inlineCode",
              "value": "sb"
            },
            {
              "type": "text",
              "value": " indicates that the corresponding element of the constraint is not satisfied."
            }
          ],
          "type": "paragraph"
        }
      ],
