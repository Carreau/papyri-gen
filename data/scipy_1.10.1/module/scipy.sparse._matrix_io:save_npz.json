    "children": [
      {
        "type": "text",
        "value": "Store sparse matrix to disk, and load it again:"
      },
      {
        "type": "code",
        "value": "import numpy as np\nimport scipy.sparse\nsparse_matrix = scipy.sparse.csc_matrix(np.array([[0, 0, 3], [4, 0, 0]]))\nsparse_matrix"
      },
      {
        "type": "code",
        "value": "sparse_matrix.toarray()"
      },
      {
        "type": "code",
        "value": "scipy.sparse.save_npz('/tmp/sparse_matrix.npz', sparse_matrix)\nsparse_matrix = scipy.sparse.load_npz('/tmp/sparse_matrix.npz')"
      },
      {
        "type": "code",
        "value": "sparse_matrix"
      },
      {
        "type": "code",
        "value": "sparse_matrix.toarray()"
      }
