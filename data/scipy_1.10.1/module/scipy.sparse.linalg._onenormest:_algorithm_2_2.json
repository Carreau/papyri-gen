              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "A linear operator that can produce matrix products."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "A",
              "type": "Param",
              "type_": "ndarray or other linear operator"
            },
            {
              "desc": [
                {
                  "children": [
                    {
