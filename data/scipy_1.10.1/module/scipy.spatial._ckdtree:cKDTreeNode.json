  "content": {
    "Attributes": {
      "children": [
        {
          "children": [
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "The depth of the node. 0 is the level of the root node."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "level",
              "type": "Param",
              "type_": "int"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "The dimension along which this node is split. If this value is -1 the node is a leafnode in the kd-tree. Leafnodes are not split further and scanned by brute force."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "split_dim",
              "type": "Param",
              "type_": "int"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "The value used to separate split this node. Points with value >= split in the split_dim dimension are sorted to the 'greater' subnode whereas those with value < split are sorted to the 'lesser' subnode."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "split",
              "type": "Param",
              "type_": "float"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "The number of data points sorted to this node."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "children",
              "type": "Param",
              "type_": "int"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "An array with the data points sorted to this node."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "data_points",
              "type": "Param",
              "type_": "ndarray of float64"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "An array with the indices of the data points sorted to this node. The indices refer to the position in the data set used to construct the kd-tree."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "indices",
              "type": "Param",
              "type_": "ndarray of intp"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Subnode with the 'lesser' data points. This attribute is None for leafnodes."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "lesser",
              "type": "Param",
              "type_": "cKDTreeNode or None"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "Subnode with the 'greater' data points. This attribute is None for leafnodes."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "greater",
              "type": "Param",
              "type_": "cKDTreeNode or None"
            }
          ],
          "type": "Parameters"
        }
