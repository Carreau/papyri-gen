    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "For arrays with "
            },
            {
              "type": "inlineCode",
              "value": "exact=True"
            },
            {
              "type": "text",
              "value": ", the factorial is computed only once, for the largest input, with each other result computed in the process. The output dtype is increased to "
            },
            {
              "type": "inlineCode",
              "value": "int64"
            },
            {
              "type": "text",
              "value": " or "
            },
            {
              "type": "inlineCode",
              "value": "object"
            },
            {
              "type": "text",
              "value": " if necessary."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "With "
            },
            {
              "type": "inlineCode",
              "value": "exact=False"
            },
            {
              "type": "text",
              "value": " the factorial is approximated using the gamma function:"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "n! = \\Gamma(n+1)"
        }
      ],
      "level": 0,
