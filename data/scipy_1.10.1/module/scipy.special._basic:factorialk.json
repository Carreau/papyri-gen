    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "This is the multifactorial of n skipping k values.  For example,"
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "children": [
                {
                  "type": "text",
                  "value": "factorialk(17, 4) = 17!!!! = 17 * 13 * 9 * 5 * 1"
                }
              ],
              "type": "paragraph"
            }
          ],
          "type": "blockquote"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "In particular, for any integer "
            },
            {
              "type": "inlineCode",
              "value": "n"
            },
            {
              "type": "text",
              "value": ", we have"
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "children": [
                {
                  "type": "text",
                  "value": "factorialk(n, 1) = factorial(n)"
                }
              ],
              "type": "paragraph"
            },
            {
              "children": [
                {
                  "type": "text",
                  "value": "factorialk(n, 2) = factorial2(n)"
                }
              ],
              "type": "paragraph"
            }
          ],
          "type": "blockquote"
        }
      ],
      "level": 0,
