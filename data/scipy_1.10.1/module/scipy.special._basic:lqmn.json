    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "Computes the associated Legendre function of the second kind of order m and degree n, "
            },
            {
              "type": "inlineCode",
              "value": "Qmn(z)"
            },
            {
              "type": "text",
              "value": " = "
            },
            {
              "type": "inlineMath",
              "value": "Q_n^m(z)"
            },
            {
              "type": "text",
              "value": ", and its derivative, "
            },
            {
              "type": "inlineCode",
              "value": "Qmn'(z)"
            },
            {
              "type": "text",
              "value": ". Returns two arrays of size "
            },
            {
              "type": "inlineCode",
              "value": "(m+1, n+1)"
            },
            {
              "type": "text",
              "value": " containing "
            },
            {
              "type": "inlineCode",
              "value": "Qmn(z)"
            },
            {
              "type": "text",
              "value": " and "
            },
            {
              "type": "inlineCode",
              "value": "Qmn'(z)"
            },
            {
              "type": "text",
              "value": " for all orders from "
            },
            {
              "type": "inlineCode",
              "value": "0..m"
            },
            {
              "type": "text",
              "value": " and degrees from "
            },
            {
              "type": "inlineCode",
              "value": "0..n"
            },
            {
              "type": "text",
              "value": "."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
