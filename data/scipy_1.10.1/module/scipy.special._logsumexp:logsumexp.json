  "example_section_data": {
    "children": [
      {
        "type": "code",
        "value": "import numpy as np\nfrom scipy.special import logsumexp\na = np.arange(10)\nlogsumexp(a)"
      },
      {
        "type": "code",
        "value": "np.log(np.sum(np.exp(a)))"
      },
      {
        "type": "text",
        "value": "With weights"
      },
      {
        "type": "code",
        "value": "a = np.arange(10)\nb = np.arange(10, 0, -1)\nlogsumexp(a, b=b)"
      },
      {
        "type": "code",
        "value": "np.log(np.sum(b*np.exp(a)))"
      },
      {
        "type": "text",
        "value": "Returning a sign flag"
      },
      {
        "type": "code",
        "value": "logsumexp([1,2],b=[1,-1],return_sign=True)"
      },
      {
        "type": "text",
        "value": "Notice that `logsumexp` does not directly support masked arrays. To use it\non a masked array, convert the mask into zero weights:"
      },
      {
        "type": "code",
        "value": "a = np.ma.array([np.log(2), 2, np.log(3)],\n                 mask=[False, True, False])\nb = (~a.mask).astype(int)\nlogsumexp(a.data, b=b), np.log(5)"
      }
    ],
