  "example_section_data": {
    "children": [
      {
        "type": "code",
        "value": "import scipy.special as sc\nfrom pytest import raises\nsc.gammaln(0)"
      },
      {
        "type": "code",
        "value": "olderr = sc.seterr(singular='raise')\nwith raises(sc.SpecialFunctionError):\n    sc.gammaln(0)"
      },
      {
        "type": "code",
        "value": "_ = sc.seterr(**olderr)"
      },
      {
        "type": "text",
        "value": "We can also raise for every category except one."
      },
      {
        "type": "code",
        "value": "olderr = sc.seterr(all='raise', singular='ignore')\nsc.gammaln(0)"
      },
      {
        "type": "code",
        "value": "with raises(sc.SpecialFunctionError):\n    sc.spence(-1)"
      },
      {
        "type": "code",
        "value": "_ = sc.seterr(**olderr)"
      }
    ],
