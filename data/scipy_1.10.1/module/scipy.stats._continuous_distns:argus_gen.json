    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "The probability density function for "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "argus"
            },
            {
              "type": "text",
              "value": " is:"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "f(x, \\chi) = \\frac{\\chi^3}{\\sqrt{2\\pi} \\Psi(\\chi)} x \\sqrt{1-x^2}\n             \\exp(-\\chi^2 (1 - x^2)/2)"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "for "
            },
            {
              "type": "inlineMath",
              "value": "0 < x < 1"
            },
            {
              "type": "text",
              "value": " and "
            },
            {
              "type": "inlineMath",
              "value": "\\chi > 0"
            },
            {
              "type": "text",
              "value": ", where"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "\\Psi(\\chi) = \\Phi(\\chi) - \\chi \\phi(\\chi) - 1/2"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "with "
            },
            {
              "type": "inlineMath",
              "value": "\\Phi"
            },
            {
              "type": "text",
              "value": " and "
            },
            {
              "type": "inlineMath",
              "value": "\\phi"
            },
            {
              "type": "text",
              "value": " being the CDF and PDF of a standard normal distribution, respectively."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "argus"
            },
            {
              "type": "text",
              "value": " takes "
            },
            {
              "type": "inlineMath",
              "value": "\\chi"
            },
            {
              "type": "text",
              "value": " as shape a parameter."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "%(after_notes)s"
            }
          ],
