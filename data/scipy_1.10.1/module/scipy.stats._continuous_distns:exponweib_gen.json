    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "The probability density function for "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "exponweib"
            },
            {
              "type": "text",
              "value": " is:"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "f(x, a, c) = a c [1-\\exp(-x^c)]^{a-1} \\exp(-x^c) x^{c-1}"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "and its cumulative distribution function is:"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "F(x, a, c) = [1-\\exp(-x^c)]^a"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "for "
            },
            {
              "type": "inlineMath",
              "value": "x > 0"
            },
            {
              "type": "text",
              "value": ", "
            },
            {
              "type": "inlineMath",
              "value": "a > 0"
            },
            {
              "type": "text",
              "value": ", "
            },
            {
              "type": "inlineMath",
              "value": "c > 0"
            },
            {
              "type": "text",
              "value": "."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "exponweib"
            },
            {
              "type": "text",
              "value": " takes "
            },
            {
              "type": "inlineMath",
              "value": "a"
            },
            {
              "type": "text",
              "value": " and "
            },
            {
              "type": "inlineMath",
              "value": "c"
            },
            {
              "type": "text",
              "value": " as shape parameters:"
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "inlineMath",
                      "value": "a"
                    },
                    {
                      "type": "text",
                      "value": " is the exponentiation parameter,   with the special case "
                    },
                    {
                      "type": "inlineMath",
                      "value": "a=1"
                    },
                    {
                      "type": "text",
                      "value": " corresponding to the   (non-exponentiated) Weibull distribution "
                    },
                    {
                      "domain": null,
                      "role": null,
                      "type": "Directive",
                      "value": "weibull_min"
                    },
                    {
                      "type": "text",
                      "value": "."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            },
            {
              "children": [
                {
                  "children": [
                    {
                      "type": "inlineMath",
                      "value": "c"
                    },
                    {
                      "type": "text",
                      "value": " is the shape parameter of the non-exponentiated Weibull law."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "spread": false,
              "type": "listItem"
            }
          ],
