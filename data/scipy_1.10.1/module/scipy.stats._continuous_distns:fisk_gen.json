    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "The probability density function for "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "fisk"
            },
            {
              "type": "text",
              "value": " is:"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "f(x, c) = \\frac{c x^{c-1}}\n               {(1 + x^c)^2}"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "for "
            },
            {
              "type": "inlineMath",
              "value": "x >= 0"
            },
            {
              "type": "text",
              "value": " and "
            },
            {
              "type": "inlineMath",
              "value": "c > 0"
            },
            {
              "type": "text",
              "value": "."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "Please note that the above expression can be transformed into the following one, which is also commonly used:"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "f(x, c) = \\frac{c x^{-c-1}}\n               {(1 + x^{-c})^2}"
        },
        {
          "children": [
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "fisk"
            },
            {
              "type": "text",
              "value": " takes "
            },
            {
              "type": "inlineCode",
              "value": "c"
            },
            {
              "type": "text",
              "value": " as a shape parameter for "
            },
            {
              "type": "inlineMath",
              "value": "c"
            },
            {
              "type": "text",
              "value": "."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "fisk"
            },
            {
              "type": "text",
              "value": " is a special case of "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "burr"
            },
            {
              "type": "text",
              "value": " or "
            },
            {
              "domain": null,
