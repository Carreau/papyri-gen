    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "The probability density function for "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "gausshyper"
            },
            {
              "type": "text",
              "value": " is:"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "f(x, a, b, c, z) = C x^{a-1} (1-x)^{b-1} (1+zx)^{-c}"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "for "
            },
            {
              "type": "inlineMath",
              "value": "0 \\le x \\le 1"
            },
            {
              "type": "text",
              "value": ", "
            },
            {
              "type": "inlineMath",
              "value": "a,b > 0"
            },
            {
              "type": "text",
              "value": ", "
            },
            {
              "type": "inlineMath",
              "value": "c"
            },
            {
              "type": "text",
              "value": " a real number, "
            },
            {
              "type": "inlineMath",
              "value": "z > -1"
            },
            {
              "type": "text",
              "value": ", and "
            },
            {
              "type": "inlineMath",
              "value": "C = \\frac{1}{B(a, b) F[2, 1](c, a; a+b; -z)}"
            },
            {
              "type": "text",
              "value": ". "
            },
            {
              "type": "inlineMath",
              "value": "F[2, 1]"
            },
            {
              "type": "text",
              "value": " is the Gauss hypergeometric function "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "scipy.special.hyp2f1"
            },
            {
              "type": "text",
              "value": "."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "gausshyper"
            },
            {
              "type": "text",
              "value": " takes "
            },
            {
              "type": "inlineMath",
              "value": "a"
            },
            {
              "type": "text",
              "value": ", "
            },
            {
              "type": "inlineMath",
              "value": "b"
            },
            {
              "type": "text",
              "value": ", "
            },
            {
              "type": "inlineMath",
              "value": "c"
            },
            {
              "type": "text",
              "value": " and "
            },
            {
              "type": "inlineMath",
              "value": "z"
            },
            {
              "type": "text",
              "value": " as shape parameters."
            }
          ],
