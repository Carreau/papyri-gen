        {
          "children": [
            {
              "type": "text",
              "value": "The probability density function for "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "gengamma"
            },
            {
              "type": "text",
              "value": " is ():"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "f(x, a, c) = \\frac{|c| x^{c a-1} \\exp(-x^c)}{\\Gamma(a)}"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "for "
            },
            {
              "type": "inlineMath",
              "value": "x \\ge 0"
            },
            {
              "type": "text",
              "value": ", "
            },
            {
              "type": "inlineMath",
              "value": "a > 0"
            },
            {
              "type": "text",
              "value": ", and "
            },
            {
              "type": "inlineMath",
              "value": "c \\ne 0"
            },
            {
              "type": "text",
              "value": ". "
            },
            {
              "type": "inlineMath",
              "value": "\\Gamma"
            },
            {
              "type": "text",
              "value": " is the gamma function ("
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "scipy.special.gamma"
            },
            {
              "type": "text",
              "value": ")."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "gengamma"
            },
            {
              "type": "text",
              "value": " takes "
            },
            {
              "type": "inlineMath",
              "value": "a"
            },
            {
              "type": "text",
              "value": " and "
            },
            {
              "type": "inlineMath",
              "value": "c"
            },
            {
              "type": "text",
              "value": " as shape parameters."
            }
