    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "The probability density function for "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "halfgennorm"
            },
            {
              "type": "text",
              "value": " is:"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "f(x, \\beta) = \\frac{\\beta}{\\Gamma(1/\\beta)} \\exp(-|x|^\\beta)"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "for "
            },
            {
              "type": "inlineMath",
              "value": "x, \\beta > 0"
            },
            {
              "type": "text",
              "value": ". "
            },
            {
              "type": "inlineMath",
              "value": "\\Gamma"
            },
            {
              "type": "text",
              "value": " is the gamma function ("
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "scipy.special.gamma"
            },
            {
              "type": "text",
              "value": ")."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "halfgennorm"
            },
            {
              "type": "text",
              "value": " takes "
            },
            {
              "type": "inlineCode",
              "value": "beta"
            },
            {
              "type": "text",
              "value": " as a shape parameter for "
            },
            {
              "type": "inlineMath",
              "value": "\\beta"
            },
            {
              "type": "text",
              "value": ". For "
            },
            {
              "type": "inlineMath",
              "value": "\\beta = 1"
            },
            {
              "type": "text",
              "value": ", it is identical to an exponential distribution. For "
            },
            {
              "type": "inlineMath",
              "value": "\\beta = 2"
            },
            {
              "type": "text",
              "value": ", it is identical to a half normal distribution (with "
            },
            {
              "type": "inlineCode",
              "value": "scale=1/sqrt(2)"
            },
            {
              "type": "text",
              "value": ")."
            }
          ],
          "type": "paragraph"
        }
      ],
