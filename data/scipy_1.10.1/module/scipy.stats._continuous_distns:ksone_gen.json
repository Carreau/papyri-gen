    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "This is the distribution of the one-sided Kolmogorov-Smirnov (KS) statistics "
            },
            {
              "type": "inlineMath",
              "value": "D_n^+"
            },
            {
              "type": "text",
              "value": " and "
            },
            {
              "type": "inlineMath",
              "value": "D_n^-"
            },
            {
              "type": "text",
              "value": " for a finite sample size "
            },
            {
              "type": "inlineCode",
              "value": "n >= 1"
            },
            {
              "type": "text",
              "value": " (the shape parameter)."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "%(before_notes)s"
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
    "Methods": {
      "children": [],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "inlineMath",
              "value": "D_n^+"
            },
            {
              "type": "text",
              "value": " and "
            },
            {
              "type": "inlineMath",
              "value": "D_n^-"
            },
            {
              "type": "text",
              "value": " are given by"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "D_n^+ &= \\text{sup}_x (F_n(x) - F(x)),\\\\\nD_n^- &= \\text{sup}_x (F(x) - F_n(x)),\\\\"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "where "
            },
            {
              "type": "inlineMath",
              "value": "F"
            },
            {
              "type": "text",
              "value": " is a continuous CDF and "
            },
            {
              "type": "inlineMath",
              "value": "F_n"
            },
            {
              "type": "text",
              "value": " is an empirical CDF. "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "ksone"
            },
            {
              "type": "text",
              "value": " describes the distribution under the null hypothesis of the KS test that the empirical CDF corresponds to "
            },
            {
              "type": "inlineMath",
              "value": "n"
            },
            {
              "type": "text",
              "value": " i.i.d. random variates with CDF "
            },
            {
              "type": "inlineMath",
              "value": "F"
            },
            {
              "type": "text",
              "value": "."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "%(after_notes)s"
            }
          ],
          "type": "paragraph"
        }
      ],
