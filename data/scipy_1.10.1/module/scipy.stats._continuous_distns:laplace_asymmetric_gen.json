    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "The probability density function for "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "laplace_asymmetric"
            },
            {
              "type": "text",
              "value": " is"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "f(x, \\kappa) &= \\frac{1}{\\kappa+\\kappa^{-1}}\\exp(-x\\kappa),\\quad x\\ge0\\\\\n             &= \\frac{1}{\\kappa+\\kappa^{-1}}\\exp(x/\\kappa),\\quad x<0\\\\"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "for "
            },
            {
              "type": "inlineMath",
              "value": "-\\infty < x < \\infty"
            },
            {
              "type": "text",
              "value": ", "
            },
            {
              "type": "inlineMath",
              "value": "\\kappa > 0"
            },
            {
              "type": "text",
              "value": "."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "laplace_asymmetric"
            },
            {
              "type": "text",
              "value": " takes "
            },
            {
              "type": "inlineCode",
              "value": "kappa"
            },
            {
              "type": "text",
              "value": " as a shape parameter for "
            },
            {
              "type": "inlineMath",
              "value": "\\kappa"
            },
            {
              "type": "text",
              "value": ". For "
            },
            {
              "type": "inlineMath",
              "value": "\\kappa = 1"
            },
            {
              "type": "text",
              "value": ", it is identical to a Laplace distribution."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "%(after_notes)s"
            }
          ],
          "type": "paragraph"
        }
      ],
