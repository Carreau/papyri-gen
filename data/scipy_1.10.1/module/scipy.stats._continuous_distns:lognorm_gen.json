    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "The probability density function for "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "lognorm"
            },
            {
              "type": "text",
              "value": " is:"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "f(x, s) = \\frac{1}{s x \\sqrt{2\\pi}}\n          \\exp\\left(-\\frac{\\log^2(x)}{2s^2}\\right)"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "for "
            },
            {
              "type": "inlineMath",
              "value": "x > 0"
            },
            {
              "type": "text",
              "value": ", "
            },
            {
              "type": "inlineMath",
              "value": "s > 0"
            },
            {
              "type": "text",
              "value": "."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "lognorm"
            },
            {
              "type": "text",
              "value": " takes "
            },
            {
              "type": "inlineCode",
              "value": "s"
            },
            {
              "type": "text",
              "value": " as a shape parameter for "
            },
            {
              "type": "inlineMath",
              "value": "s"
            },
            {
              "type": "text",
              "value": "."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "%(after_notes)s"
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "Suppose a normally distributed random variable "
            },
            {
              "type": "inlineCode",
              "value": "X"
            },
            {
              "type": "text",
              "value": " has  mean "
            },
            {
              "type": "inlineCode",
              "value": "mu"
            },
            {
              "type": "text",
              "value": " and standard deviation "
            },
            {
              "type": "inlineCode",
              "value": "sigma"
            },
            {
              "type": "text",
              "value": ". Then "
            },
            {
              "type": "inlineCode",
              "value": "Y = exp(X)"
            },
            {
              "type": "text",
              "value": " is lognormally distributed with "
            },
            {
              "type": "inlineCode",
              "value": "s = sigma"
            },
            {
              "type": "text",
              "value": " and "
            },
            {
              "type": "inlineCode",
              "value": "scale = exp(mu)"
            },
            {
              "type": "text",
              "value": "."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
