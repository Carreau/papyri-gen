    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "The probability density function for "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "pearson3"
            },
            {
              "type": "text",
              "value": " is:"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "f(x, \\kappa) = \\frac{|\\beta|}{\\Gamma(\\alpha)}\n               (\\beta (x - \\zeta))^{\\alpha - 1}\n               \\exp(-\\beta (x - \\zeta))"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "where:"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "\\beta = \\frac{2}{\\kappa}\n\n\\alpha = \\beta^2 = \\frac{4}{\\kappa^2}\n\n\\zeta = -\\frac{\\alpha}{\\beta} = -\\beta"
        },
        {
          "children": [
            {
              "type": "inlineMath",
              "value": "\\Gamma"
            },
            {
              "type": "text",
              "value": " is the gamma function ("
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "scipy.special.gamma"
            },
            {
              "type": "text",
              "value": "). Pass the skew "
            },
            {
              "type": "inlineMath",
              "value": "\\kappa"
            },
            {
              "type": "text",
              "value": " into "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "pearson3"
            },
            {
              "type": "text",
              "value": " as the shape parameter "
            },
            {
              "type": "inlineCode",
              "value": "skew"
            },
            {
              "type": "text",
              "value": "."
            }
          ],
          "type": "paragraph"
        },
