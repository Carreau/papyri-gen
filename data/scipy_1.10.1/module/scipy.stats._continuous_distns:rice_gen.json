    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "The probability density function for "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "rice"
            },
            {
              "type": "text",
              "value": " is:"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "f(x, b) = x \\exp(- \\frac{x^2 + b^2}{2}) I_0(x b)"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "for "
            },
            {
              "type": "inlineMath",
              "value": "x >= 0"
            },
            {
              "type": "text",
              "value": ", "
            },
            {
              "type": "inlineMath",
              "value": "b > 0"
            },
            {
              "type": "text",
              "value": ". "
            },
            {
              "type": "inlineMath",
              "value": "I_0"
            },
            {
              "type": "text",
              "value": " is the modified Bessel function of order zero ("
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "scipy.special.i0"
            },
            {
              "type": "text",
              "value": ")."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "rice"
            },
            {
              "type": "text",
              "value": " takes "
            },
            {
              "type": "inlineCode",
              "value": "b"
            },
            {
              "type": "text",
              "value": " as a shape parameter for "
            },
            {
              "type": "inlineMath",
              "value": "b"
            },
            {
              "type": "text",
              "value": "."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "%(after_notes)s"
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "The Rice distribution describes the length, "
            },
            {
              "type": "inlineMath",
              "value": "r"
            },
            {
              "type": "text",
              "value": ", of a 2-D vector with components "
            },
            {
              "type": "inlineMath",
              "value": "(U+u, V+v)"
            },
            {
              "type": "text",
              "value": ", where "
            },
            {
              "type": "inlineMath",
              "value": "U, V"
            },
            {
              "type": "text",
              "value": " are constant, "
            },
            {
              "type": "inlineMath",
              "value": "u,\nv"
            },
            {
              "type": "text",
              "value": " are independent Gaussian random variables with standard deviation "
            },
            {
              "type": "inlineMath",
              "value": "s"
            },
            {
              "type": "text",
              "value": ".  Let "
            },
            {
              "type": "inlineMath",
              "value": "R = \\sqrt{U^2 + V^2}"
            },
            {
              "type": "text",
              "value": ". Then the pdf of "
            },
            {
              "type": "inlineMath",
              "value": "r"
            },
            {
              "type": "text",
              "value": " is "
            },
            {
              "type": "inlineCode",
              "value": "rice.pdf(x, R/s, scale=s)"
            },
            {
              "type": "text",
              "value": "."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "%(example)s"
            }
          ],
          "type": "paragraph"
        }
      ],
