    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "This distribution is the normal distribution centered on "
            },
            {
              "type": "inlineCode",
              "value": "loc"
            },
            {
              "type": "text",
              "value": " (default 0), with standard deviation "
            },
            {
              "type": "inlineCode",
              "value": "scale"
            },
            {
              "type": "text",
              "value": " (default 1), and clipped at "
            },
            {
              "type": "inlineCode",
              "value": "a"
            },
            {
              "type": "text",
              "value": ", "
            },
            {
              "type": "inlineCode",
              "value": "b"
            },
            {
              "type": "text",
              "value": " standard deviations to the left, right (respectively) from "
            },
            {
              "type": "inlineCode",
              "value": "loc"
            },
            {
              "type": "text",
              "value": ". If "
            },
            {
              "type": "inlineCode",
              "value": "myclip_a"
            },
            {
              "type": "text",
              "value": " and "
            },
            {
              "type": "inlineCode",
              "value": "myclip_b"
            },
            {
              "type": "text",
              "value": " are clip values in the sample space (as opposed to the number of standard deviations) then they can be converted to the required form according to      "
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "code",
          "value": "a, b = (myclip_a - loc) / scale, (myclip_b - loc) / scale"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "%(example)s"
            }
          ],
          "type": "paragraph"
        }
