    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "The probability density function for "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "truncweibull_min"
            },
            {
              "type": "text",
              "value": " is:"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "f(x, a, b, c) = \\frac{c x^{c-1} \\exp(-x^c)}{\\exp(-a^c) - \\exp(-b^c)}"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "for "
            },
            {
              "type": "inlineMath",
              "value": "a < x <= b"
            },
            {
              "type": "text",
              "value": ", "
            },
            {
              "type": "inlineMath",
              "value": "0 \\le a < b"
            },
            {
              "type": "text",
              "value": " and "
            },
            {
              "type": "inlineMath",
              "value": "c > 0"
            },
            {
              "type": "text",
              "value": "."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "truncweibull_min"
            },
            {
              "type": "text",
              "value": " takes "
            },
            {
              "type": "inlineMath",
              "value": "a"
            },
            {
              "type": "text",
              "value": ", "
            },
            {
              "type": "inlineMath",
              "value": "b"
            },
            {
              "type": "text",
              "value": ", and "
            },
            {
              "type": "inlineMath",
              "value": "c"
            },
            {
              "type": "text",
              "value": " as shape parameters."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "Notice that the truncation values, "
            },
            {
              "type": "inlineMath",
              "value": "a"
            },
            {
              "type": "text",
              "value": " and "
            },
            {
              "type": "inlineMath",
              "value": "b"
            },
            {
              "type": "text",
              "value": ", are defined in standardized form:"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "a = (u_l - loc)/scale\nb = (u_r - loc)/scale"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "where "
            },
            {
              "type": "inlineMath",
              "value": "u_l"
            },
            {
              "type": "text",
              "value": " and "
            },
            {
              "type": "inlineMath",
              "value": "u_r"
            },
            {
              "type": "text",
              "value": " are the specific left and right truncation values, respectively. In other words, the support of the distribution becomes "
            },
            {
              "type": "inlineMath",
              "value": "(a*scale + loc) < x <= (b*scale + loc)"
            },
            {
              "type": "text",
              "value": " when "
            },
            {
              "type": "inlineMath",
              "value": "loc"
            },
            {
              "type": "text",
              "value": " and/or "
            },
            {
              "type": "inlineMath",
              "value": "scale"
            },
            {
              "type": "text",
              "value": " are provided."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "%(after_notes)s"
            }
          ],
          "type": "paragraph"
        }
