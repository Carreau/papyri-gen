      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "The probability mass function for "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "binom"
            },
            {
              "type": "text",
              "value": " is:"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "f(k) = \\binom{n}{k} p^k (1-p)^{n-k}"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "for "
            },
            {
              "type": "inlineMath",
              "value": "k \\in \\{0, 1, \\dots, n\\}"
            },
            {
              "type": "text",
              "value": ", "
            },
            {
              "type": "inlineMath",
              "value": "0 \\leq p \\leq 1"
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "binom"
            },
            {
              "type": "text",
              "value": " takes "
            },
            {
              "type": "inlineMath",
              "value": "n"
            },
            {
              "type": "text",
              "value": " and "
            },
            {
              "type": "inlineMath",
              "value": "p"
            },
            {
              "type": "text",
              "value": " as shape parameters, where "
            },
            {
              "type": "inlineMath",
              "value": "p"
            },
            {
              "type": "text",
              "value": " is the probability of a single success and "
            },
            {
              "type": "inlineMath",
              "value": "1-p"
            },
            {
              "type": "text",
              "value": " is the probability of a single failure."
            }
          ],
          "type": "paragraph"
        },
