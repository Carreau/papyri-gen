    "Extended Summary": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "This is the two-sample version of the Cram\u00e9r-von Mises test (): for two independent samples "
            },
            {
              "type": "inlineMath",
              "value": "X_1, ..., X_n"
            },
            {
              "type": "text",
              "value": " and "
            },
            {
              "type": "inlineMath",
              "value": "Y_1, ..., Y_m"
            },
            {
              "type": "text",
              "value": ", the null hypothesis is that the samples come from the same (unspecified) continuous distribution."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
    "Methods": {
      "children": [],
