    "Notes": {
      "children": [
        {
          "children": [
            {
              "type": "text",
              "value": "Let:"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "X_1 \\sim \\mbox{Poisson}(\\mathtt{n1}\\lambda_1)"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "be a random variable independent of"
            }
          ],
          "type": "paragraph"
        },
        {
          "type": "math",
          "value": "X_2  \\sim \\mbox{Poisson}(\\mathtt{n2}\\lambda_2)"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "and let "
            },
            {
              "type": "inlineCode",
              "value": "k1"
            },
            {
              "type": "text",
              "value": " and "
            },
            {
              "type": "inlineCode",
              "value": "k2"
            },
            {
              "type": "text",
              "value": " be the observed values of "
