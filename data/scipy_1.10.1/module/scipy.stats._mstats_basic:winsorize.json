        {
          "children": [
            {
              "type": "text",
              "value": "This function is applied to reduce the effect of possibly spurious outliers by limiting the extreme values."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
