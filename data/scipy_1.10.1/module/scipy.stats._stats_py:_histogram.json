        {
          "children": [
            {
              "type": "text",
              "value": "This histogram is based on numpy's histogram but has a larger range by default if default limits is not set."
            }
          ],
          "type": "paragraph"
        }
      ],
      "level": 0,
      "target": null,
      "title": null,
      "type": "Section"
    },
