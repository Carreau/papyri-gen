              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "The Kruskal-Wallis H statistic, corrected for ties."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "statistic",
              "type": "Param",
              "type_": "float"
            },
            {
              "desc": [
                {
                  "children": [
                    {
                      "type": "text",
                      "value": "The p-value for the test using the assumption that H has a chi square distribution. The p-value returned is the survival function of the chi square distribution evaluated at H."
                    }
                  ],
                  "type": "paragraph"
                }
              ],
              "param": "pvalue",
              "type": "Param",
              "type_": "float"
            }
