    "Extended Summary": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "This function computes the inverse of the 1-D *n*-point discrete Fourier transform computed by "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "fft"
                  ],
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ".  In other words, "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "ifft(fft(x))",
                    " ",
                    "==",
                    " ",
                    "x"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " to within numerical accuracy."
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "The input should be ordered in the same way as is returned by "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "fft"
                  ],
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ", i.e.,"
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "* "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "x[0]"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " should contain the zero frequency term, * "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "x[1",
                    ":",
                    "n//2]"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " should contain the positive-frequency terms, * "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "x[n//2",
                    " ",
                    "+",
                    " ",
                    "1",
                    ":",
                    "]"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " should contain the negative-frequency terms, in"
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "  increasing order starting from the most negative frequency."
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
