                {
                  "_line": ">>> from scipy import sparse",
                  "_number": 22,
                  "_offset": 0
                },
                {
                  "_line": ">>> n_rows, n_columns, density, sketch_n_rows = 15000, 100, 0.01, 200",
                  "_number": 23,
                  "_offset": 0
                },
                {
                  "_line": ">>> A = sparse.rand(n_rows, n_columns, density=density, format='csc')",
                  "_number": 24,
