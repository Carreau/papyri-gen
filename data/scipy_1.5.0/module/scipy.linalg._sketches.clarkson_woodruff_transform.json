                  "_number": 26,
                  "_offset": 0
                },
                {
                  "_line": ">>> D = np.random.randn(n_rows, n_columns)",
                  "_number": 27,
                  "_offset": 0
                },
                {
                  "_line": ">>> SA = linalg.clarkson_woodruff_transform(A, sketch_n_rows) # fastest",
                  "_number": 28,
