            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "Solves a standard or generalized eigenvalue problem for a complex Hermitian or real symmetric matrix."
                }
              }
            ],
            "inner": []
          }
        }
      ],
      "title": null
    },
    "Extended Summary": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "Find eigenvalues array "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "w"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " of array "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "a"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ", where "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "b"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " is positive definite such that for every eigenvalue \u03bb (i-th entry of w) and its eigenvector vi (i-th column of v) satisfies::"
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "BlockVerbatim",
          "data": {
            "lines": {
              "_lines": [
                {
                  "_line": "                  a @ vi = \u03bb * b @ vi",
                  "_number": 4,
                  "_offset": 4
                },
                {
                  "_line": "    vi.conj().T @ a @ vi = \u03bb",
                  "_number": 5,
                  "_offset": 4
                },
                {
                  "_line": "    vi.conj().T @ b @ vi = 1",
                  "_number": 6,
                  "_offset": 4
                },
                {
                  "_line": "",
                  "_number": 7,
                  "_offset": 4
                }
              ]
            }
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "In the standard problem, b is assumed to be the identity matrix."
                }
              }
            ],
            "inner": []
          }
        }
      ],
      "title": null
    },
