                  "_line": "                n",
                  "_number": 3,
                  "_offset": 2
                },
                {
                  "_line": "  y_i = sqrt(sum (x[i]-b[i])**2)",
                  "_number": 4,
                  "_offset": 2
                },
                {
                  "_line": "                i",
                  "_number": 5,
                  "_offset": 2
                },
                {
                  "_line": "",
