        },
        {
          "type": "Param",
          "data": {
            "param": "norm",
            "type_": "{'phase', 'delay', 'mag'}, optional",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "Frequency normalization:"
                      }
                    }
                  ],
                  "inner": []
                }
              },
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "`"
                      }
                    },
                    {
                      "type": "Directive",
                      "data": {
                        "value": [
                          "phase"
                        ],
                        "domain": null,
                        "role": null
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": "`"
                      }
                    }
                  ],
                  "inner": []
                }
              },
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "    The filter is normalized such that the phase response reaches its     midpoint at an angular (e.g., rad/s) cutoff frequency of 1. This     happens for both low-pass and high-pass filters, so this is the     \"phase-matched\" case. [6]_"
                      }
                    }
                  ],
                  "inner": []
                }
              },
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "    The magnitude response asymptotes are the same as a Butterworth     filter of the same order with a cutoff of "
                      }
                    },
                    {
                      "type": "Directive",
                      "data": {
                        "value": [
                          "Wn"
                        ],
                        "domain": null,
                        "role": null
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": "."
                      }
                    }
                  ],
                  "inner": []
                }
              },
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "    This is the default, and matches MATLAB's implementation."
                      }
                    }
                  ],
                  "inner": []
                }
              },
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "`"
                      }
                    },
                    {
                      "type": "Directive",
                      "data": {
                        "value": [
                          "delay"
                        ],
                        "domain": null,
                        "role": null
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": "`"
                      }
                    }
                  ],
                  "inner": []
                }
              },
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "    The filter is normalized such that the group delay in the passband     is 1 (e.g., 1 second). This is the \"natural\" type obtained by     solving Bessel polynomials"
                      }
                    }
                  ],
                  "inner": []
                }
              },
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "`"
                      }
                    },
                    {
                      "type": "Directive",
                      "data": {
                        "value": [
                          "mag"
                        ],
                        "domain": null,
                        "role": null
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": "`"
                      }
                    }
                  ],
                  "inner": []
                }
              },
              {
                "type": "Paragraph",
                "data": {
                  "inline": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "    The filter is normalized such that the gain magnitude is -3 dB at     angular frequency 1. This is called \"frequency normalization\" by     Bond. [1]_"
                      }
                    }
                  ],
                  "inner": []
                }
              },
              {
