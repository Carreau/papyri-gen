                    {
                      "type": "Directive",
                      "data": {
                        "value": [
                          "dlti"
                        ],
                        "domain": null,
                        "role": null
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": ")     * 3: (num, den, dt)     * 4: (zeros, poles, gain, dt)     * 5: (A, B, C, D, dt)"
