                "lines": {
                  "_lines": [
                    {
                      "_line": "This can be instantiated in several ways:",
                      "_number": 0,
                      "_offset": 0
                    }
                  ]
                },
                "wh": {
                  "_lines": []
                },
                "ind": {
                  "_lines": [
                    {
                      "_line": "    bsr_matrix(D, [blocksize=(R,C)])",
                      "_number": 1,
                      "_offset": 4
                    },
                    {
                      "_line": "        where D is a dense matrix or 2-D ndarray.",
                      "_number": 2,
                      "_offset": 4
                    },
                    {
                      "_line": "",
                      "_number": 3,
                      "_offset": 4
                    },
                    {
                      "_line": "    bsr_matrix(S, [blocksize=(R,C)])",
                      "_number": 4,
                      "_offset": 4
                    },
                    {
                      "_line": "        with another sparse matrix S (equivalent to S.tobsr())",
                      "_number": 5,
                      "_offset": 4
                    },
                    {
                      "_line": "",
                      "_number": 6,
                      "_offset": 4
                    },
                    {
                      "_line": "    bsr_matrix((M, N), [blocksize=(R,C), dtype])",
                      "_number": 7,
                      "_offset": 4
                    },
                    {
                      "_line": "        to construct an empty matrix with shape (M, N)",
                      "_number": 8,
                      "_offset": 4
                    },
                    {
                      "_line": "        dtype is optional, defaulting to dtype='d'.",
                      "_number": 9,
                      "_offset": 4
                    },
                    {
                      "_line": "",
                      "_number": 10,
                      "_offset": 4
                    },
                    {
                      "_line": "    bsr_matrix((data, ij), [blocksize=(R,C), shape=(M, N)])",
                      "_number": 11,
                      "_offset": 4
                    },
                    {
                      "_line": "        where ``data`` and ``ij`` satisfy ``a[ij[0, k], ij[1, k]] = data[k]``",
                      "_number": 12,
                      "_offset": 4
                    },
                    {
                      "_line": "",
                      "_number": 13,
                      "_offset": 4
                    },
                    {
                      "_line": "    bsr_matrix((data, indices, indptr), [shape=(M, N)])",
                      "_number": 14,
                      "_offset": 4
                    },
                    {
                      "_line": "        is the standard BSR representation where the block column",
                      "_number": 15,
                      "_offset": 4
                    },
                    {
                      "_line": "        indices for row i are stored in ``indices[indptr[i]:indptr[i+1]]``",
                      "_number": 16,
                      "_offset": 4
                    },
                    {
                      "_line": "        and their corresponding block values are stored in",
                      "_number": 17,
                      "_offset": 4
                    },
                    {
                      "_line": "        ``data[ indptr[i]: indptr[i+1] ]``. If the shape parameter is not",
                      "_number": 18,
                      "_offset": 4
                    },
                    {
                      "_line": "        supplied, the matrix dimensions are inferred from the index arrays.",
                      "_number": 19,
                      "_offset": 4
                    }
                  ]
                },
