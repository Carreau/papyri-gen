    "Notes": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "The Yeo-Johnson transform is given by::"
                }
              }
            ],
            "inner": []
          }
        },
        {
          "type": "BlockVerbatim",
          "data": {
            "lines": {
              "_lines": [
                {
                  "_line": "    y = ((x + 1)**lmbda - 1) / lmbda,                for x >= 0, lmbda != 0",
                  "_number": 2,
                  "_offset": 4
                },
                {
                  "_line": "        log(x + 1),                                  for x >= 0, lmbda = 0",
                  "_number": 3,
                  "_offset": 4
                },
                {
                  "_line": "        -((-x + 1)**(2 - lmbda) - 1) / (2 - lmbda),  for x < 0, lmbda != 2",
                  "_number": 4,
                  "_offset": 4
                },
                {
                  "_line": "        -log(-x + 1),                                for x < 0, lmbda = 2",
                  "_number": 5,
                  "_offset": 4
                },
                {
                  "_line": "",
                  "_number": 6,
                  "_offset": 4
                }
              ]
            }
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "inline": [
              {
                "type": "Words",
                "data": {
                  "value": "Unlike "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "boxcox"
                  ],
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ", "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "yeojohnson"
                  ],
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " does not require the input data to be positive."
                }
              }
            ],
