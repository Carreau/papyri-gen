            "desc": [
              {
                "type": "DefList",
                "data": {
                  "children": [
                    {
                      "lines": {
                        "_lines": [
                          {
                            "_line": "'avg' : Average two results:1) use the first N-2 intervals with",
                            "_number": 0,
                            "_offset": 0
