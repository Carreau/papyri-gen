                        "children": [
                          {
                            "type": "Words",
                            "data": {
                              "value": "'last' : Use Simpson's rule for the last N-2 intervals with a"
                            }
                          }
                        ]
                      },
                      "dd": {
                        "children": [
                          {
                            "type": "Words",
                            "data": {
                              "value": "trapezoidal rule on the first interval."
