    "Notes": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "See splev for evaluation of the spline and its derivatives. Uses the FORTRAN routine curfit from FITPACK."
                }
              }
            ]
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "The user is responsible for assuring that the values of *x* are unique. Otherwise, *splrep* will not return sensible results."
                }
              }
            ]
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "If provided, knots "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "t"
                  ],
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " must satisfy the Schoenberg-Whitney conditions, i.e., there must be a subset of data points "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "x[j]"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " such that "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "t[j]",
                    " ",
                    "<",
                    " ",
                    "x[j]",
                    " ",
                    "<",
                    " ",
                    "t[j+k+1]"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ", for "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "j=0,",
                    " ",
                    "1,...,n-k-2"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": "."
                }
              }
            ]
          }
        }
      ]
    },
    "Warnings": {
      "children": []
    }
