                    " ",
                    "z',",
                    " ",
                    "...)"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " is evaluated by first computing the interval indices "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "i"
                  ],
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " such that::"
                }
              }
            ]
          }
        },
        {
          "type": "BlockVerbatim",
          "data": {
            "lines": {
              "_lines": [
                {
                  "_line": "    x[0][i[0]] <= x' < x[0][i[0]+1]",
                  "_number": 3,
                  "_offset": 4
                },
                {
                  "_line": "    x[1][i[1]] <= y' < x[1][i[1]+1]",
                  "_number": 4,
                  "_offset": 4
                },
                {
                  "_line": "    ...",
                  "_number": 5,
                  "_offset": 4
                },
                {
                  "_line": "",
                  "_number": 6,
                  "_offset": 4
                }
              ]
            }
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "and then computing::"
                }
              }
            ]
          }
        },
        {
          "type": "BlockVerbatim",
          "data": {
            "lines": {
              "_lines": [
                {
                  "_line": "    S = sum(c[k0-m0-1,...,kn-mn-1,i[0],...,i[n]]",
                  "_number": 9,
                  "_offset": 4
                },
                {
                  "_line": "            * (xp[0] - x[0][i[0]])**m0",
                  "_number": 10,
                  "_offset": 4
                },
                {
                  "_line": "            * ...",
                  "_number": 11,
                  "_offset": 4
                },
                {
                  "_line": "            * (xp[n] - x[n][i[n]])**mn",
                  "_number": 12,
                  "_offset": 4
                },
                {
                  "_line": "            for m0 in range(k[0]+1)",
                  "_number": 13,
                  "_offset": 4
                },
                {
                  "_line": "            ...",
                  "_number": 14,
                  "_offset": 4
                },
                {
                  "_line": "            for mn in range(k[n]+1))",
                  "_number": 15,
                  "_offset": 4
                },
                {
                  "_line": "",
                  "_number": 16,
                  "_offset": 4
                }
              ]
            }
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "where "
                }
