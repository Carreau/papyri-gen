          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "On exit, "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "(ALPHAR(j)",
                    " ",
                    "+",
                    " ",
                    "ALPHAI(j)",
                    "*",
                    "i)/BETA(j),",
                    " ",
                    "j=1,...,N"
                  ]
