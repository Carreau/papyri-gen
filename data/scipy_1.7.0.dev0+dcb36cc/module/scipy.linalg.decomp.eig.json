          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "Solve an ordinary or generalized eigenvalue problem of a square matrix."
                }
              }
            ]
          }
        }
      ]
    },
    "Extended Summary": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "Find eigenvalues w and right or left eigenvectors of a general matrix::"
                }
              }
            ]
          }
        },
        {
          "type": "BlockVerbatim",
          "data": {
            "lines": {
              "_lines": [
                {
                  "_line": "    a   vr[:,i] = w[i]        b   vr[:,i]",
                  "_number": 2,
                  "_offset": 4
                },
                {
                  "_line": "    a.H vl[:,i] = w[i].conj() b.H vl[:,i]",
                  "_number": 3,
                  "_offset": 4
                },
                {
                  "_line": "",
                  "_number": 4,
                  "_offset": 4
                }
              ]
            }
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "where "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    ".H"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " is the Hermitian conjugation."
                }
              }
            ]
          }
        }
      ]
    },
