                  ]
                }
              },
              {
                "type": "BlockVerbatim",
                "data": {
                  "lines": {
                    "_lines": [
                      {
                        "_line": "    w[1,i] a vr[:,i] = w[0,i] b vr[:,i]",
                        "_number": 3,
                        "_offset": 4
                      },
                      {
                        "_line": "",
                        "_number": 4,
                        "_offset": 4
                      }
