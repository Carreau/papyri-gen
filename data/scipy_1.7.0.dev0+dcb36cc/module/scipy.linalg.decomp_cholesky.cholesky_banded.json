{
  "_content": {
    "Summary": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "Cholesky decompose a banded Hermitian positive-definite matrix"
                }
              }
            ]
          }
        }
      ]
    },
    "Extended Summary": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "The matrix a is stored in ab either in lower-diagonal or upper- diagonal ordered form::"
                }
              }
            ]
          }
        },
        {
          "type": "BlockVerbatim",
          "data": {
            "lines": {
              "_lines": [
                {
                  "_line": "    ab[u + i - j, j] == a[i,j]        (if upper form; i <= j)",
                  "_number": 3,
                  "_offset": 4
                },
                {
                  "_line": "    ab[    i - j, j] == a[i,j]        (if lower form; i >= j)",
                  "_number": 4,
                  "_offset": 4
                },
                {
                  "_line": "",
                  "_number": 5,
                  "_offset": 4
                }
              ]
            }
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "Example of ab (shape of a is (6,6), u=2)::"
                }
              }
            ]
          }
        },
        {
          "type": "BlockVerbatim",
          "data": {
            "lines": {
              "_lines": [
                {
                  "_line": "    upper form:",
                  "_number": 8,
                  "_offset": 4
                },
                {
                  "_line": "    *   *   a02 a13 a24 a35",
                  "_number": 9,
                  "_offset": 4
                },
                {
                  "_line": "    *   a01 a12 a23 a34 a45",
                  "_number": 10,
                  "_offset": 4
                },
                {
                  "_line": "    a00 a11 a22 a33 a44 a55",
                  "_number": 11,
                  "_offset": 4
                },
                {
                  "_line": "",
                  "_number": 12,
                  "_offset": 4
                },
                {
                  "_line": "    lower form:",
                  "_number": 13,
                  "_offset": 4
                },
                {
                  "_line": "    a00 a11 a22 a33 a44 a55",
                  "_number": 14,
                  "_offset": 4
                },
                {
                  "_line": "    a10 a21 a32 a43 a54 *",
                  "_number": 15,
                  "_offset": 4
                },
                {
                  "_line": "    a20 a31 a42 a53 *   *",
                  "_number": 16,
                  "_offset": 4
                }
              ]
            }
          }
        }
      ]
    },
    "Parameters": {
      "children": [
        {
          "type": "Param",
          "data": {
            "param": "ab",
            "type_": "(u + 1, M) array_like",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "children": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "Banded matrix"
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "type": "Param",
          "data": {
            "param": "overwrite_ab",
            "type_": "bool, optional",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "children": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "Discard data in ab (may enhance performance)"
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "type": "Param",
          "data": {
            "param": "lower",
            "type_": "bool, optional",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "children": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "Is the matrix in the lower form. (Default is upper form)"
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "type": "Param",
          "data": {
            "param": "check_finite",
            "type_": "bool, optional",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "children": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "Whether to check that the input matrix contains only finite numbers. Disabling may give a performance gain, but may result in problems (crashes, non-termination) if the inputs do contain infinities or NaNs."
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      ]
    },
    "Returns": {
      "children": [
        {
          "type": "Param",
          "data": {
            "param": "c",
            "type_": "(u + 1, M) ndarray",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "children": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "Cholesky factorization of a, in the same banded format as ab"
                      }
                    }
                  ]
                }
              }
            ]
          }
        }
      ]
