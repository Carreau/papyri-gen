            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "By default, an order 8 Chebyshev type I filter is used. A 30 point FIR filter with Hamming window is used if "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "ftype"
                  ],
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " is 'fir'."
                }
              }
            ]
          }
        }
      ]
    },
