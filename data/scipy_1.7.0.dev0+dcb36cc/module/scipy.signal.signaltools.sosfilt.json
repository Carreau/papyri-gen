      "children": [
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "Filter a data sequence, "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "x"
                  ],
                  "domain": null,
                  "role": null
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": ", using a digital IIR filter defined by "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "sos"
                  ],
                  "domain": null,
                  "role": null
                }
              },
