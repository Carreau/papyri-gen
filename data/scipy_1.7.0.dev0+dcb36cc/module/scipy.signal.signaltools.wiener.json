          "type": "Param",
          "data": {
            "param": "mysize",
            "type_": "int or array_like, optional",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "children": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "A scalar or an N-length list giving the size of the Wiener filter window in each dimension.  Elements of mysize should be odd. If mysize is a scalar, then this scalar is used as the size in each dimension."
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "type": "Param",
          "data": {
            "param": "noise",
            "type_": "float, optional",
            "desc": [
