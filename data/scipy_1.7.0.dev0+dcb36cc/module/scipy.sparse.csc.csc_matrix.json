                "dd": {
                  "children": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "with another sparse matrix S (equivalent to S.tocsc()) "
                      }
                    }
                  ]
                }
              },
              {
                "lines": {
                  "_lines": [
                    {
                      "_line": "    csc_matrix((M, N), [dtype])",
                      "_number": 8,
                      "_offset": 4
                    }
                  ]
                },
                "wh": {
                  "_lines": []
                },
                "ind": {
                  "_lines": [
                    {
                      "_line": "        to construct an empty matrix with shape (M, N)",
                      "_number": 9,
                      "_offset": 4
                    },
                    {
                      "_line": "        dtype is optional, defaulting to dtype='d'.",
                      "_number": 10,
                      "_offset": 4
                    },
                    {
                      "_line": "",
                      "_number": 11,
                      "_offset": 4
                    }
                  ]
                },
                "dt": {
                  "children": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "csc_matrix((M, N), [dtype])"
                      }
                    }
                  ]
                },
                "dd": {
                  "children": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "to construct an empty matrix with shape (M, N) dtype is optional, defaulting to dtype='d'. "
                      }
                    }
                  ]
                }
              },
              {
                "lines": {
                  "_lines": [
                    {
                      "_line": "    csc_matrix((data, (row_ind, col_ind)), [shape=(M, N)])",
                      "_number": 12,
                      "_offset": 4
                    }
                  ]
                },
                "wh": {
                  "_lines": []
                },
                "ind": {
                  "_lines": [
                    {
                      "_line": "        where ``data``, ``row_ind`` and ``col_ind`` satisfy the",
                      "_number": 13,
                      "_offset": 4
                    },
                    {
                      "_line": "        relationship ``a[row_ind[k], col_ind[k]] = data[k]``.",
                      "_number": 14,
                      "_offset": 4
                    },
                    {
                      "_line": "",
                      "_number": 15,
                      "_offset": 4
                    }
                  ]
                },
                "dt": {
                  "children": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "csc_matrix((data, (row_ind, col_ind)), [shape=(M, N)])"
                      }
                    }
                  ]
                },
                "dd": {
                  "children": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "where "
                      }
                    },
                    {
                      "type": "Verbatim",
                      "data": {
                        "value": [
                          "data"
                        ]
                      }
                    },
                    {
                      "type": "Words",
                      "data": {
                        "value": ", "
