                "wh": {
                  "_lines": []
                },
                "ind": {
                  "_lines": [
                    {
                      "_line": "    - efficient arithmetic operations CSC + CSC, CSC * CSC, etc.",
                      "_number": 4,
                      "_offset": 0
                    },
                    {
                      "_line": "    - efficient column slicing",
                      "_number": 5,
                      "_offset": 0
                    },
                    {
