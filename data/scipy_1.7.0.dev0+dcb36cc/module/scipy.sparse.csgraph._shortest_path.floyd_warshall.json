          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "Compute the shortest path lengths using the Floyd-Warshall algorithm"
                }
              }
            ]
          }
        }
      ]
    },
    "Extended Summary": {
      "children": [
        {
          "type": "BlockDirective",
          "data": {
            "lines": {
              "_lines": [
                {
                  "_line": ".. versionadded:: 0.11.0",
