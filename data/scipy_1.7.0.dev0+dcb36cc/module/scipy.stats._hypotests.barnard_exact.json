                    "_offset": 0
                  }
                ]
              },
              "wh": {
                "_lines": []
              },
              "ind": {
                "_lines": [
                  {
                    "_line": "        Vaccine  Placebo",
                    "_number": 0,
                    "_offset": 0
                  },
                  {
                    "_line": "    Yes     7        12",
                    "_number": 1,
                    "_offset": 0
                  },
                  {
                    "_line": "    No      8        3",
                    "_number": 2,
                    "_offset": 0
                  }
                ]
              },
              "dt": {
