          "children": [
            {
              "type": "Words",
              "data": {
                "value": "Order statistics of the uniform distribution on the unit interval are marginally distributed according to beta distributions. The expectations of these order statistic are evenly spaced across the interval, but the distributions are skewed in a way that pushes the medians slightly towards the endpoints of the unit interval:"
              }
            }
          ]
        }
