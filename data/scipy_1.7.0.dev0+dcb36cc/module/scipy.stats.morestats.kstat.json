          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "Return the nth k-statistic (1<=n<=4 so far)."
                }
              }
            ]
          }
        }
      ]
    },
    "Extended Summary": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "The nth k-statistic k_n is the unique symmetric unbiased estimator of the nth cumulant kappa_n."
                }
              }
            ]
          }
        }
      ]
    },
