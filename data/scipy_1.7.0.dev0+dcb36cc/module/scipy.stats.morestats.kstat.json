    "Notes": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "For a sample size n, the first few k-statistics are given by:"
                }
              }
            ]
          }
        },
        {
          "type": "BlockDirective",
          "data": {
            "lines": {
              "_lines": [
                {
                  "_line": ".. math::",
                  "_number": 2,
                  "_offset": 0
                }
              ]
            },
            "wh": {
              "_lines": [
                {
                  "_line": "",
                  "_number": 3,
                  "_offset": 0
                }
              ]
            },
            "ind": {
              "_lines": [
                {
                  "_line": "    k_{1} = \\mu",
                  "_number": 4,
                  "_offset": 0
                },
                {
                  "_line": "    k_{2} = \\frac{n}{n-1} m_{2}",
                  "_number": 5,
                  "_offset": 0
                },
                {
                  "_line": "    k_{3} = \\frac{ n^{2} } {(n-1) (n-2)} m_{3}",
                  "_number": 6,
                  "_offset": 0
                },
                {
                  "_line": "    k_{4} = \\frac{ n^{2} [(n + 1)m_{4} - 3(n - 1) m^2_{2}]} {(n-1) (n-2) (n-3)}",
                  "_number": 7,
                  "_offset": 0
                },
                {
                  "_line": "",
                  "_number": 8,
                  "_offset": 0
                }
              ]
            },
            "directive_name": "math",
            "args0": [
              ""
            ],
            "inner": {
              "children": [
                {
                  "type": "Words",
                  "data": {
                    "value": "k_{1} = \\mu k_{2} = \\frac{n}{n-1} m_{2} k_{3} = \\frac{ n^{2} } {(n-1) (n-2)} m_{3} k_{4} = \\frac{ n^{2} [(n + 1)m_{4} - 3(n - 1) m^2_{2}]} {(n-1) (n-2) (n-3)} "
                  }
                }
              ]
            }
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "where "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "\\mu"
                  ],
                  "domain": null,
                  "role": "math"
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " is the sample mean, "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "m",
                    "_",
                    "2"
                  ],
                  "domain": null,
                  "role": "math"
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " is the sample variance, and "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "m",
                    "_",
                    "i"
                  ],
                  "domain": null,
                  "role": "math"
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " is the i-th sample central moment."
                }
              }
            ]
