          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "The formula used for the theoretical quantiles (horizontal axis of the probability plot) is Filliben's estimate::"
                }
              }
            ]
          }
        },
        {
          "type": "BlockVerbatim",
          "data": {
            "lines": {
              "_lines": [
                {
                  "_line": "    quantiles = dist.ppf(val), for",
                  "_number": 11,
                  "_offset": 4
                },
                {
                  "_line": "",
                  "_number": 12,
                  "_offset": 4
                },
                {
                  "_line": "            0.5**(1/n),                  for i = n",
                  "_number": 13,
                  "_offset": 4
                },
                {
                  "_line": "      val = (i - 0.3175) / (n + 0.365),  for i = 2, ..., n-1",
                  "_number": 14,
                  "_offset": 4
                },
                {
                  "_line": "            1 - 0.5**(1/n),              for i = 1",
                  "_number": 15,
                  "_offset": 4
                },
                {
                  "_line": "",
                  "_number": 16,
                  "_offset": 4
                }
              ]
            }
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "where "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "i"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " indicates the i-th ordered value and "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "n"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " is the total number of values."
                }
              }
            ]
          }
        }
      ]
