          "data": {
            "param": "x, y",
            "type_": "array_like",
            "desc": [
              {
                "type": "Paragraph",
                "data": {
                  "children": [
                    {
                      "type": "Words",
                      "data": {
                        "value": "Arrays of rankings, of the same shape. If arrays are not 1-D, they will be flattened to 1-D."
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "type": "Param",
          "data": {
            "param": "initial_lexsort",
