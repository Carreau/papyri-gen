    "Summary": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "Calculate the t-test on TWO RELATED samples of scores, a and b."
                }
              }
            ]
          }
        }
      ]
    },
    "Extended Summary": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "This is a two-sided test for the null hypothesis that 2 related or repeated samples have identical average (expected) values."
                }
              }
            ]
          }
        }
      ]
    },
    "Parameters": {
      "children": [
