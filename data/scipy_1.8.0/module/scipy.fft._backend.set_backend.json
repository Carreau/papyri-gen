          "data": {
            "children": [
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "The backend to use. Can either be a "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "str"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": " containing the name of a known backend {'scipy'} or an object that implements the uarray protocol."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "backend",
                "type_": "{object, 'scipy'}"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Whether to allow expensive conversions for the "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "x"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": " parameter. e.g., copying a NumPy array to the GPU for a CuPy backend. Implies "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "only"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": "."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "coerce",
                "type_": "bool, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "If only is "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "True"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": " and this backend returns "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "NotImplemented"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": ", then a BackendNotImplemented error will be raised immediately. Ignoring any lower priority backends."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "only",
                "type_": "bool, optional"
