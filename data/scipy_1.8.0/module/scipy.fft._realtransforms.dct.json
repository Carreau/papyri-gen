          "type": "Admonition"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "content": {
                    "value": "Type II"
                  }
                },
                "type": "Strong"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "There are several definitions of the DCT-II; we use the following (for "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "norm=\"backward\""
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ")"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "y_k = 2 \\sum_{n=0}^{N-1} x_n \\cos\\left(\\frac{\\pi k(2n+1)}{2N} \\right)"
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "If "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "orthogonalize=True"
                  ]
                },
