    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The observation equations are "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "A @ c = y"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ", and the normal equations are "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "A.T @ A @ c = A.T @ y"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ". This routine fills in the rhs and lhs for the latter."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The B-spline collocation matrix is defined as "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "A_{j,l} = B_l(x_j)"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ", so that row "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "j"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " contains all the B-splines which are non-zero at "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "x_j"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": "."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The normal eq matrix has at most "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "2k+1"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " bands and is constructed in the LAPACK symmetrix banded storage: "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "A[i, j] == ab[i-j, j]"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " with "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "i >= j"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": ". See the doctsring for "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "module",
                  "reference": {
                    "kind": "api",
                    "module": "scipy",
                    "path": "scipy.linalg._decomp_cholesky.cholesky_banded",
                    "version": "*"
                  },
                  "value": "scipy.linalg.cholesky_banded"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " for more info."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
