    "Extended Summary": {
      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "The result is a linear combination"
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "S ( x) = \\sum_j c_j B_j ( x; t) "
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "of the B-spline basis elements, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "B_j(x; t)"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": ", which minimizes"
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "\\sum_ { j} \\left ( w_j \\times ( S ( x_j) - y_j) \\right)^2 "
          },
          "type": "BlockMath"
        }
      ],
      "title": null
    },
