                      "children": [
                        {
                          "data": {
                            "value": "Sequence of length 4 specifying the boundary of the rectangular approximation domain.  By default, "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "bbox=[min(x), max(x), min(y), max(y)]"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": "."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "bbox",
                "type_": "array_like, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Degrees of the bivariate spline. Default is 3."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "kx, ky",
                "type_": "ints, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Positive smoothing factor defined for estimation condition: "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "sum((z[i]-f(x[i], y[i]))**2, axis=0) <= s"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": " where f is a spline function. Default is "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "s=0"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": ", which is for interpolation."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "s",
                "type_": "float, optional"
              }
            ]
