                      "type": "Link"
                    },
                    {
                      "data": {
                        "value": " is None, "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "value": [
                          "s = len(w)"
                        ]
                      },
                      "type": "Verbatim"
                    },
                    {
                      "data": {
                        "value": " which should be a good value if "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "value": [
                          "1/w[i]"
                        ]
                      },
                      "type": "Verbatim"
                    },
                    {
                      "data": {
                        "value": " is an estimate of the standard deviation of "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "value": [
                          "y[i]"
                        ]
                      },
                      "type": "Verbatim"
                    },
                    {
                      "data": {
                        "value": ". If 0, spline will interpolate through all data points. Default is None."
                      },
                      "type": "Words"
                    }
                  ],
                  "inner": []
                },
                "type": "Paragraph"
              }
            ],
            "param": "s",
            "type_": "float or None, optional"
