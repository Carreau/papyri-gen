                    },
                    {
                      "data": {
                        "value": [
                          "per != 0"
                        ]
                      },
                      "type": "Verbatim"
                    },
                    {
                      "data": {
                        "value": ") there must be either at least k interior knots t(j) satisfying "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "value": [
                          "t(k+1)<t(j)<=x"
                        ]
                      },
                      "type": "Verbatim"
                    },
                    {
                      "data": {
                        "value": " or at least k interior knots t(j) satisfying "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
