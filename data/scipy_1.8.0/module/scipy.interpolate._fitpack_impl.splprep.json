      "title": null
    },
    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Given a list of N rank-1 arrays, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "x",
                    "version": null
                  },
                  "value": "x"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": ", which represent a curve in N-dimensional space parametrized by "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "u",
                    "version": null
                  },
                  "value": "u"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": ", find a smooth approximating spline curve g("
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "u",
                    "version": null
                  },
                  "value": "u"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": "). Uses the FORTRAN routine parcur from FITPACK."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
    "Methods": {
      "children": [],
      "title": null
    },
    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "See "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "module",
                  "reference": {
                    "kind": "module",
                    "module": "scipy",
                    "path": "scipy.interpolate._fitpack_impl.splev",
                    "version": "1.8.0"
                  },
                  "value": "splev"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " for evaluation of the spline and its derivatives. The number of dimensions N must be smaller than 11."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
    "Other Parameters": {
      "children": [],
      "title": null
    },
    "Parameters": {
      "children": [
        {
          "data": {
            "children": [
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "A list of sample vector arrays representing the curve."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "x",
                "type_": "array_like"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Strictly positive rank-1 array of weights the same length as "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "domain": null,
                            "role": null,
                            "value": "x[0]"
                          },
                          "type": "Directive"
                        },
                        {
                          "data": {
                            "value": ". The weights are used in computing the weighted least-squares spline fit. If the errors in the "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "exists": true,
                            "kind": "local",
                            "reference": {
                              "kind": "local",
                              "module": null,
                              "path": "x",
                              "version": null
                            },
                            "value": "x"
                          },
                          "type": "Link"
                        },
                        {
                          "data": {
                            "value": " values have standard-deviation given by the vector d, then "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "exists": true,
                            "kind": "local",
                            "reference": {
                              "kind": "local",
                              "module": null,
                              "path": "w",
                              "version": null
                            },
                            "value": "w"
                          },
                          "type": "Link"
                        },
                        {
                          "data": {
                            "value": " should be 1/d. Default is "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "ones(len(x[0]))"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": "."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "w",
                "type_": "array_like, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "An array of parameter values. If not given, these values are calculated automatically as "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "M = len(x[0])"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": ", where"
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  },
                  {
                    "data": {
                      "value": [
                        "v[0] = 0",
                        "",
                        "v[i] = v[i-1] + distance(`x[i]`, `x[i-1]`)",
                        "",
                        "u[i] = v[i] / v[M-1]"
                      ]
                    },
                    "type": "BlockQuote"
                  }
                ],
                "param": "u",
                "type_": "array_like, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "The end-points of the parameters interval. Defaults to u[0] and u[-1]."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "ub, ue",
                "type_": "int, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Degree of the spline. Cubic splines are recommended. Even values of "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "exists": true,
                            "kind": "local",
                            "reference": {
                              "kind": "local",
                              "module": null,
                              "path": "k",
                              "version": null
                            },
                            "value": "k"
                          },
                          "type": "Link"
                        },
                        {
                          "data": {
                            "value": " should be avoided especially with a small s-value. "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "1 <= k <= 5"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": ", default is 3."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "k",
                "type_": "int, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "If task==0 (default), find t and c for a given smoothing factor, s. If task==1, find t and c for another value of the smoothing factor, s. There must have been a previous call with task=0 or task=1 for the same set of data. If task=-1 find the weighted least square spline for a given set of knots, t."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "task",
                "type_": "int, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "A smoothing condition. The amount of smoothness is determined by satisfying the conditions: "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "sum((w * (y - g))**2,axis=0) <= s"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": ", where g(x) is the smoothed interpolation of (x,y).  The user can use "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "exists": true,
                            "kind": "local",
                            "reference": {
                              "kind": "local",
                              "module": null,
                              "path": "s",
                              "version": null
                            },
                            "value": "s"
                          },
                          "type": "Link"
                        },
                        {
                          "data": {
                            "value": " to control the trade-off between closeness and smoothness of fit. Larger "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "exists": true,
                            "kind": "local",
                            "reference": {
                              "kind": "local",
                              "module": null,
                              "path": "s",
                              "version": null
                            },
                            "value": "s"
                          },
                          "type": "Link"
                        },
                        {
                          "data": {
                            "value": " means more smoothing while smaller values of "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "exists": true,
                            "kind": "local",
                            "reference": {
                              "kind": "local",
                              "module": null,
                              "path": "s",
                              "version": null
                            },
                            "value": "s"
                          },
                          "type": "Link"
                        },
                        {
                          "data": {
                            "value": " indicate less smoothing. Recommended values of "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "exists": true,
                            "kind": "local",
                            "reference": {
                              "kind": "local",
                              "module": null,
                              "path": "s",
                              "version": null
                            },
                            "value": "s"
                          },
                          "type": "Link"
                        },
                        {
                          "data": {
                            "value": " depend on the weights, w.  If the weights represent the inverse of the standard-deviation of y, then a good "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "exists": true,
                            "kind": "local",
                            "reference": {
                              "kind": "local",
                              "module": null,
                              "path": "s",
                              "version": null
                            },
                            "value": "s"
                          },
                          "type": "Link"
                        },
                        {
                          "data": {
                            "value": " value should be found in the range "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "(m-sqrt(2*m),m+sqrt(2*m))"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": ", where m is the number of data points in x, y, and w."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "s",
                "type_": "float, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "The knots needed for task=-1."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "t",
                "type_": "int, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "If non-zero, then return optional outputs."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "full_output",
                "type_": "int, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "An over-estimate of the total number of knots of the spline to help in determining the storage space. By default nest=m/2. Always large enough is nest=m+k+1."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "nest",
                "type_": "int, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "If non-zero, data points are considered periodic with period "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "x[m-1] - x[0]"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": " and a smooth periodic spline approximation is returned.  Values of "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "y[m-1]"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": " and "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "w[m-1]"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": " are not used."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "per",
                "type_": "int, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Non-zero to suppress messages. This parameter is deprecated; use standard Python warning filters instead."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "quiet",
                "type_": "int, optional"
              }
            ]
          },
          "type": "Parameters"
        }
