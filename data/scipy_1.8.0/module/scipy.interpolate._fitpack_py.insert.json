                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "A 1-D point at which to insert a new knot(s).  If "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "domain": null,
                            "role": null,
                            "value": "tck"
                          },
                          "type": "Directive"
                        },
                        {
                          "data": {
                            "value": " was returned from "
                          },
                          "type": "Words"
                        },
                        {
