    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Given the set of data points "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "(x[i], y[i])"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " determine a smooth spline approximation of degree k on the interval "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "xb <= x <= xe"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": "."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
    "Methods": {
      "children": [],
      "title": null
    },
    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "See "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "module",
                  "reference": {
                    "kind": "module",
                    "module": "scipy",
                    "path": "scipy.interpolate._fitpack_py.splev",
                    "version": "1.8.0"
                  },
                  "value": "splev"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " for evaluation of the spline and its derivatives. Uses the FORTRAN routine "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "curfit"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " from FITPACK."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The user is responsible for assuring that the values of "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "x",
                    "version": null
                  },
                  "value": "x"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " are unique. Otherwise, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "module",
                  "reference": {
                    "kind": "module",
                    "module": "scipy",
                    "path": "scipy.interpolate._fitpack_py.splrep",
                    "version": "1.8.0"
                  },
                  "value": "splrep"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " will not return sensible results."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "If provided, knots "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "t",
                    "version": null
                  },
                  "value": "t"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " must satisfy the Schoenberg-Whitney conditions, i.e., there must be a subset of data points "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "x[j]"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " such that "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "t[j] < x[j] < t[j+k+1]"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ", for "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "j=0, 1,...,n-k-2"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": "."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "This routine zero-pads the coefficients array "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "c"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " to have the same length as the array of knots "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "t"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " (the trailing "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "k + 1"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " coefficients are ignored by the evaluation routines, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "module",
                  "reference": {
                    "kind": "module",
                    "module": "scipy",
                    "path": "scipy.interpolate._fitpack_py.splev",
                    "version": "1.8.0"
                  },
                  "value": "splev"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "module",
                  "reference": {
                    "kind": "module",
                    "module": "scipy",
                    "path": "scipy.interpolate._bsplines.BSpline",
                    "version": "1.8.0"
                  },
                  "value": "BSpline"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": ".) This is in contrast with "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "module",
                  "reference": {
                    "kind": "module",
                    "module": "scipy",
                    "path": "scipy.interpolate._fitpack_py.splprep",
                    "version": "1.8.0"
                  },
                  "value": "splprep"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": ", which does not zero-pad the coefficients."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
    "Other Parameters": {
      "children": [],
      "title": null
    },
    "Parameters": {
      "children": [
        {
          "data": {
            "children": [
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "The data points defining a curve y = f(x)."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "x, y",
                "type_": "array_like"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Strictly positive rank-1 array of weights the same length as x and y. The weights are used in computing the weighted least-squares spline fit. If the errors in the y values have standard-deviation given by the vector d, then w should be 1/d. Default is ones(len(x))."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "w",
                "type_": "array_like, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "The interval to fit.  If None, these default to x[0] and x[-1] respectively."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "xb, xe",
                "type_": "float, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "The degree of the spline fit. It is recommended to use cubic splines. Even values of k should be avoided especially with small s values. 1 <= k <= 5"
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "k",
                "type_": "int, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "If task==0 find t and c for a given smoothing factor, s."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  },
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "If task==1 find t and c for another value of the smoothing factor, s. There must have been a previous call with task=0 or task=1 for the same set of data (t will be stored an used internally)"
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  },
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "If task=-1 find the weighted least square spline for a given set of knots, t. These should be interior knots as knots on the ends will be added automatically."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "task",
                "type_": "{1, 0, -1}, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "A smoothing condition. The amount of smoothness is determined by satisfying the conditions: sum((w * (y - g))**2,axis=0) <= s where g(x) is the smoothed interpolation of (x,y). The user can use s to control the tradeoff between closeness and smoothness of fit. Larger s means more smoothing while smaller values of s indicate less smoothing. Recommended values of s depend on the weights, w. If the weights represent the inverse of the standard-deviation of y, then a good s value should be found in the range (m-sqrt(2*m),m+sqrt(2*m)) where m is the number of datapoints in x, y, and w. default : s=m-sqrt(2*m) if weights are supplied. s = 0.0 (interpolating) if no weights are supplied."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "s",
                "type_": "float, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "The knots needed for task=-1. If given then task is automatically set to -1."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "t",
                "type_": "array_like, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "If non-zero, then return optional outputs."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "full_output",
                "type_": "bool, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "If non-zero, data points are considered periodic with period x[m-1] - x[0] and a smooth periodic spline approximation is returned. Values of y[m-1] and w[m-1] are not used."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "per",
                "type_": "bool, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Non-zero to suppress messages. This parameter is deprecated; use standard Python warning filters instead."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "quiet",
                "type_": "bool, optional"
              }
            ]
          },
          "type": "Parameters"
        }
      ],
