    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The Toeplitz matrix has constant diagonals, with c as its first column and r as its first row. If r is not given, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "r == conjugate(c)"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " is assumed."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
    "Methods": {
      "children": [],
      "title": null
    },
    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The solution is computed using Levinson-Durbin recursion, which is faster than generic least-squares methods, but can be less numerically stable."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
    "Other Parameters": {
      "children": [],
      "title": null
    },
    "Parameters": {
      "children": [
        {
          "data": {
            "children": [
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "The vector "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "c"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": ", or a tuple of arrays ("
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "c"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": ", "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "r"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": "). Whatever the actual shape of "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "c"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": ", it will be converted to a 1-D array. If not supplied, "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "r = conjugate(c)"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": " is assumed; in this case, if c[0] is real, the Toeplitz matrix is Hermitian. r[0] is ignored; the first row of the Toeplitz matrix is "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "[c[0], r[1:]]"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": ". Whatever the actual shape of "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "r"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": ", it will be converted to a 1-D array."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "c_or_cr",
                "type_": "array_like or tuple of (array_like, array_like)"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Right-hand side in "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
