    "Extended Summary": {
      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "Find eigenvalues w and right or left eigenvectors of a general matrix::      "
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "a   vr[:,i] = w[i]        b   vr[:,i]\na.H vl[:,i] = w[i].conj() b.H vl[:,i]"
          },
          "type": "BlockVerbatim"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    ".H"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " is the Hermitian conjugation."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
