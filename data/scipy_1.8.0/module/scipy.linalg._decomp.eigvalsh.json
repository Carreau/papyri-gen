                      "type": "Words"
                    },
                    {
                      "data": {
                        "value": [
                          "b"
                        ]
                      },
                      "type": "Verbatim"
                    },
                    {
                      "data": {
                        "value": " (may improve performance). Default is False."
                      },
                      "type": "Words"
                    }
                  ],
                  "inner": []
                },
                "type": "Paragraph"
              }
            ],
            "param": "overwrite_b",
            "type_": "bool, optional"
