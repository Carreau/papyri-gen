    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Returns a matrix containing the Cholesky decomposition, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "A = L L*"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " or "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "A = U* U"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " of a Hermitian positive-definite matrix "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "a",
                    "version": null
                  },
                  "value": "a"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": ". The return value can be directly used as the first parameter to cho_solve."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "children": [
                    {
                      "data": {
                        "value": "The returned matrix also contains random data in the entries not used by the Cholesky decomposition. If you need to zero these entries, use the function "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "domain": null,
                        "role": null,
                        "value": "cholesky"
                      },
                      "type": "Directive"
                    },
                    {
                      "data": {
                        "value": " instead."
                      },
                      "type": "Words"
                    }
                  ]
                },
                "type": "Paragraph"
              }
            ],
            "kind": "warning",
            "title": ""
          },
          "type": "Admonition"
        }
      ],
