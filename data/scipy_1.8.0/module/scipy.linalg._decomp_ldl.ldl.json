              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "This switches between the lower and upper triangular outer factors of the factorization. Lower triangular ("
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "lower=True"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": ") is the default."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "lower",
                "type_": "bool, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "For complex-valued arrays, this defines whether "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "A = A.conj().T"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": " or "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "A = A.T"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": " is assumed. For real-valued arrays, this switch has no effect."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "hermitian",
                "type_": "bool, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Allow overwriting data in "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "exists": true,
                            "kind": "local",
                            "reference": {
                              "kind": "local",
                              "module": null,
                              "path": "A",
                              "version": null
                            },
                            "value": "A"
                          },
                          "type": "Link"
                        },
                        {
                          "data": {
                            "value": " (may enhance performance). The default is False."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "overwrite_a",
                "type_": "bool, optional"
              },
              {
                "desc": [
