    "Extended Summary": {
      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "Calculate the decomposition "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "A = Q R"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " where Q is unitary/orthogonal and R upper triangular."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
    "Methods": {
      "children": [],
      "title": null
    },
    "Notes": {
      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "This is an interface to the LAPACK routines dgeqrf, zgeqrf, dorgqr, zungqr, dgeqp3, and zgeqp3."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "If "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "mode=economic"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ", the shapes of Q and R are (M, K) and (K, N) instead of (M,M) and (M,N), with "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "K=min(M,N)"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": "."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
