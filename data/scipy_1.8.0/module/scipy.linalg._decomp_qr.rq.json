    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Calculate the decomposition "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "A = R Q"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " where Q is unitary/orthogonal and R upper triangular."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
    "Methods": {
      "children": [],
      "title": null
    },
    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "This is an interface to the LAPACK routines sgerqf, dgerqf, cgerqf, zgerqf, sorgrq, dorgrq, cungrq and zungrq."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "If "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "mode=economic"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ", the shapes of Q and R are (K, N) and (M, K) instead of (N,N) and (M,N), with "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "K=min(M,N)"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": "."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
