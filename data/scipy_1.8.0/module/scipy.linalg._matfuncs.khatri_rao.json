    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The mathematical definition of the Khatri-Rao product is:"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "(A_{ij}  \\bigotimes B_{ij})_{ij}"
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "which is the Kronecker product of every column of A and B, e.g.:      "
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "c = np.vstack([np.kron(a[:, k], b[:, k]) for k in range(b.shape[1])]).T"
          },
          "type": "BlockVerbatim"
        }
      ],
      "level": 0,
      "title": null
    },
