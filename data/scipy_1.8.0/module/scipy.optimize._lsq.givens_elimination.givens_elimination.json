    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The matrix has the structure::      "
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "[ S ]\n[ D ]"
          },
          "type": "BlockVerbatim"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Where S is an upper triangular matrix with shape (n, n) and D is a diagonal matrix with shape (n, n) with elements from "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "diag"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": ". This function applies Givens rotations to it such that the resulting matrix has zeros in place of D."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Array "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "S"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " will be modified in-place."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Array "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "v"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " of shape (n,) is the part of the full vector with shape (2*n,)::      "
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "[ v ]\n[ 0 ]"
          },
          "type": "BlockVerbatim"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "to which Givens rotations are applied. This array is modified in place, such that on exit it contains the first n components of the above mentioned vector after rotations were applied."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
