                "type": "Directive"
              },
              {
                "data": {
                  "value": " ::         "
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "func(params) = ydata - f(xdata, params)"
          },
          "type": "BlockVerbatim"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "so that the objective function is ::         "
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "min   sum((ydata - f(xdata, params))**2, axis=0)\nrams"
