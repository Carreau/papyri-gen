                        },
                        {
                          "data": {
                            "value": [
                              "pinv(A.T) x"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": " is the least_square solution to "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "A.T y = x"
                            ]
                          },
                          "type": "Verbatim"
