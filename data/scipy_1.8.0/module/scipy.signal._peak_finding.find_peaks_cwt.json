    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The general approach is to smooth "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "vector"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " by convolving it with "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "wavelet(width)"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " for each width in "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "widths"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": ". Relative maxima which appear at enough length scales, and with sufficiently high SNR, are accepted."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "level": 0,
