    },
    "Parameters": {
      "children": [
        {
          "data": {
            "children": [
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "1-D array in which to find the peaks."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "vector",
                "type_": "ndarray"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Single width or 1-D array-like of widths to use for calculating the CWT matrix. In general, this range should cover the expected width of peaks of interest."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "widths",
                "type_": "float or sequence"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Should take two parameters and return a 1-D array to convolve with "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "domain": null,
                            "role": null,
                            "value": "vector"
                          },
                          "type": "Directive"
                        },
                        {
                          "data": {
                            "value": ". The first parameter determines the number of points of the returned wavelet array, the second parameter is the scale ("
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "domain": null,
                            "role": null,
                            "value": "width"
                          },
                          "type": "Directive"
                        },
                        {
                          "data": {
                            "value": ") of the wavelet. Should be normalized and symmetric. Default is the ricker wavelet."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "wavelet",
                "type_": "callable, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "At each row, a ridge line is only connected if the relative max at row[n] is within "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "max_distances[n]"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": " from the relative max at "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "row[n+1]"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": ".  Default value is "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "widths/4"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": "."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "max_distances",
                "type_": "ndarray, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "If a relative maximum is not found within "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "domain": null,
                            "role": null,
                            "value": "max_distances"
                          },
                          "type": "Directive"
                        },
                        {
                          "data": {
                            "value": ", there will be a gap. A ridge line is discontinued if there are more than "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "domain": null,
                            "role": null,
                            "value": "gap_thresh"
                          },
                          "type": "Directive"
                        },
                        {
                          "data": {
                            "value": " points without connecting a new relative maximum. Default is the first value of the widths array i.e. widths[0]."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "gap_thresh",
                "type_": "float, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Minimum length a ridge line needs to be acceptable. Default is "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "cwt.shape[0] / 4"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": ", ie 1/4-th the number of widths."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "min_length",
                "type_": "int, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Minimum SNR ratio. Default 1. The signal is the value of the cwt matrix at the shortest length scale ("
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "cwt[0, loc]"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": "), the noise is the "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "domain": null,
                            "role": null,
                            "value": "noise_perc'th percentile of datapoints contained within a\nwindow of 'window_size"
                          },
                          "type": "Directive"
                        },
                        {
                          "data": {
                            "value": " around "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "cwt[0, loc]"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": "."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "min_snr",
                "type_": "float, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "When calculating the noise floor, percentile of data points examined below which to consider noise. Calculated using "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "domain": null,
                            "role": null,
                            "value": "stats.scoreatpercentile"
                          },
                          "type": "Directive"
                        },
                        {
                          "data": {
                            "value": ".  Default is 10."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "noise_perc",
                "type_": "float, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Size of window to use to calculate noise floor. Default is "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "cwt.shape[1] / 20"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": "."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "window_size",
                "type_": "int, optional"
              }
            ]
          },
          "type": "Parameters"
        }
      ],
