    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "This approach was designed for finding sharp peaks among noisy data, however with proper parameter selection it should function well for different peak shapes."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "dd": [
                  {
                    "data": {
                      "children": [
                        {
                          "children": [
                            {
                              "data": {
                                "children": [
                                  {
                                    "data": {
                                      "value": "Perform a continuous wavelet transform on "
                                    },
                                    "type": "Words"
                                  },
                                  {
                                    "data": {
                                      "domain": null,
                                      "role": null,
                                      "value": "vector"
                                    },
                                    "type": "Directive"
                                  },
                                  {
                                    "data": {
                                      "value": ", for the supplied     "
                                    },
                                    "type": "Words"
                                  },
                                  {
                                    "data": {
                                      "domain": null,
                                      "role": null,
                                      "value": "widths"
                                    },
                                    "type": "Directive"
                                  },
                                  {
                                    "data": {
                                      "value": ". This is a convolution of "
                                    },
                                    "type": "Words"
                                  },
                                  {
                                    "data": {
                                      "domain": null,
                                      "role": null,
                                      "value": "vector"
                                    },
                                    "type": "Directive"
                                  },
                                  {
                                    "data": {
                                      "value": " with "
                                    },
                                    "type": "Words"
                                  },
                                  {
                                    "data": {
                                      "domain": null,
                                      "role": null,
                                      "value": "wavelet(width)"
                                    },
                                    "type": "Directive"
                                  },
                                  {
                                    "data": {
                                      "value": " for     each width in "
                                    },
                                    "type": "Words"
                                  },
                                  {
                                    "data": {
                                      "domain": null,
                                      "role": null,
                                      "value": "widths"
                                    },
                                    "type": "Directive"
                                  },
                                  {
                                    "data": {
                                      "value": ". See "
                                    },
                                    "type": "Words"
                                  },
                                  {
                                    "data": {
                                      "exists": true,
                                      "kind": "module",
                                      "reference": {
                                        "kind": "module",
                                        "module": "scipy",
                                        "path": "scipy.signal._wavelets.cwt",
                                        "version": "1.8.0"
                                      },
                                      "value": "cwt"
                                    },
                                    "type": "Link"
                                  },
                                  {
                                    "data": {
                                      "value": "."
                                    },
                                    "type": "Words"
                                  }
                                ]
                              },
                              "type": "Paragraph"
                            }
                          ]
                        },
                        {
                          "children": [
                            {
                              "data": {
                                "children": [
                                  {
                                    "data": {
                                      "value": "Identify \"ridge lines\" in the cwt matrix. These are relative maxima     at each row, connected across adjacent rows. See identify_ridge_lines"
                                    },
                                    "type": "Words"
                                  }
                                ]
                              },
                              "type": "Paragraph"
                            }
                          ]
                        },
                        {
                          "children": [
                            {
                              "data": {
                                "children": [
                                  {
                                    "data": {
                                      "value": "Filter the ridge_lines using filter_ridge_lines."
                                    },
                                    "type": "Words"
                                  }
                                ]
                              },
                              "type": "Paragraph"
                            }
                          ]
                        }
                      ]
                    },
                    "type": "EnumeratedList"
                  }
                ],
                "dt": {
                  "children": [
                    {
                      "data": {
                        "value": "The algorithm is as follows:"
                      },
                      "type": "Words"
                    }
                  ]
                }
              }
            ]
          },
          "type": "DefList"
        },
        {
