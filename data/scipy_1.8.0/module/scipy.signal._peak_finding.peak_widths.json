      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "This function calculates the width of a peak in samples at a relative distance to the peak's height and prominence."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
