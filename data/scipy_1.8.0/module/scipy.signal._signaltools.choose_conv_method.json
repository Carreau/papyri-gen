    "Extended Summary": {
      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "This primarily exists to be called during the "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "method='auto'"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " option in "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "module",
                  "reference": {
                    "kind": "module",
                    "module": "scipy",
                    "path": "scipy.signal._signaltools.convolve",
                    "version": "1.8.0"
                  },
                  "value": "convolve"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "module",
                  "reference": {
                    "kind": "module",
                    "module": "scipy",
                    "path": "scipy.signal._signaltools.correlate",
                    "version": "1.8.0"
                  },
                  "value": "correlate"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": ". It can also be used to determine the value of "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "method"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " for many different convolutions of the same dtype/shape. In addition, it supports timing the convolution to adapt the value of "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "method"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " to a particular set of inputs and/or hardware."
                },
                "type": "Words"
              }
            ],
