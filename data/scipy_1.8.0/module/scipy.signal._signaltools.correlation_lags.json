    "Notes": {
      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "Cross-correlation for continuous functions "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "f"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "g"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is defined as:"
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "\\left ( f\\star g \\right )\\left ( \\tau \\right ) \\triangleq \\int_ { t_0}^ { t_0 +T} \\overline { f\\left ( t \\right )}g\\left ( t+\\tau \\right )dt "
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "Where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "\\tau"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is defined as the displacement, also known as the lag."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "Cross correlation for discrete functions "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "f"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "g"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is defined as:"
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "\\left ( f\\star g \\right )\\left [ n \\right ] \\triangleq \\sum_ { - \\infty}^ { \\infty} \\overline { f\\left [ m \\right ]}g\\left [ m+n \\right ] "
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "Where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "n"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is the lag."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        }
      ],
