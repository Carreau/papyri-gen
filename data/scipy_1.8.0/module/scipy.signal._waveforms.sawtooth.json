    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The sawtooth waveform has a period "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "2*pi"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ", rises from -1 to 1 on the interval 0 to "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "width*2*pi"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ", then drops from 1 to -1 on the interval "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "width*2*pi"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " to "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "2*pi"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ". "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "width"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " must be in the interval [0, 1]."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Note that this is not band-limited.  It produces an infinite number of harmonics, which are aliased back and forth across the frequency spectrum."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
