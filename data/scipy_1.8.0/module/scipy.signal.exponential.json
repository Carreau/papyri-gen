    "Notes": {
      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "The Exponential window is defined as"
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "w ( n) = e^ { - |n - center| / \\tau} "
          },
          "type": "BlockMath"
        }
      ],
      "title": null
    },
