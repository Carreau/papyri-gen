        {
          "data": {
            "children": [
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "The N x N matrix of distances between nodes along the path specified by the predecessor matrix.  If no path exists, the distance is zero."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "dist_matrix",
                "type_": "ndarray"
              }
            ]
          },
          "type": "Parameters"
        }
      ],
