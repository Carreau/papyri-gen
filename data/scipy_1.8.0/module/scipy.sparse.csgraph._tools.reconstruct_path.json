                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "The N x N matrix representing the directed or undirected graph from which the predecessors are drawn."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "csgraph",
