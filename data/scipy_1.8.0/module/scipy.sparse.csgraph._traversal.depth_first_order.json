    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Note that a depth-first order is not unique.  Furthermore, for graphs with cycles, the tree generated by a depth-first search is not unique either."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [],
            "kind": "versionadded",
