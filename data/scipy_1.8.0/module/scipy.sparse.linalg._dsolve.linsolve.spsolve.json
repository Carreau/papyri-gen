                  "inline": [
                    {
                      "data": {
                        "value": "The matrix or vector representing the right hand side of the equation. If a vector, b.shape must be (n,) or (n, 1)."
                      },
                      "type": "Words"
                    }
                  ],
                  "inner": []
                },
