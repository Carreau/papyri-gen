                  "inline": [
                    {
                      "data": {
                        "value": "Which Minkowski p-norm to use.  Should be in the range [1, inf]. A finite large p may cause a ValueError if overflow can occur."
                      },
                      "type": "Words"
                    }
                  ],
                  "inner": []
                },
                "type": "Paragraph"
              }
            ],
            "param": "p",
