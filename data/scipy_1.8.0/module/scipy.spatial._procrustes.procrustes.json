                "value": [
                  {
                    "data": {
                      "inline": [
                        {
                          "data": {
                            "value": "The disparity should not depend on the order of the input matrices, but   the output matrices will, as only the first output matrix is guaranteed   to be scaled such that "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "tr(AA^{T}) = 1"
                            ]
                          },
                          "type": "Math"
                        },
                        {
                          "data": {
                            "value": "."
                          },
                          "type": "Words"
                        }
                      ],
                      "inner": []
                    },
                    "type": "Paragraph"
                  }
                ]
              }
            ]
