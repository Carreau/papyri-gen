                  {
                    "data": {
                      "inline": [
                        {
                          "data": {
                            "value": "Duplicate data points are generally ok, duplicating a data point will   increase its effect on the procrustes fit."
                          },
                          "type": "Words"
                        }
                      ],
                      "inner": []
                    },
                    "type": "Paragraph"
                  }
