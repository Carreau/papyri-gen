    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The Yule dissimilarity is defined as"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "\\frac{R}{c_{TT} * c_{FF} + \\frac{R}{2}}"
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "c_{ij}"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is the number of occurrences of "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\mathtt{u[k]} = i"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "\\mathtt{v[k]} = j"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " for "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "k < n"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "R = 2.0 * c_{TF} * c_{FT}"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": "."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
