    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The factorial of non-negative integer "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "n",
                    "version": null
                  },
                  "value": "n"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": " is the product of all positive integers less than or equal to "
                },
                "type": "Words"
              },
              {
                "data": {
                  "exists": true,
                  "kind": "local",
                  "reference": {
                    "kind": "local",
                    "module": null,
                    "path": "n",
                    "version": null
                  },
                  "value": "n"
                },
                "type": "Link"
              },
              {
                "data": {
                  "value": "      "
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "n! = n * (n - 1) * (n - 2) * ... * 1"
          },
          "type": "BlockVerbatim"
        }
      ],
      "title": null
    },
    "Methods": {
      "children": [],
      "title": null
    },
    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "For arrays with "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "exact=True"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ", the factorial is computed only once, for the largest input, with each other result computed in the process. The output dtype is increased to "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "int64"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " or "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "object"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " if necessary."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "With "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "exact=False"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " the factorial is approximated using the gamma function:"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "n! = \\Gamma(n+1)"
          },
          "type": "BlockMath"
        }
      ],
