    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The Fourier series of the even solutions of the Mathieu differential equation are of the form"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "\\mathrm{ce}_{2n}(z, q) = \\sum_{k=0}^{\\infty} A_{(2n)}^{(2k)} \\cos 2kz"
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "value": "\\mathrm{ce}_{2n+1}(z, q) = \\sum_{k=0}^{\\infty} A_{(2n+1)}^{(2k+1)} \\cos (2k+1)z"
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "This function returns the coefficients "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "A_{(2n)}^{(2k)}"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " for even input m=2n, and the coefficients "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "A_{(2n+1)}^{(2k+1)}"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " for odd input m=2n+1."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "level": 0,
      "title": null
    },
