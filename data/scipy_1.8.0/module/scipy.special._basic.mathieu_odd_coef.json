    "Extended Summary": {
      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "The Fourier series of the odd solutions of the Mathieu differential equation are of the form"
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "\\mathrm { se}_ { 2n+1} ( z, q) = \\sum_ { k=0}^ { \\infty} B_ { ( 2n+1)}^ { ( 2k+1)} \\sin ( 2k+1)z "
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "value": "\\mathrm { se}_ { 2n+2} ( z, q) = \\sum_ { k=0}^ { \\infty} B_ { ( 2n+2)}^ { ( 2k+2)} \\sin ( 2k+2)z "
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "This function returns the coefficients "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "B_{(2n+2)}^{(2k+2)}"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " for even input m=2n+2, and the coefficients "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "B_{(2n+1)}^{(2k+1)}"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " for odd input m=2n+1."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        }
      ],
