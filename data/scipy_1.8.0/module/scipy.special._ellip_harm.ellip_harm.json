    "Extended Summary": {
      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "These are also known as Lame functions of the first kind, and are solutions to the Lame equation:"
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "( s^2 - h^2) ( s^2 - k^2)E ' ' ( s) + s ( 2s^2 - h^2 - k^2)E ' ( s) + ( a - q s^2)E ( s) = 0 "
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "q = (n+1)n"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " and "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "a"
                  ]
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is the eigenvalue (not returned) corresponding to the solutions."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
