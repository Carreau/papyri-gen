    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Defined as ,"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "y_n(z) = \\sqrt{\\frac{\\pi}{2z}} Y_{n + 1/2}(z),"
          },
          "type": "BlockMath"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "where "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": "Y_n"
                },
                "type": "Math"
              },
              {
                "data": {
                  "value": " is the Bessel function of the second kind."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
    "Methods": {
      "children": [],
      "title": null
    },
    "Notes": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "For real arguments, the function is computed using the ascending recurrence .  For complex arguments, the definitional relation to the cylindrical Bessel function of the second kind is used."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The derivative is computed using the relations ,"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "y_n' = y_{n-1} - \\frac{n + 1}{z} y_n.\n\ny_0' = -y_1"
          },
          "type": "BlockMath"
        },
