    "Extended Summary": {
      "children": [
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "Create a rectangular montage from an input array representing an ensemble of equally shaped single- (gray) or multichannel (color) images."
                }
              }
            ]
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "For example, "
                }
              },
              {
                "type": "Verbatim",
                "data": {
                  "value": [
                    "montage(arr",
                    "_",
                    "in)"
                  ]
                }
              },
              {
                "type": "Words",
                "data": {
                  "value": " called with the following "
                }
              },
              {
                "type": "Directive",
                "data": {
                  "value": [
                    "arr",
                    "_",
                    "in"
                  ],
                  "domain": null,
                  "role": null
                }
              }
            ]
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "+---+---+---+ | 1 | 2 | 3 | +---+---+---+"
                }
              }
            ]
          }
        },
        {
          "type": "Paragraph",
          "data": {
            "children": [
              {
                "type": "Words",
                "data": {
                  "value": "will return"
