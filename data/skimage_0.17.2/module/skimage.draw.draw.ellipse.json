          "type": "Param"
        },
        {
          "data": {
            "desc": [
              {
                "data": {
                  "inline": [
                    {
                      "data": {
                        "value": "Minor and major semi-axes. "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "value": [
                          "(r/r_radius)**2 + (c/c_radius)**2 = 1"
                        ]
                      },
                      "type": "Verbatim"
                    },
                    {
                      "data": {
                        "value": "."
                      },
                      "type": "Words"
