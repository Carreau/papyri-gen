    "Extended Summary": {
      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "Also known as Power Law Transform. This function transforms the input image pixelwise according to the equation "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "O = I**gamma"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " after scaling each pixel to the range 0 to 1."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        }
      ],
      "title": null
    },
    "Methods": {
      "children": [],
      "title": null
    },
    "Notes": {
      "children": [
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "For gamma greater than 1, the histogram will shift towards left and the output image will be darker than the input image."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "inline": [
              {
                "data": {
                  "value": "For gamma less than 1, the histogram will shift towards right and the output image will be brighter than the input image."
                },
                "type": "Words"
              }
            ],
            "inner": []
          },
          "type": "Paragraph"
        }
      ],
