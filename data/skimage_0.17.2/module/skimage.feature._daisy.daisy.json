    "Returns": {
      "children": [
        {
          "data": {
            "children": [
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Grid of DAISY descriptors for the given image as an array dimensionality  (P, Q, R) where"
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  },
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "children": [
                              {
                                "data": {
                                  "value": [
                                    "P = ceil((M - radius*2) / step)"
                                  ]
                                },
                                "type": "Verbatim"
                              },
                              {
                                "data": {
                                  "value": "     "
                                },
                                "type": "Words"
                              },
                              {
                                "data": {
                                  "value": [
                                    "Q = ceil((N - radius*2) / step)"
                                  ]
                                },
                                "type": "Verbatim"
                              },
                              {
                                "data": {
                                  "value": "     "
                                },
                                "type": "Words"
                              },
                              {
                                "data": {
                                  "value": [
                                    "R = (rings * histograms + 1) * orientations"
                                  ]
                                },
                                "type": "Verbatim"
                              }
                            ]
                          },
                          "type": "Paragraph"
                        }
                      ]
                    },
                    "type": "BlockQuote"
                  }
                ],
                "param": "descs",
                "type_": "array"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Visualization of the DAISY descriptors."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "descs_img",
                "type_": "(M, N, 3) array (only if visualize==True)"
              }
            ]
          },
          "type": "Parameters"
        }
      ],
