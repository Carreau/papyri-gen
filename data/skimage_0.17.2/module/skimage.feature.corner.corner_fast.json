                      "children": [
                        {
                          "data": {
                            "value": "Threshold used in deciding whether the pixels on the circle are brighter, darker or similar w.r.t. the test pixel. Decrease the threshold when more corners are desired and vice-versa."
                          },
                          "type": "Words"
                        }
                      ]
