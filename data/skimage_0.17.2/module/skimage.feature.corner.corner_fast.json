                        },
                        {
                          "data": {
                            "value": " and brighter if "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "domain": null,
                            "role": null,
                            "value": "Ic > Ip + threshold"
                          },
                          "type": "Directive"
                        },
                        {
                          "data": {
                            "value": ". Also stands for the n in "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "domain": null,
                            "role": null,
                            "value": "FAST-n"
                          },
                          "type": "Directive"
                        },
                        {
                          "data": {
                            "value": " corner detector."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "n",
                "type_": "int, optional"
              },
