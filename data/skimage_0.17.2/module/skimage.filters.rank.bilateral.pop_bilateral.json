      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The number of pixels is defined as the number of pixels which are included in the structuring element and the mask. Additionally pixels must have a greylevel inside the interval [g-s0, g+s1] where g is the greyvalue of the center pixel."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
