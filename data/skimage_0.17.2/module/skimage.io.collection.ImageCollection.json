                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "If a pattern string is given for "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "domain": null,
                            "role": null,
                            "value": "load_pattern"
                          },
                          "type": "Directive"
                        },
                        {
                          "data": {
                            "value": ", this attribute stores the expanded file list. Otherwise, this is equal to "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "domain": null,
                            "role": null,
                            "value": "load_pattern"
                          },
                          "type": "Directive"
                        },
                        {
                          "data": {
                            "value": "."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "files",
                "type_": "list of str"
              }
            ]
          },
          "type": "Parameters"
        }
      ],
      "title": null
    },
    "Extended Summary": {
      "children": [],
      "title": null
    },
