    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Two pixels are connected when they are neighbors and have the same value. In 2D, they can be neighbors either in a 1- or 2-connected sense. The value refers to the maximum number of orthogonal hops to consider a pixel/voxel a neighbor::    "
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": "1-connectivity     2-connectivity     diagonal connection close-up\n\n     [ ]           [ ]  [ ]  [ ]             [ ]\n      |               \\  |  /                 |  <- hop 2\n[ ]--[x]--[ ]      [ ]--[x]--[ ]        [x]--[ ]\n      |               /  |  \\             hop 1\n     [ ]           [ ]  [ ]  [ ]"
          },
          "type": "BlockVerbatim"
        }
      ],
      "title": null
    },
    "Methods": {
      "children": [],
      "title": null
    },
    "Notes": {
      "children": [],
      "title": null
    },
    "Other Parameters": {
      "children": [],
      "title": null
    },
    "Parameters": {
      "children": [
        {
          "data": {
            "children": [
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Image to label."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "input",
                "type_": "ndarray of dtype int"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Whether to use 4- or 8-\"connectivity\". In 3D, 4-\"connectivity\" means connected pixels have to share face, whereas with 8-\"connectivity\", they have to share only edge or vertex. "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "content": {
                              "value": "Deprecated, use"
                            }
                          },
                          "type": "Strong"
                        },
                        {
                          "data": {
                            "value": " "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "connectivity"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": " "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "content": {
                              "value": "instead."
                            }
                          },
                          "type": "Strong"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "neighbors",
                "type_": "{4, 8}, int, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Consider all pixels with this value as background pixels, and label them as 0. By default, 0-valued pixels are considered as background pixels."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "background",
                "type_": "int, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Whether to return the number of assigned labels."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "return_num",
                "type_": "bool, optional"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Maximum number of orthogonal hops to consider a pixel/voxel as a neighbor. Accepted values are ranging from  1 to input.ndim. If "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "None"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": ", a full connectivity of "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "input.ndim"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": " is used."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "connectivity",
                "type_": "int, optional"
              }
            ]
          },
          "type": "Parameters"
        }
