    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The center coordinates (cr, cc) can be calculated from the raw moments as: {"
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "M[1, 0] / M[0, 0]"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ", "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "M[0, 1] / M[0, 0]"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": "}."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Note that central moments are translation invariant but not scale and rotation invariant."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
