                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "M[1, 0] / M[0, 0]"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": ", "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "M[0, 1] / M[0, 0]"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": "}."
                },
                "type": "Words"
              }
            ],
            "inner": []
