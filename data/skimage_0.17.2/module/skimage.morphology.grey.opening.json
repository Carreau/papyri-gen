    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "The morphological opening on an image is defined as an erosion followed by a dilation. Opening can remove small bright spots (i.e. \"salt\") and connect small dark cracks. This tends to \"open\" up (dark) gaps between (bright) features."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "level": 0,
      "title": null
    },
