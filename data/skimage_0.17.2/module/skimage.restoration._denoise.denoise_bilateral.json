      "title": null
    },
    "Parameters": {
      "children": [
        {
          "data": {
            "children": [
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Input image, 2D grayscale or RGB."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "image",
                "type_": "ndarray, shape (M, N[, 3])"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Window size for filtering. If win_size is not specified, it is calculated as "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "max(5, 2 * ceil(3 * sigma_spatial) + 1)"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": "."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "win_size",
                "type_": "int"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Standard deviation for grayvalue/color distance (radiometric similarity). A larger value results in averaging of pixels with larger radiometric differences. Note, that the image will be converted using the "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "anchor": null,
                            "exists": true,
                            "kind": "module",
                            "reference": {
                              "kind": "module",
                              "module": "skimage",
                              "path": "skimage.util.dtype.img_as_float",
                              "version": "0.17.2"
                            },
                            "value": "img_as_float"
                          },
                          "type": "Link"
                        },
                        {
                          "data": {
                            "value": " function and thus the standard deviation is in respect to the range "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "[0, 1]"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": ". If the value is "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "None"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": " the standard deviation of the "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "image"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": " will be used."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "sigma_color",
                "type_": "float"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Standard deviation for range distance. A larger value results in averaging of pixels with larger spatial differences."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "sigma_spatial",
                "type_": "float"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Number of discrete values for Gaussian weights of color filtering. A larger value results in improved accuracy."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "bins",
                "type_": "int"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "How to handle values outside the image borders. See "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "anchor": null,
                            "exists": true,
                            "kind": "module",
                            "reference": {
                              "kind": "api",
                              "module": "numpy",
                              "path": "numpy.pad",
                              "version": "*"
                            },
                            "value": "numpy.pad"
                          },
                          "type": "Link"
                        },
                        {
                          "data": {
                            "value": " for detail."
                          },
                          "type": "Words"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ],
                "param": "mode",
                "type_": "{'constant', 'edge', 'symmetric', 'reflect', 'wrap'}"
              },
              {
                "desc": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "Used in conjunction with mode 'constant', the value outside the image boundaries."
