                  "inline": [
                    {
                      "data": {
                        "value": "The noise standard deviation used when computing the wavelet detail coefficient threshold(s). When None (default), the noise standard deviation is estimated via the method in ."
                      },
                      "type": "Words"
                    }
                  ],
                  "inner": []
                },
