              }
            ],
            "param": "sigma",
            "type_": "float or list, optional"
          },
          "type": "Param"
        },
        {
          "data": {
            "desc": [
              {
                "data": {
                  "inline": [
                    {
                      "data": {
                        "value": "The type of wavelet to perform and can be any of the options "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "value": [
                          "pywt.wavelist"
                        ]
                      },
                      "type": "Verbatim"
                    },
                    {
                      "data": {
                        "value": " outputs. The default is "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "domain": null,
                        "role": null,
                        "value": "'db1'"
                      },
                      "type": "Directive"
                    },
                    {
                      "data": {
                        "value": ". For example, "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "value": [
                          "wavelet"
                        ]
                      },
                      "type": "Verbatim"
                    },
                    {
                      "data": {
                        "value": " can be any of "
                      },
                      "type": "Words"
                    },
                    {
                      "data": {
                        "value": [
                          "{'db2', 'haar', 'sym9'}"
                        ]
                      },
                      "type": "Verbatim"
                    },
                    {
                      "data": {
                        "value": " and many more."
                      },
                      "type": "Words"
                    }
                  ],
                  "inner": []
                },
                "type": "Paragraph"
              }
            ],
            "param": "wavelet",
            "type_": "string, optional"
          },
          "type": "Param"
        },
        {
          "data": {
            "desc": [
              {
                "data": {
                  "inline": [
                    {
                      "data": {
                        "value": "An optional argument to choose the type of denoising performed. It noted that choosing soft thresholding given additive noise finds the best approximation of the original image."
                      },
                      "type": "Words"
                    }
                  ],
                  "inner": []
                },
                "type": "Paragraph"
              }
            ],
            "param": "mode",
            "type_": "{'soft', 'hard'}, optional"
          },
