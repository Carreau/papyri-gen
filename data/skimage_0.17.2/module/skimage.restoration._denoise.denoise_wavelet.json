        },
        {
          "data": {
            "desc": [
              {
                "data": {
                  "inline": [
                    {
                      "data": {
                        "value": "The number of wavelet decomposition levels to use.  The default is three less than the maximum number of possible decomposition levels."
                      },
                      "type": "Words"
                    }
                  ],
                  "inner": []
                },
                "type": "Paragraph"
              }
            ],
            "param": "wavelet_levels",
            "type_": "int or None, optional"
