                          "data": {
                            "value": "If called with a masked 1D array or called with a 1D array and "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "wrap_around=True"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": "."
                          },
                          "type": "Words"
                        }
                      ]
                    },
