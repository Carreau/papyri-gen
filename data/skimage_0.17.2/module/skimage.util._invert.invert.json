                  "inline": [
                    {
                      "data": {
                        "value": "If True and the image is of type float, the range is assumed to be [-1, 1]. If False and the image is of type float, the range is assumed to be [0, 1]."
                      },
                      "type": "Words"
                    }
                  ],
