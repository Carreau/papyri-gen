    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Create a rectangular montage from an input array representing an ensemble of equally shaped single- (gray) or multichannel (color) images."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "For example, "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "montage(arr_in)"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " called with the following "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "arr_in"
                },
                "type": "Directive"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "+---+---+---+ | 1 | 2 | 3 | +---+---+---+"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "will return"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "+---+---+ | 1 | 2 | +---+---+ | 3 | * | +---+---+"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "where the '*' patch will be determined by the "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "fill"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " parameter."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
