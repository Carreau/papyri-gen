    "Extended Summary": {
      "children": [
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Compute a feature of a gray level co-occurrence matrix to serve as a compact summary of the matrix. The properties are computed as follows:"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "children": [
              {
                "children": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "'contrast': "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": "\\sum_{i,j=0}^{levels-1} P_{i,j}(i-j)^2"
                          },
                          "type": "Math"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ]
              },
              {
                "children": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "'dissimilarity': "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": "\\sum_{i,j=0}^{levels-1}P_{i,j}|i-j|"
                          },
                          "type": "Math"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ]
              },
              {
                "children": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "'homogeneity': "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": "\\sum_{i,j=0}^{levels-1}\\frac{P_{i,j}}{1+(i-j)^2}"
                          },
                          "type": "Math"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ]
              },
              {
                "children": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "'ASM': "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": "\\sum_{i,j=0}^{levels-1} P_{i,j}^2"
                          },
                          "type": "Math"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ]
              },
              {
                "children": [
                  {
                    "data": {
                      "children": [
                        {
                          "data": {
                            "value": "'energy': "
                          },
                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": "\\sqrt{ASM}"
                          },
                          "type": "Math"
                        }
                      ]
                    },
                    "type": "Paragraph"
                  }
                ]
              },
              {
                "children": [
                  {
                    "data": {
                      "children": [
                        {
                          "dd": [
                            {
                              "data": {
                                "value": "\\sum_{i,j=0}^{levels-1} P_{i,j}\\left[\\frac{(i-\\mu_i) \\(j-\\mu_j)}{\\sqrt{(\\sigma_i^2)(\\sigma_j^2)}}\\right]"
                              },
                              "type": "BlockMath"
                            }
                          ],
                          "dt": {
                            "children": [
                              {
                                "data": {
                                  "value": "'correlation':"
                                },
                                "type": "Words"
                              }
                            ]
                          }
                        }
                      ]
                    },
                    "type": "DefList"
                  }
                ]
              }
            ]
          },
          "type": "BulletList"
        },
        {
          "data": {
            "children": [
              {
                "data": {
                  "value": "Each GLCM is normalized to have a sum of 1 before the computation of texture properties."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
      ],
      "level": 0,
