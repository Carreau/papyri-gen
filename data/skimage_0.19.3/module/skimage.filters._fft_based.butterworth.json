              },
              {
                "data": {
                  "value": " the "
                },
                "type": "Words"
              },
              {
                "data": {
                  "value": [
                    "order"
                  ]
                },
                "type": "Verbatim"
              },
              {
                "data": {
                  "value": " modeled after"
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        }
