            "children": [
              {
                "data": {
                  "value": "Given an image's initial segmentation and its edge map this method constructs the corresponding Region Adjacency Graph (RAG). Each node in the RAG represents a set of pixels within the image with the same label in "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "labels"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": ". The weight between two adjacent regions is the average value in "
                },
                "type": "Words"
              },
              {
                "data": {
                  "domain": null,
                  "role": null,
                  "value": "edge_map"
                },
                "type": "Directive"
              },
              {
                "data": {
                  "value": " along their boundary."
                },
                "type": "Words"
              }
