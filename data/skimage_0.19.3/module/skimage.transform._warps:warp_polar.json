  "example_section_data": {
    "children": [
      {
        "type": "text",
        "value": "Perform a basic polar warp on a grayscale image:"
      },
      {
        "type": "code",
        "value": "from skimage import data\nfrom skimage.transform import warp_polar\nimage = data.checkerboard()\nwarped = warp_polar(image)"
      },
      {
        "type": "text",
        "value": "Perform a log-polar warp on a grayscale image:"
      },
      {
        "type": "code",
        "value": "warped = warp_polar(image, scaling='log')"
      },
      {
        "type": "text",
        "value": "Perform a log-polar warp on a grayscale image while specifying center,\nradius, and output shape:"
      },
      {
        "type": "code",
        "value": "warped = warp_polar(image, (100,100), radius=100,\n                    output_shape=image.shape, scaling='log')"
      },
      {
        "type": "text",
        "value": "Perform a log-polar warp on a color image:"
      },
      {
        "type": "code",
        "value": "image = data.astronaut()\nwarped = warp_polar(image, scaling='log', channel_axis=-1)"
      }
    ],
