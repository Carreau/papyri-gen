  "example_section_data": {
    "children": [
      {
        "type": "code",
        "value": "SIZE = 59\nimg = np.tri(SIZE, dtype=np.int32)"
      },
      {
        "type": "text",
        "value": "Apply the Finite Radon Transform:"
      },
      {
        "type": "code",
        "value": "f = frt2(img)"
      },
      {
        "type": "text",
        "value": "Apply the Inverse Finite Radon Transform to recover the input"
      },
      {
        "type": "code",
        "value": "fi = ifrt2(f)"
      },
      {
        "type": "text",
        "value": "Check that it's identical to the original"
      },
      {
        "type": "code",
        "value": "assert len(np.nonzero(img-fi)[0]) == 0"
      }
    ],
