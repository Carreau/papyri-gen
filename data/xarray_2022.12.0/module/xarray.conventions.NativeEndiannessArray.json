            "children": [
              {
                "data": {
                  "value": "This is useful for decoding arrays from netCDF3 files (which are all big endian) into native endianness, so they can be used with Cython functions, such as those found in bottleneck and pandas."
                },
                "type": "Words"
              }
            ]
          },
          "type": "Paragraph"
        },
        {
          "data": {
            "value": ">>> x = np.arange(5, dtype=\">i2\")"
          },
          "type": "BlockVerbatim"
        },
        {
          "data": {
            "value": ">>> x.dtype\ndtype('>i2')"
          },
          "type": "BlockVerbatim"
        },
        {
          "data": {
            "value": ">>> NativeEndiannessArray(x).dtype\ndtype('int16')"
          },
          "type": "BlockVerbatim"
        },
        {
          "data": {
            "value": ">>> indexer = indexing.BasicIndexer((slice(None),))\n>>> NativeEndiannessArray(x)[indexer].dtype\ndtype('int16')"
          },
          "type": "BlockVerbatim"
        }
