                          "type": "Words"
                        },
                        {
                          "data": {
                            "value": [
                              "variables"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": ". One of variables or variables_kwargs must be provided."
