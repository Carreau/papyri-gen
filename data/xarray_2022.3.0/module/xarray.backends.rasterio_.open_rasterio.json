                        },
                        {
                          "data": {
                            "value": [
                              "parse_coordinates=False"
                            ]
                          },
                          "type": "Verbatim"
                        },
                        {
                          "data": {
                            "value": " if your files are very large or if you don't need the coordinates."
                          },
