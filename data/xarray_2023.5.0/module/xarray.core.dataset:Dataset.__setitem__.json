            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "select_vars()"
            },
            {
              "type": "text",
              "value": " method, rename it to "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "key"
            },
            {
              "type": "text",
              "value": " and merge the contents of the resulting dataset into this dataset."
            }
          ],
          "type": "paragraph"
        },
        {
          "children": [
            {
              "type": "text",
              "value": "If value is a "
            },
            {
              "domain": null,
              "role": null,
              "type": "Directive",
              "value": "Variable"
            },
            {
              "type": "text",
              "value": " object (or tuple of form "
            },
            {
              "type": "inlineCode",
              "value": "(dims, data[, attrs])"
            },
            {
              "type": "text",
              "value": "), add it to this dataset as a new variable."
            }
