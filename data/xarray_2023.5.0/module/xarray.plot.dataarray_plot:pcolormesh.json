                    {
                      "type": "text",
                      "value": " with "
                    },
                    {
                      "type": "inlineCode",
                      "value": "levels=N"
                    },
                    {
                      "type": "text",
                      "value": " is equivalent to setting "
                    },
                    {
                      "type": "inlineCode",
                      "value": "levels=np.linspace(vmin, vmax, N)"
